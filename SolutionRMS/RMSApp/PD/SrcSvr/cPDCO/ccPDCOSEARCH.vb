        strFormat = strFormat & " '' SUBSEQNAME,"
        strFormat = strFormat & " '' JOBNO,"
        strFormat = strFormat & " '' JOBNAME,"
        strFormat = strFormat & " '' DEMANDDAY,"
        strFormat = strFormat & " ISNULL(SUM(B.AMT),0) TAXAMT,"
        strFormat = strFormat & " SUM(ISNULL(A.PAYMENT,0))+SUM(ISNULL(A.ACCAMT,0)) PAYMENT,"
        strFormat = strFormat & " SUM(ISNULL(A.INCOM,0)) INCOM,"
        strFormat = strFormat & " CASE ISNULL(SUM(B.AMT),0) WHEN 0 THEN 0 ELSE"
        strFormat = strFormat & " Round(((ISNULL(SUM(B.AMT),0)-(SUM(ISNULL(A.PAYMENT,0))+SUM(ISNULL(A.ACCAMT,0))))/ISNULL(SUM(B.AMT),0))*100,2)"
        strFormat = strFormat & " END AS RATE,"
        strFormat = strFormat & " '' EMPNAME,"
        strFormat = strFormat & " 'Y' JOBGUBN"
        strFormat = strFormat & " FROM PD_EXE_HDR A "
        strFormat = strFormat & " LEFT JOIN (SELECT JOBNO,SUM(AMT) AMT,SUBSTRING(DEMANDDAY,1,6) DEMANDDAY FROM PD_TAX_DTL GROUP BY JOBNO,SUBSTRING(DEMANDDAY,1,6)) B"
        strFormat = strFormat & " ON A.JOBNO = B.JOBNO"
