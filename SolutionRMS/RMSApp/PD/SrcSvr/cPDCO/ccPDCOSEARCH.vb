        If strPOPUPTYPE <> "" Then

            If strPOPUPTYPE = "REG" Then '외주처 등록날짜조회
                If strFrom <> "" And strTo <> "" Then
                    Con1 = String.Format(" AND (B.REGDATE BETWEEN '{0}' AND  '{1}')", strFrom, strTo)
                End If
                If strFrom <> "" And strTo = "" Then
                    Con1 = String.Format(" AND (B.REGDATE >= '{0}')", strFrom)
                End If
                If strFrom = "" And strTo <> "" Then
                    Con1 = String.Format(" AND (B.REGDATE <= '{0}')", strTo)
                End If

            Else '전표날짜조회
                If strFrom <> "" And strTo <> "" Then
                    Con1 = String.Format(" AND (B.ADJDAY BETWEEN '{0}' AND  '{1}')", strFrom, strTo)
                End If
                If strFrom <> "" And strTo = "" Then
                    Con1 = String.Format(" AND (B.ADJDAY >= '{0}')", strFrom)
                End If
                If strFrom = "" And strTo <> "" Then
                    Con1 = String.Format(" AND (B.ADJDAY <= '{0}')", strTo)
                End If

