                                         ByVal strTo As String, _
                                         ByVal strCUSTCODE As String, _
                                         ByVal strCUSTNAME As String, _
                                         ByVal strJOBGUBN As String) As Object

        Dim strWhere As String       'Where조건 변수
        Dim strFormat As String      'SQL Format 변수
        Dim strSQL As String          'SQL 변수
        Dim vntData As Object        'Array Return 변수(Array 를사용할 때 선언)
        Dim strXMLData As String    'XML  Return 변수(XML  을 사용할 때 선언)
        Dim Con1, Con2, Con3, Con4 As String


        If strFrom <> "" And strTo <> "" Then
            Con1 = String.Format(" AND (A.ENDDAY BETWEEN '{0}' AND  '{1}')", strFrom, strTo)
        End If
        If strFrom <> "" And strTo = "" Then
            Con1 = String.Format(" AND (A.ENDDAY >= '{0}')", strFrom)
        End If
        If strFrom = "" And strTo <> "" Then
            Con1 = String.Format(" AND (A.ENDDAY <= '{0}')", strTo)
        End If

        If strCUSTCODE <> "" Then Con2 = String.Format(" AND C.CLIENTCODE = '{0}'", strCUSTCODE)
        If strCUSTNAME <> "" Then Con3 = String.Format(" AND DBO.SC_GET_HIGHCUSTNAME_FUN(C.CLIENTCODE) like '%{0}%'", strCUSTNAME)
        If strJOBGUBN <> "" Then Con4 = String.Format(" AND C.JOBGUBN = '{0}'", strJOBGUBN)


        strWhere = BuildFields(" ", Con1, Con2, Con3, Con4)

        strFormat = " SELECT "
        strFormat = strFormat & " 1 GBN,"
        strFormat = strFormat & " A.ENDDAY,"
        strFormat = strFormat & " C.CLIENTCODE,"
        strFormat = strFormat & " DBO.SC_GET_HIGHCUSTNAME_FUN(C.CLIENTCODE) CLIENTNAME,"
        strFormat = strFormat & " DBO.SC_GET_SUBSEQNAME_FUN(C.SUBSEQ) SUBSEQNAME,"
        strFormat = strFormat & " A.JOBNO,"
        strFormat = strFormat & " C.JOBNAME,"
        strFormat = strFormat & " CASE ISNULL(B.DEMANDDAY,'') WHEN '' THEN '' ELSE SUBSTRING(B.DEMANDDAY,1,4)+'-'+SUBSTRING(B.DEMANDDAY,5,2) END AS DEMANDDAY,"
        strFormat = strFormat & " ISNULL(B.AMT,0) TAXAMT,"
        strFormat = strFormat & " ISNULL(A.PAYMENT,0)+ISNULL(A.ACCAMT,0) PAYMENT,"
        strFormat = strFormat & " ISNULL(A.INCOM,0) INCOM,"
        strFormat = strFormat & " ISNULL(A.RATE,0) RATE,"
