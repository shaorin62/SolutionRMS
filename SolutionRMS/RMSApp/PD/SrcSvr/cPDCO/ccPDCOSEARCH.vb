        strFormat = strFormat & " '' CONTRACTNO,"
        strFormat = strFormat & " '' CONFIRMFLAG"
        strFormat = strFormat & " FROM"
        strFormat = strFormat & " ("
        strFormat = strFormat & " 	SELECT"
        strFormat = strFormat & " 	B.OUTSCODE,"
        strFormat = strFormat & " 	DBO.SC_GET_CUSTNAME_FUN(B.OUTSCODE)+ ' 소계' OUTSNAME,"
        strFormat = strFormat & " 	SUM(B.ADJAMT) ADJAMT,"
        strFormat = strFormat & " 	CASE WHEN (VATCODE = '세금계산서영세율' OR VATCODE = '계산서' OR VATCODE = 'INVOICE')  THEN 0 ELSE SUM(ROUND(B.ADJAMT*0.1,0)) END AS VAT,"
        strFormat = strFormat & " 	'' VATCODE,"
        strFormat = strFormat & " 	SUM(B.ADJAMT) + CASE WHEN (VATCODE = '세금계산서영세율' OR VATCODE = '계산서' OR VATCODE = 'INVOICE')  THEN 0 ELSE SUM(ROUND(B.ADJAMT*0.1,0)) END AS SUMAMT"
        strFormat = strFormat & " 	FROM V_JOBNO A,PD_EXE_DTL B"
        strFormat = strFormat & " 	WHERE A.JOBNO = B.JOBNO {0}"
        strFormat = strFormat & " 	GROUP BY B.OUTSCODE,VATCODE"
        strFormat = strFormat & " ) SUBDATA"
        strFormat = strFormat & " GROUP BY OUTSCODE,VATCODE"

        strFormat = strFormat & " UNION ALL"

        strFormat = strFormat & " SELECT"
        strFormat = strFormat & " 3 GBN,"
