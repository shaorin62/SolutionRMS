        strFormat = strFormat & " 	FROM V_JOBNO A,PD_EXE_DTL B"
        strFormat = strFormat & " 	WHERE A.JOBNO = B.JOBNO {0}"
        strFormat = strFormat & " 	GROUP BY B.OUTSCODE,VATCODE"
        strFormat = strFormat & "  ) SUBDATA"
        strFormat = strFormat & " ORDER BY 2,1"

        SetConfig(strInfoXML) '기본정보 Setting
        With mobjSCGLConfig '기본정보 Config 개체
            strSQL = String.Format(strFormat, strWhere)
            Try
                ' DB 접속
