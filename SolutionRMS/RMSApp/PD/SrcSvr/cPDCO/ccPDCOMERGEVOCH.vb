            strSelFields = strSelFields & " A.VAT,"
            strSelFields = strSelFields & " A.MTAXYEARMON,"
            strSelFields = strSelFields & " A.MTAXNO,"
            strSelFields = strSelFields & " A.VOCHNO,"
            strSelFields = strSelFields & " A.VOCH_TYPE,"
            strSelFields = strSelFields & " B.RMSNO,"
            strSelFields = strSelFields & " B.ERRCODE,"
            strSelFields = strSelFields & " B.ERRMSG "
            strSelFields = strSelFields & " FROM PD_MERGETAX_HDR  A "
            strSelFields = strSelFields & " LEFT JOIN PD_MERGEVOCH_MST B "
            strSelFields = strSelFields & " ON A.MTAXYEARMON = B.MTAXYEARMON AND A.MTAXNO = B.MTAXNO AND ISNULL(B.MTAXNOSEQ, 0) = '0' "
            strSelFields = strSelFields & " WHERE isnull(A.attr10,0) <> 999999"
            strSelFields = strSelFields & " {0} "
            strSelFields = strSelFields & " ORDER BY  A.MTAXYEARMON, A.MTAXNO"

