            strSelFields = strSelFields & " CASE ISNULL(B.POSTINGDATE,'') WHEN '' "
            strSelFields = strSelFields & " THEN DBO.PD_MTAXNO_DEMANDDAY_FUN(A.MTAXYEARMON,A.MTAXNO) "
            strSelFields = strSelFields & " ELSE B.POSTINGDATE END AS POSTINGDATE,"
