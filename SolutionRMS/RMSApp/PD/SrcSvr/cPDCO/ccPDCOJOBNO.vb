    '============== ProcessRtn PROJECT 코드 수정
    Public Function ProcessRtnSheet(ByVal strInfoXML As String, _
                                    ByVal vntData As Object) As Integer '데이터 INSERT/UPDATE
        Dim intRtn As Integer
        Dim i, intColCnt, intRows As Integer
        Dim strREQDAY As String
        Dim strGROUPGBN
        Dim strHOPEENDDAY As String
        SetConfig(strInfoXML)
        With mobjSCGLConfig
            Try

                .mobjSCGLSql.SQLConnect(.DBConnStr)
                .mobjSCGLSql.SQLBeginTrans()
                If IsArray(vntData) Then
                    '''사용할 Entity 개체생성(Config 정보를 넘겨생성)
                    mobjcePD_JOBNO = New cePD_JOBNO(mobjSCGLConfig)
                    '''vntData의 컬럼수, 로우수를 변수입력
                    intColCnt = UBound(vntData, 1) : intRows = UBound(vntData, 2)
                    For i = 1 To intRows
                        If GetElement(vntData, "REQDAY", intColCnt, i, OPTIONAL_STR) <> "" Then strREQDAY = GetElement(vntData, "REQDAY", intColCnt, i, OPTIONAL_STR).SUBSTRING(0, 4) & GetElement(vntData, "REQDAY", intColCnt, i, OPTIONAL_STR).SUBSTRING(5, 2) & GetElement(vntData, "REQDAY", intColCnt, i, OPTIONAL_STR).SUBSTRING(8, 2)
                        If GetElement(vntData, "HOPEENDDAY", intColCnt, i, OPTIONAL_STR) <> "" Then strHOPEENDDAY = GetElement(vntData, "HOPEENDDAY", intColCnt, i, OPTIONAL_STR).SUBSTRING(0, 4) & GetElement(vntData, "HOPEENDDAY", intColCnt, i, OPTIONAL_STR).SUBSTRING(5, 2) & GetElement(vntData, "HOPEENDDAY", intColCnt, i, OPTIONAL_STR).SUBSTRING(8, 2)
                        intRtn = UpdateRtn_JOBNO(vntData, intColCnt, i, strREQDAY, strHOPEENDDAY)

                    Next
                End If
                .mobjSCGLSql.SQLCommitTrans()
                Return intRows
            Catch err As Exception
                .mobjSCGLSql.SQLRollbackTrans()
                Throw RaiseSysErr(err, CLASS_NAME & ".ProcessRtnSheet")
            Finally
                .mobjSCGLSql.SQLDisconnect()
                mobjcePD_JOBNO.Dispose()
            End Try
        End With
    End Function

