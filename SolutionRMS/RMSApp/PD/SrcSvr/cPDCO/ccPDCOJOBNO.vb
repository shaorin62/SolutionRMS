    Public Function ProcessRtn_Seq(ByVal xmlRoot As XmlElement, _
                                   ByVal strSEQFLAG As String, _
                                   ByRef strJOBNO As String) As Integer

        Dim intRtn
        Dim strCUSTCODE As String
        Dim strYEAR As String
        Dim strREQDAY As String
        Dim strHOPEENDDAY As String
        Dim strGUBN As String
        With mobjSCGLConfig
            Try
                If XMLGetElement(xmlRoot, "REQDAY") <> "" Then strREQDAY = XMLGetElement(xmlRoot, "REQDAY").Substring(0, 4) & XMLGetElement(xmlRoot, "REQDAY").Substring(5, 2) & XMLGetElement(xmlRoot, "REQDAY").Substring(8, 2)
                If XMLGetElement(xmlRoot, "HOPEENDDAY") <> "" Then strHOPEENDDAY = XMLGetElement(xmlRoot, "HOPEENDDAY").Substring(0, 4) & XMLGetElement(xmlRoot, "HOPEENDDAY").Substring(5, 2) & XMLGetElement(xmlRoot, "HOPEENDDAY").Substring(8, 2)
                ''' 사용할 Entity 개체생성(Config 정보를 넘겨생성)
                mobjcePD_JOBNO = New cePD_JOBNO(mobjSCGLConfig)


                strYEAR = Mid(XMLGetElement(xmlRoot, "REQDAY"), 3, 2)
                strGUBN = Mid(XMLGetElement(xmlRoot, "CREPART"), 2, 1)
                If strSEQFLAG = "new" Then
