            strCols = strCols & " CASE DBO.PD_TAX_USE_FUN(A.TRANSYEARMON,A.TRANSNO) "
            strCols = strCols & " WHEN 'Y' THEN 'O'"
            strCols = strCols & " ELSE '' END AS CONFIRMFLAG,"
            strCols = strCols & " CASE ISNULL(A.CREFLAG,'0') WHEN '0' THEN '' ELSE '임시' END CREFLAG,   "
            strCols = strCols & " DBO.SC_GET_HIGHCUSTNAME_FUN(A.CLIENTCODE) CLIENTNAME,  "
            strCols = strCols & " ISNULL(A.AMT,0) AMT,  "
            strCols = strCols & " ISNULL(A.VAT,0) VAT,  "
            strCols = strCols & " ISNULL(A.AMT,0)+ISNULL(A.VAT,0) SUMAMTVAT,  "
            strCols = strCols & " A.DEMANDDAY DEMANDDAY,  "
            strCols = strCols & " A.PRINTDAY PRINTDAY,  "
            strCols = strCols & " A.TRANSYEARMON,"
            strCols = strCols & " A.TRANSNO,"
            strCols = strCols & " A.MEMO "

            strFormat = "SELECT {0} "
            strFormat = strFormat & " FROM PD_TRANS_HDR A, "
            strFormat = strFormat & " ("
            strFormat = strFormat & " 	SELECT TRANSYEARMON,TRANSNO,CONFIRMFLAG "
            strFormat = strFormat & " 	FROM ("
            strFormat = strFormat & " 		SELECT TRANSYEARMON,TRANSNO,"
            strFormat = strFormat & " 		CASE SUM(ISNULL(CONFIRMFLAG,0)) WHEN 0 THEN null ELSE 1 END CONFIRMFLAG "
            strFormat = strFormat & " 		FROM PD_TRANS_DTL "
            strFormat = strFormat & " 		WHERE 1=1   "
            strFormat = strFormat & " 		GROUP BY TRANSYEARMON,TRANSNO"
            strFormat = strFormat & " 	) DATA "
            strFormat = strFormat & " ) B , PD_TRANS_DTL C"
            strFormat = strFormat & " WHERE A.TRANSYEARMON = B.TRANSYEARMON  "
            strFormat = strFormat & " AND A.TRANSNO = B.TRANSNO AND A.TRANSYEARMON = C.TRANSYEARMON AND A.TRANSNO = C.TRANSNO AND C.SEQ=1 AND  isnull(B.CONFIRMFLAG,0) = 1 {1} "

            strSQL = String.Format(strFormat, strCols, strWhere)
            Try
                ' DB 접속
                .mobjSCGLSql.SQLConnect(.DBConnStr)
                ' ------ Array 데이터 조회 (True 일때 헤더정보 포함 조회(Sheet Data Binding 할 경우 사용), False 일때 데이터만 조회)
                vntData = .mobjSCGLSql.SQLSelectArr(strSQL, intRowCnt, intColCnt, , True)
                Return vntData
            Catch err As Exception
                ' 오류 전송
                '.mobjSCGLSql.SQLRollbackTrans()
                Throw RaiseSysErr(err, CLASS_NAME & ".SelectRtn_TransList_Cancel")
            Finally
                ' DB 접속 종료
                .mobjSCGLSql.SQLDisconnect()
            End Try
        End With

    End Function

    ' =============== SelectRtn_TransList_DTL 거래명세디테일 내역조회
    Public Function SelectRtn_TransList_DTL(ByVal strInfoXML As String, _
                                            ByRef intRowCnt As Integer, _
                                            ByRef intColCnt As Integer, _
                                            ByVal strTRANSYEARMON As String, _
                                            ByVal strTRANSNO As String) As Object

        Dim strSQL As String            'SQL문
        Dim strFormat As String         '임시 SQL문
        Dim strWhere As String
        Dim vntData As Object
        Dim Con1, Con2 As String

        SetConfig(strInfoXML)   '기본정보 설정

        Con1 = ""
        Con2 = ""

        If strTRANSYEARMON <> "" Then Con1 = String.Format(" AND (A.TRANSYEARMON = '{0}')", strTRANSYEARMON)
        If strTRANSNO <> "" Then Con2 = String.Format(" AND (A.TRANSNO = '{0}')", strTRANSNO)


        strWhere = BuildFields(" ", Con1, Con2)


        strFormat = " SELECT "
        strFormat = strFormat & " A.TRANSYEARMON,"
        strFormat = strFormat & " A.TRANSNO, "
        strFormat = strFormat & " A.SEQ, "
        strFormat = strFormat & " A.PRINT_SEQ, "
        strFormat = strFormat & " DBO.SC_GET_HIGHCUSTNAME_FUN(B.CLIENTCODE) CLIENTNAME, "
        strFormat = strFormat & " DBO.SC_GET_SUBSEQNAME_FUN(B.SUBSEQ) SUBSEQNAME, "
        strFormat = strFormat & " DBO.SC_GET_CUSTNAME_FUN(B.TIMCODE) TIMNAME, "
        strFormat = strFormat & " DBO.SC_GET_CUSTNAME_FUN(B.CLIENTSUBCODE) CLIENTSUBNAME,"
        strFormat = strFormat & " ISNULL(B.JOBNAME,0) JOBNAME, "
        strFormat = strFormat & " ISNULL(B.JOBNO,0) JOBNO, "
        strFormat = strFormat & " ISNULL(B.SEQ,0) JOBSEQ, "
        strFormat = strFormat & " ISNULL(A.AMT,0) AMT, "
        strFormat = strFormat & " ISNULL(A.ADJAMT,0) ADJAMT, "
        strFormat = strFormat & " ISNULL(A.BALANCE,0) BALANCE, "
        strFormat = strFormat & " A.MEMO, "
        strFormat = strFormat & " A.TAXYEARMON, "
        strFormat = strFormat & " A.TAXNO, "
        strFormat = strFormat & " A.CONFIRMFLAG "
        strFormat = strFormat & " FROM PD_TRANS_DTL A INNER JOIN PD_DIVAMT B "
        strFormat = strFormat & " ON A.JOBNO = B.JOBNO  AND A.JOBNOSEQ = B.SEQ "
        strFormat = strFormat & " WHERE 1=1 {0} "

        strSQL = String.Format(strFormat, strWhere)
        '기본정보 Setting
        With mobjSCGLConfig '기본정보 Config 개체
            Try
                ' DB 접속
                .mobjSCGLSql.SQLConnect(.DBConnStr)
                vntData = .mobjSCGLSql.SQLSelectArr(strSQL, intRowCnt, intColCnt, , True)

                Return vntData
            Catch err As Exception
                ' 오류 전송
                Throw RaiseSysErr(err, CLASS_NAME & ".SelectRtn_TransList_DTL")
            Finally
                ' DB 접속 종료
                .mobjSCGLSql.SQLDisconnect()
            End Try
        End With
    End Function

    '============== 승인 처리 1
    Public Function ProcessRtn_Confirm(ByVal strInfoXML As String, _
                                       ByVal vntData As Object, _
                                       ByVal strGBN As String) As Integer

        Dim intRtn As Integer
        Dim i, intColCnt, intRows As Integer

        SetConfig(strInfoXML) '기본정보 Setting

        With mobjSCGLConfig '기본정보를 가지고 있는 Config 개체

            Try
                'DB접속 및 트랜잭션 시작
                .mobjSCGLSql.SQLConnect(.DBConnStr)
                .mobjSCGLSql.SQLBeginTrans()


                If IsArray(vntData) Then
                    '''사용할 Entity 개체생성(Config 정보를 넘겨생성)
                    mobjcePD_TRANS_HDR = New cePD_TRANS_HDR(mobjSCGLConfig)
                    mobjcePD_TRANS_DTL = New cePD_TRANS_DTL(mobjSCGLConfig)
                    '''vntData의 컬럼수, 로우수를 변수입력
                    intColCnt = UBound(vntData, 1) : intRows = UBound(vntData, 2)
                    '''해당하는Row 만큼 Loop

                    For i = 1 To intRows
                        If strGBN = "T" Then
                            If GetElement(vntData, "CHK", intColCnt, i, OPTIONAL_STR) = "1" Then
                                intRtn = UpdateRtn_TRANSHDRCONFIRM(vntData, intColCnt, i)
                                intRtn = UpdateRtn_TRANSDTLCONFIRM(vntData, intColCnt, i)
                            End If
                        Else
                            If GetElement(vntData, "CHK", intColCnt, i, OPTIONAL_STR) = "1" Then
                                intRtn = UpdateRtn_TRANSHDRCONFIRMCANCEL(vntData, intColCnt, i)
                                intRtn = UpdateRtn_TRANSDTLCONFIRMCANCEL(vntData, intColCnt, i)
                            End If
                        End If
                    Next
                End If

                '트랜잭션Commit
                .mobjSCGLSql.SQLCommitTrans()
                Return intRtn
            Catch err As Exception
                '트랜잭션RollBack 및 오류 전송
                .mobjSCGLSql.SQLRollbackTrans()
                Throw RaiseSysErr(err, CLASS_NAME & ".ProcessRtn_Confirm_OK")
            Finally
                mobjcePD_TRANS_HDR.Dispose()
                mobjcePD_TRANS_DTL.Dispose()
                'Resource해제
                .mobjSCGLSql.SQLDisconnect()
            End Try


        End With
    End Function
