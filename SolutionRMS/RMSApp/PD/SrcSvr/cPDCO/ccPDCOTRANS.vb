        Dim strCols As String         '컬럼변수
        Dim strWhere As String       'Where조건 변수
        Dim strFormat As String      'SQL Format 변수
        Dim Con1, Con2, Con3, Con4, Con5, Con6 As String


        SetConfig(strInfoXML)
        With mobjSCGLConfig '기본정보 Config 개체


            Con1 = "" : Con2 = "" : Con3 = "" : Con4 = "" : Con5 = "" : Con6 = ""

            If strYEARMON <> "" Then Con1 = String.Format(" AND (A.TRANSYEARMON = '{0}')", strYEARMON)

            If strFROM <> "" And strTO <> "" Then
                Con2 = String.Format(" AND (A.DEMANDDAY BETWEEN '{0}' AND  '{1}')", strFROM, strTO)
            End If
            If strFROM <> "" And strTO = "" Then
                Con2 = String.Format(" AND (A.DEMANDDAY > '{0}')", strFROM)
            End If
            If strFROM = "" And strTO <> "" Then
                Con2 = String.Format(" AND (A.DEMANDDAY < '{0}')", strTO)
            End If

            If strCLIENTCODE <> "" Then Con3 = String.Format(" AND (A.CLIENTCODE = '{0}')", strCLIENTCODE)
            If strCLIENTNAME <> "" Then Con4 = String.Format(" AND (DBO.SC_GET_HIGHCUSTNAME_FUN(A.CLIENTCODE) LIKE '%{0}%')", strCLIENTNAME)
            If strTIMCODE <> "" Then Con5 = String.Format(" AND (C.TIMCODE = '{0}')", strTIMCODE)
            If strTIMNAME <> "" Then Con6 = String.Format(" AND (DBO.SC_GET_CUSTNAME_FUN(C.TIMNAME) LIKE '%{0}%')", strTIMNAME)

            strWhere = BuildFields(" ", Con1, Con2, Con3, Con4, Con5, Con6)

            strCols = strCols & " 0 CHK,  "
            strCols = strCols & " CASE ISNULL(B.CONFIRMFLAG,0) WHEN 0 THEN '' ELSE '승인' END CONFIRMGBN,  "
            strCols = strCols & " CASE DBO.PD_TAX_USE_FUN(A.TRANSYEARMON,A.TRANSNO) "
            strCols = strCols & " WHEN 'Y' THEN 'O'"
            strCols = strCols & " ELSE '' END AS CONFIRMFLAG,"
            strCols = strCols & " CASE ISNULL(A.CREFLAG,'0') WHEN '0' THEN '' ELSE '임시' END CREFLAG,   "
