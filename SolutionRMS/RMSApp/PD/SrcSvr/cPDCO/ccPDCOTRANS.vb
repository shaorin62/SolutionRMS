        Dim strCLIENTCODE As String
        Dim strTIMCODE As String
        Dim strPREESTNO As String
        Dim strSUBSEQ As String
        Dim strDEPT_CD As String
        Dim strMEMO As String

        SetConfig(strInfoXML) '기본정보 Setting

        With mobjSCGLConfig '기본정보를 가지고 있는 Config 개체
            Try
                'XML Element 변수 선언 (strMasterXML을 변환)
                Dim xmlRoot As XmlElement
                xmlRoot = XMLGetRoot(strMasterXML) 'XML 데이터
                If XMLGetElement(xmlRoot, "PRINTDAY") <> "" Then strPRINTDAY = XMLGetElement(xmlRoot, "PRINTDAY").Substring(0, 4) & XMLGetElement(xmlRoot, "PRINTDAY").Substring(5, 2) & XMLGetElement(xmlRoot, "PRINTDAY").Substring(8, 2)
                If XMLGetElement(xmlRoot, "DEMANDDAY") <> "" Then strDEMANDDAY = XMLGetElement(xmlRoot, "DEMANDDAY").Substring(0, 4) & XMLGetElement(xmlRoot, "DEMANDDAY").Substring(5, 2) & XMLGetElement(xmlRoot, "DEMANDDAY").Substring(8, 2)
                'DB접속 및 트랜잭션 시작

                .mobjSCGLSql.SQLConnect(.DBConnStr)
                .mobjSCGLSql.SQLBeginTrans()
                mobjcePD_TRANS_HDR = New cePD_TRANS_HDR(mobjSCGLConfig)
                mobjcePD_DIVAMT = New ePDCO.cePD_DIVAMT(mobjSCGLConfig)
                mobjcePD_TRANS_DTL = New ePDCO.cePD_TRANS_DTL(mobjSCGLConfig)
                'Master 데이터 처리
                '청구지그룹별 청구지코드 가져오기
                If IsArray(vntData) Then
                    intColCnt = UBound(vntData, 1) : intRows = UBound(vntData, 2)
                    '''해당하는Row 만큼 Loop
                    For i = 1 To intRows
                        If GetElement(vntData, "CHK", intColCnt, i, OPTIONAL_STR) = "1" Then
                            strCLIENTCODE = GetElement(vntData, "CLIENTCODE", intColCnt, i, OPTIONAL_STR)
                            strTIMCODE = GetElement(vntData, "TIMCODE", intColCnt, i, OPTIONAL_STR)
                            strPREESTNO = GetElement(vntData, "PREESTNO", intColCnt, i, OPTIONAL_STR)
                            strSUBSEQ = GetElement(vntData, "SUBSEQ", intColCnt, i, OPTIONAL_STR)
                            strDEPT_CD = GetElement(vntData, "DEPT_CD", intColCnt, i, OPTIONAL_STR)
                            strMEMO = GetElement(vntData, "MEMO", intColCnt, i, OPTIONAL_STR)
                            lngSUMAMT = GetElement(vntData, "ADJAMT", intColCnt, i, OPTIONAL_STR)
                            lngSUMVAT = GetElement(vntData, "VAT", intColCnt, i, OPTIONAL_STR)

                            '헤더저장
                            intTRANSNO = SelectRtn_TRANSNO(strTRANSYEARMON)
                            intRtn = InsertRtn_TRANS_HDR(xmlRoot, intTRANSNO, strTRANSYEARMON, strDEMANDDAY, strPRINTDAY, lngSUMAMT, lngSUMVAT, strCLIENTCODE, strDEPT_CD, strMEMO)
                            '디테일저장
                            intTRANSNOSEQ = 1
                            intRtn = InsertRtn_TRANS_DTL(vntData, intColCnt, i, strTRANSYEARMON, intTRANSNO, intTRANSNOSEQ, strDEMANDDAY, strPRINTDAY, strMEMO)

                            'JOB Status [청구] 변경
                            strJOBNO = GetElement(vntData, "JOBNO", intColCnt, i)
                            intRtn2 = mobjcePD_TRANS_DTL.Update_EndFlag(strJOBNO, strDEMANDDAY)
                            'intRtn = strDEMANDDAY
                            '청구금액 및 분담금 정산처리
                            intRtn = UpdateRtn_PD_GROUP_DIVAMT(vntData, intColCnt, i, strTRANSYEARMON, intTRANSNO, intTRANSNOSEQ, strPREESTNO)
                        End If
                    Next
                End If
                .mobjSCGLSql.SQLCommitTrans()
                Return intRtn
            Catch err As Exception
                '트랜잭션RollBack 및 오류 전송
                .mobjSCGLSql.SQLRollbackTrans()
                Throw RaiseSysErr(err, CLASS_NAME & ".ProcessRtn_Div")
            Finally
                'Resource해제
                .mobjSCGLSql.SQLDisconnect()
                mobjcePD_TRANS_HDR.Dispose()
                mobjcePD_TRANS_DTL.Dispose()
                mobjcePD_DIVAMT.Dispose()

            End Try
        End With
