    ' =============== SelectRtn 거래명세생성전 DIVAMT 조회
    Public Function SelectRtn(ByVal strInfoXML As String, _
                              ByRef intRowCnt As Integer, _
                              ByRef intColCnt As Integer, _
                              ByRef strYEARMON As String, _
                              ByRef strCLIENTCODE As String, _
                              ByRef strCLIENTNAME As String, _
                              ByRef strTIMCODE As String, _
                              ByRef strTIMNAME As String, _
                              ByRef strJOBGUBN As String) As Object

        Dim strWhere As String       'Where조건 변수
        Dim strFormat, strCols As String     'SQL Format 변수
        Dim strSQL As String         'SQL 변수
        Dim vntData As Object        'Array Return 변수(Array 를사용할 때 선언)
        Dim Con1, Con2, Con3, Con4, Con5, Con6

        SetConfig(strInfoXML)

        Con1 = "" : Con2 = "" : Con3 = "" : Con4 = "" : Con5 = "" : Con6 = ""

        If strYEARMON <> "" Then Con1 = String.Format(" AND (YEARMON = '{0}')", strYEARMON)
        If strCLIENTCODE <> "" Then Con2 = String.Format(" AND (CLIENTCODE = '{0}')", strCLIENTCODE)
        If strCLIENTNAME <> "" Then Con3 = String.Format(" AND (DBO.SC_GET_HIGHCUSTNAME_FUN(CLIENTCODE) LIKE '%{0}%')", strCLIENTNAME)
        If strTIMCODE <> "" Then Con4 = String.Format(" AND (TIMCODE = '{0}')", strTIMCODE)
        If strTIMNAME <> "" Then Con5 = String.Format(" AND (DBO.SC_GET_CUSTNAME_FUN(TIMCODE) LIKE '%{0}%')", strTIMNAME)
        If strJOBGUBN <> "" Then Con6 = String.Format(" AND (DBO.PD_JOBGUBN_FUN(JOBNO) = '{0}')", strJOBGUBN)

        strWhere = BuildFields(" ", Con1, Con2, Con3, Con4, Con5, Con6)


        strFormat = " SELECT "
        strFormat = strFormat & " 0 CHK,"
        strFormat = strFormat & " PREESTNO,"
        strFormat = strFormat & " CLIENTCODE,"
        strFormat = strFormat & " DBO.SC_GET_HIGHCUSTNAME_FUN(CLIENTCODE) CLIENTNAME,"
        strFormat = strFormat & " TIMCODE,"
        strFormat = strFormat & " DBO.SC_GET_CUSTNAME_FUN(TIMCODE) TIMNAME,"
        strFormat = strFormat & " SUBSEQ,"
        strFormat = strFormat & " DBO.SC_GET_SUBSEQNAME_FUN(SUBSEQ) SUBSEQNAME,"
        strFormat = strFormat & " JOBNO,"
        strFormat = strFormat & " JOBNAME,"
        strFormat = strFormat & " SEQ ,"
        strFormat = strFormat & " ISNULL(DIVAMT,0) AS AMT,"
        strFormat = strFormat & " ISNULL(ADJAMT,0) AS ADJAMT,"
        strFormat = strFormat & " ISNULL(DIVAMT,0)-ISNULL(ADJAMT,0) BALANCE, "
        strFormat = strFormat & " CASE TAXCODE WHEN 'TA01' THEN floor(ADJAMT * 0.1) ELSE 0 END AS VAT,"
        strFormat = strFormat & " DBO.MD_GETPUBNAME_FUN(ENDFLAG) ENDFLAGNAME ,"
        strFormat = strFormat & " MEMO,"
        strFormat = strFormat & " DENSE_RANK() OVER (ORDER BY CLIENTCODE, TIMCODE,DBO.PD_JOBGUBN_FUN(JOBNO) DESC) TRANSTIMRANK, "
        strFormat = strFormat & " DENSE_RANK() OVER (ORDER BY CLIENTCODE,DBO.PD_JOBGUBN_FUN(JOBNO) DESC) TRANSCUSTRANK, "
        strFormat = strFormat & " DEMANDFLAG,DBO.SC_GET_CODENAME_FUN(DEMANDFLAG) DEMANDFLAGNAME,"
        strFormat = strFormat & " INCJOBNOSEQ,"
        strFormat = strFormat & " INCJOBNO,"
        strFormat = strFormat & " DBO.PD_JOBNO_DEPTCD_FUN(JOBNO) DEPT_CD,TAXCODE,DBO.SC_GET_CODENAME_FUN(TAXCODE) TAXNAME"
        strFormat = strFormat & " FROM PD_DIVAMT WHERE 1=1 AND CONFIRMFLAG = 3 "
        strFormat = strFormat & " {0} "
        strFormat = strFormat & " ORDER BY DBO.SC_GET_HIGHCUSTNAME_FUN(CLIENTCODE) , DBO.SC_GET_CUSTNAME_FUN(TIMCODE), DBO.PD_JOBCUST_NAME_FUN(SUBSEQ) "


        strSQL = String.Format(strFormat, strWhere)

        '기본정보 Setting
        With mobjSCGLConfig '기본정보 Config 개체
            Try
                ' DB 접속
                .mobjSCGLSql.SQLConnect(.DBConnStr)
                ' ------ Array 데이터 조회 (True 일때 헤더정보 포함 조회(Sheet Data Binding 할 경우 사용), False 일때 데이터만 조회)
