    Public Function DeleteRtn(ByVal strInfoXML As String, _
                              ByVal vntData As Object) As Integer   '데이터 DELETE

        Dim intRtn, i As Integer      'Return변수( 처리건수 또는 0 )
        Dim intColCnt, intRows
        Dim strTRANSNO
        Dim strTRANSYEARMON
        Dim dblJOBNOSEQ As Double
        Dim strJOBNO
        Dim strCUSTCODE
        Dim strPREESTNO
        Dim strDEMANDDAY
        Dim dblADJAMT As Double

        SetConfig(strInfoXML)    '기본정보 Setting
        With mobjSCGLConfig    '기본정보 Config 개체
            Try
                ' 사용할Entity 개체생성(Config 정보를 넘겨생성)
                mobjcePD_DIVAMT = New cePD_DIVAMT(mobjSCGLConfig)
                mobjcePD_JOBNO = New cePD_JOBNO(mobjSCGLConfig)
                mobjcePD_TRANS_HDR = New cePD_TRANS_HDR(mobjSCGLConfig)
                mobjcePD_TRANS_DTL = New cePD_TRANS_DTL(mobjSCGLConfig)
                ' DB 접속 및 트랜잭션 시작
                .mobjSCGLSql.SQLConnect(.DBConnStr)
                .mobjSCGLSql.SQLBeginTrans()
                ' 엔티티 오브젝트의 Delete 메소드 호출

                If IsArray(vntData) Then
                    intColCnt = UBound(vntData, 1) : intRows = UBound(vntData, 2)
                    '''해당하는Row 만큼 Loop
                    For i = 1 To intRows
                        If GetElement(vntData, "CHK", intColCnt, i, OPTIONAL_STR) = 1 Then
                            strTRANSYEARMON = GetElement(vntData, "TRANSYEARMON", intColCnt, i, OPTIONAL_STR)
                            strTRANSNO = GetElement(vntData, "TRANSNO", intColCnt, i, OPTIONAL_STR)
                            strJOBNO = GetElement(vntData, "JOBNO", intColCnt, i, OPTIONAL_STR)
                            strDEMANDDAY = GetElement(vntData, "DEMANDDAY", intColCnt, i, OPTIONAL_STR)

                            intRtn = mobjcePD_TRANS_HDR.DeleteDo(strTRANSYEARMON, strTRANSNO)
                            intRtn = mobjcePD_JOBNO.DeleteUpdateDo(strTRANSYEARMON, strTRANSNO)
                            intRtn = mobjcePD_DIVAMT.DeleteUpdateDo(strTRANSYEARMON, strTRANSNO)
                            intRtn = mobjcePD_TRANS_DTL.DeleteDo(strTRANSYEARMON, strTRANSNO)
                        End If
                    Next
                End If

                ' 트랜잭션 Commit
                .mobjSCGLSql.SQLCommitTrans()
                Return intRtn
            Catch err As Exception
                '트랜잭션 RollBack 및 오류 전송
                .mobjSCGLSql.SQLRollbackTrans()
                Throw RaiseSysErr(err, CLASS_NAME & "DeleteRtn")
            Finally
                'DB접속 종료
                .mobjSCGLSql.SQLDisconnect()
                '사용한 Entity(개체Dispose)
                mobjcePD_DIVAMT.Dispose()
                mobjcePD_JOBNO.Dispose()
                mobjcePD_TRANS_DTL.Dispose()
                mobjcePD_TRANS_HDR.Dispose()
            End Try
        End With
    End Function

