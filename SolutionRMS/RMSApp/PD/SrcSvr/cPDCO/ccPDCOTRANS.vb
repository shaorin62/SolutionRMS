                Return intRows
            Catch err As Exception
                Throw RaiseSysErr(err, CLASS_NAME & ".ProcessRtn_CUSTDtl")
            Finally
                mobjcePD_TRANS_DTL.Dispose()
            End Try
        End With
    End Function

