    '============== ProcessRtn_Group 합산생성
    Public Function ProcessRtn_Group(ByVal strInfoXML As String, _
                                     ByVal vntData As Object, _
                                     ByVal intColFlag As Integer) As Integer

        Dim intRtn, intRtn1, intRtn2, intRtn3 As Integer '결과값 변수
        Dim intCnt, intCnt2, strCNT
        Dim i, intColCnt, intRows As Integer '루프, 컬럼Cnt, 로우Cnt 변수
        Dim strMTAXYEARMON As String
        Dim intMTAXNO
        Dim lngAMT, lngSUMAMT, lngVAT, lngSUMVAT
        Dim strSUMM
        Dim strDEMANDDAY, strPRINTDAY
        Dim strTAXYEARMON, strTAXNO
        Dim lngTAXNO, lngTAXNOSEQ, intMTAXNOSEQ
        Dim strMEDFLAG
        Dim strMTAXNO
        Dim strVOCH_TYPE


        SetConfig(strInfoXML) '기본정보 Setting
        With mobjSCGLConfig '기본정보를 가지고 있는 Config 개체
            Try
                Dim xmlRoot As XmlElement

                .mobjSCGLSql.SQLConnect(.DBConnStr)
                .mobjSCGLSql.SQLBeginTrans()

                mobjcePD_MERGETAX_HDR = New cePD_MERGETAX_HDR(mobjSCGLConfig)
                mobjcePD_MERGETAX_DTL = New cePD_MERGETAX_DTL(mobjSCGLConfig)

                intColCnt = UBound(vntData, 1) : intRows = UBound(vntData, 2)
                For intCnt = 1 To intColFlag
                    intCnt2 = 0
                    strCNT = 0
                    If IsArray(vntData) Then
                        intColCnt = UBound(vntData, 1) : intRows = UBound(vntData, 2)
                        '''해당하는Row 만큼 Loop
                        lngAMT = 0
                        lngSUMAMT = 0
                        lngVAT = 0
                        lngSUMVAT = 0
                        strSUMM = ""
                        '쉬트 내 헤더값 검출
                        For i = 1 To intRows
                            '청구합계
                            If GetElement(vntData, "CHK", intColCnt, i, OPTIONAL_STR) = 1 And GetElement(vntData, "RANKTRANS", intColCnt, i, OPTIONAL_STR) = intCnt Then
                                lngAMT = GetElement(vntData, "AMT", intColCnt, i, OPTIONAL_STR)
                                lngSUMAMT = lngSUMAMT + lngAMT
                                lngVAT = GetElement(vntData, "VAT", intColCnt, i, OPTIONAL_STR)
                                lngSUMVAT = lngSUMVAT + lngVAT

                                '계산서년월
                                strMTAXYEARMON = GetElement(vntData, "MTAXYEARMON", intColCnt, i, OPTIONAL_STR)
                                '계산서번호
                                intMTAXNO = SelectRtn_TAXNO(strMTAXYEARMON)
                                '헤더정보
                                strDEMANDDAY = GetElement(vntData, "DEMANDDAY", intColCnt, i, OPTIONAL_STR).Substring(0, 4) & GetElement(vntData, "DEMANDDAY", intColCnt, i, OPTIONAL_STR).Substring(5, 2) & GetElement(vntData, "DEMANDDAY", intColCnt, i, OPTIONAL_STR).Substring(8, 2)
                                strPRINTDAY = GetElement(vntData, "PRINTDAY", intColCnt, i, OPTIONAL_STR).Substring(0, 4) & GetElement(vntData, "PRINTDAY", intColCnt, i, OPTIONAL_STR).Substring(5, 2) & GetElement(vntData, "PRINTDAY", intColCnt, i, OPTIONAL_STR).Substring(8, 2)

                                strSUMM = GetElement(vntData, "SUMM", intColCnt, i, OPTIONAL_STR)

                                strVOCH_TYPE = GetElement(vntData, "VOCH_TYPE", intColCnt, i, OPTIONAL_STR)
                            End If
                        Next

                        '검출된 헤더값 실제저장
                        For i = 1 To intRows
                            If GetElement(vntData, "CHK", intColCnt, i, OPTIONAL_STR) = 1 And GetElement(vntData, "RANKTRANS", intColCnt, i, OPTIONAL_STR) = intCnt Then
                                If intCnt2 = intCnt Then
                                Else
                                    '헤더저장
                                    intRtn = InsertRtn_MERGETAX_HDR(vntData, _
                                                                  intColCnt, _
                                                                  i, _
                                                                  strMTAXYEARMON, _
                                                                  intMTAXNO, _
                                                                  lngSUMAMT, _
                                                                  lngSUMVAT, _
                                                                  strDEMANDDAY, _
                                                                  strPRINTDAY, _
                                                                  strSUMM, _
                                                                  strVOCH_TYPE)
                                End If
                                '무한업데이트.
                                intCnt2 = intCnt
                                '디테일저장 
                                If IsArray(vntData) Then
                                    '거래명세서 번호는 위에서 참조
                                    strTAXYEARMON = GetElement(vntData, "TAXYEARMON", intColCnt, i, OPTIONAL_STR)
                                    lngTAXNO = GetElement(vntData, "TAXNO", intColCnt, i, OPTIONAL_STR)
                                    lngTAXNOSEQ = GetElement(vntData, "TAXNOSEQ", intColCnt, i, OPTIONAL_STR)
                                    strMEDFLAG = GetElement(vntData, "MEDFLAG", intColCnt, i, OPTIONAL_STR)

                                    strDEMANDDAY = GetElement(vntData, "DEMANDDAY", intColCnt, i, OPTIONAL_STR).Substring(0, 4) & GetElement(vntData, "DEMANDDAY", intColCnt, i, OPTIONAL_STR).Substring(5, 2) & GetElement(vntData, "DEMANDDAY", intColCnt, i, OPTIONAL_STR).Substring(8, 2)
                                    strPRINTDAY = GetElement(vntData, "PRINTDAY", intColCnt, i, OPTIONAL_STR).Substring(0, 4) & GetElement(vntData, "PRINTDAY", intColCnt, i, OPTIONAL_STR).Substring(5, 2) & GetElement(vntData, "PRINTDAY", intColCnt, i, OPTIONAL_STR).Substring(8, 2)

                                    strTAXNO = strTAXYEARMON & "-" & lngTAXNO
                                    strMTAXNO = strMTAXYEARMON & "-" & intMTAXNO
                                    intMTAXNOSEQ = SelectRtn_MERGETAXNOSEQ(strMTAXYEARMON, intMTAXNO)

                                    '디테일저장
                                    intRtn1 = InsertRtn_MERGETAX_DTL(vntData, intColCnt, i, strMTAXYEARMON, intMTAXNO, intMTAXNOSEQ)

                                    '거래명세서 디테일의 세금계산서번호 저장
                                    intRtn2 = mobjcePD_MERGETAX_HDR.Update_MERGETaxNo(strTAXYEARMON, lngTAXNO, lngTAXNOSEQ, strMEDFLAG, strMTAXNO, strVOCH_TYPE)
                                End If
                            End If
                        Next
                    End If
                Next

                .mobjSCGLSql.SQLCommitTrans()
                Return intRtn
            Catch err As Exception
                .mobjSCGLSql.SQLRollbackTrans()
                Throw RaiseSysErr(err, CLASS_NAME & ".ProcessRtn_Group")
            Finally
                'Resource해제
                mobjcePD_MERGETAX_HDR.Dispose()
                mobjcePD_MERGETAX_DTL.Dispose()
                .mobjSCGLSql.SQLDisconnect()
            End Try
        End With
    End Function

    '생성된 세금계산서 삭제
    Public Function DeleteRtn_MERGETax(ByVal strInfoXML As String, _
                                       ByVal strMTAXYEARMON As String, _
                                       ByVal intMTAXNO As Integer, _
                                       ByVal strMEDFLAG As String, _
                                       ByVal strVOCH_TYPE As String) As Integer
        Dim intRtn As Integer      'Return변수( 처리건수 또는 0 )
        Dim strMTAXNO As String

        SetConfig(strInfoXML)    '기본정보 Setting
        With mobjSCGLConfig    '기본정보 Config 개체
            Try
                ' 사용할Entity 개체생성(Config 정보를 넘겨생성)
                strMTAXNO = strMTAXYEARMON & "-" & intMTAXNO

                mobjcePD_MERGETAX_HDR = New cePD_MERGETAX_HDR(mobjSCGLConfig)
                mobjcePD_MERGETAX_DTL = New cePD_MERGETAX_DTL(mobjSCGLConfig)

                ' DB 접속 및 트랜잭션 시작
                .mobjSCGLSql.SQLConnect(.DBConnStr)
                .mobjSCGLSql.SQLBeginTrans()

                If strVOCH_TYPE = "3" Then
                    intRtn = mobjcePD_MERGETAX_HDR.AORTaxDeleteUpdateDo(strMTAXNO)
                Else
                    If strMEDFLAG = "D" Or strMEDFLAG = "G" Or strMEDFLAG = "O" Then
                        intRtn = mobjcePD_MERGETAX_HDR.MDTaxDeleteUpdateDo(strMTAXNO)

                    ElseIf strMEDFLAG = "A2" Or strMEDFLAG = "B" Or strMEDFLAG = "C" Then
                        intRtn = mobjcePD_MERGETAX_HDR.MDTruTaxDeleteUpdateDo(strMTAXNO)
                    End If
                End If

                intRtn = mobjcePD_MERGETAX_HDR.PDTaxDeleteUpdateDo(strMTAXNO)

                intRtn = mobjcePD_MERGETAX_HDR.DeleteMark(strMTAXYEARMON, intMTAXNO)

                intRtn = mobjcePD_MERGETAX_DTL.DeleteMark(strMTAXYEARMON, intMTAXNO)

                ' 트랜잭션 Commit
                .mobjSCGLSql.SQLCommitTrans()
                Return intRtn
            Catch err As Exception
                .mobjSCGLSql.SQLRollbackTrans()
                Throw RaiseSysErr(err, CLASS_NAME & "DeleteRtn_MERGETax")
            Finally
                'DB접속 종료
                .mobjSCGLSql.SQLDisconnect()
                '사용한 Entity(개체Dispose)
                mobjcePD_MERGETAX_HDR.Dispose()
                mobjcePD_MERGETAX_DTL.Dispose()
            End Try
        End With

    End Function


