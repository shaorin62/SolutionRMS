    '다음은 간접비 최초 조회시 저장 하는 로직이다.[INPUT 으로 저장]

    Public Function ProcessRtn_Indirect(ByVal strInfoXML As String, _
                                        ByVal vntData As Object) As Integer
        Dim intRtn As Integer '결과값 변수
        Dim i, intColCnt, intRows As Integer '루프, 컬럼Cnt, 로우Cnt 변수

        SetConfig(strInfoXML) '기본정보 Setting
        With mobjSCGLConfig '기본정보를 가지고 있는 Config 개체
            Try
                'XML Element 변수 선언 (strMasterXML을 변환)
                'DB접속 및 트랜잭션 시작
                .mobjSCGLSql.SQLConnect(.DBConnStr)
                .mobjSCGLSql.SQLBeginTrans()

                intRtn = ProcessRtn_INDIRECTDETAIL(vntData)

                .mobjSCGLSql.SQLCommitTrans()
                Return intRtn
            Catch err As Exception
                '트랜잭션RollBack 및 오류 전송
                .mobjSCGLSql.SQLRollbackTrans()
                Throw RaiseSysErr(err, CLASS_NAME & ".ProcessRtn_Indirect")
            Finally
                'Resource해제
                .mobjSCGLSql.SQLDisconnect()
            End Try
        End With
    End Function

    '다음은 최초 간접비 실제 저장 [최초에 팝업을 열었을때 템프에 파일 이 없을 경우 저장]

    Public Function ProcessRtn_INDIRECTDETAIL(ByVal vntData As Object) As Integer
        Dim intRtn As Integer
        Dim i, intColCnt, intRows As Integer
        Dim dbSEQ As Double '자동 ID 를사용할 때만 사용
        Dim strPREESTNO As String
        Dim dblITEMCODESEQ As Double
        Dim strSQL As String
        Dim intSave As Integer
        Dim strPRODUCTIONCOMMISSION As String
        Dim strCHK As String

        With mobjSCGLConfig
            Try
                If IsArray(vntData) Then
                    '''사용할 Entity 개체생성(Config 정보를 넘겨생성)
                    mobjcePD_PRODUCTIONCOMMI_INPUT = New cePD_PRODUCTIONCOMMI_INPUT(mobjSCGLConfig)

                    'strSQL = "DELETE FROM PD_PRODUCTIONCOMMI_INPUT"
                    'intSave = mobjcePD_PRODUCTIONCOMMI_INPUT.SqlExe(strSQL)

                    '''vntData의 컬럼수, 로우수를 변수입력
                    intColCnt = UBound(vntData, 1) : intRows = UBound(vntData, 2)
                    '''해당하는Row 만큼 Loop
                    For i = 1 To intRows
                        strPREESTNO = GetElement(vntData, "PREESTNO", intColCnt, i, OPTIONAL_STR)
                        dbSEQ = SelectRtn_INPUTSEQNO(strPREESTNO)
                        strPRODUCTIONCOMMISSION = GetElement(vntData, "PRODUCTIONCOMMISSION", intColCnt, i, OPTIONAL_STR)

                        If GetElement(vntData, "ITEMCODESEQ", intColCnt, i, NULL_NUM, True) = -999999 Then
                            dblITEMCODESEQ = "0"
                        Else
                            dblITEMCODESEQ = GetElement(vntData, "ITEMCODESEQ", intColCnt, i, NULL_NUM, True)
                        End If

                        'strPRODUCTIONCOMMISSION = "Y" 이 투입 조건은 더이상 투입 조건이 아니며 모든 항목이 투입 되며 단지 체크가 되어 투입 되느냐 차이이다.

                        If GetElement(vntData, "ITEMCODE", intColCnt, i, OPTIONAL_STR) <> "242001" Then
                            If strPRODUCTIONCOMMISSION = "Y" Then
                                strCHK = "1"
                            Else
                                strCHK = "0"
                            End If

                            intRtn = InsertRtn_Indirect(vntData, intColCnt, i, dbSEQ, dblITEMCODESEQ, strCHK)
                        End If

                    Next
                End If

                Return intRows
            Catch err As Exception

                Throw RaiseSysErr(err, CLASS_NAME & ".ProcessRtn_INDIRECTDETAIL")
            Finally
                mobjcePD_PRODUCTIONCOMMI_INPUT.Dispose()
            End Try
        End With
    End Function

    '간접비 팝업 저장처리
    Public Function ProcessRtn(ByVal strInfoXML As String, _
                               ByVal strMasterXML As String, _
                               ByVal vntData As Object, _
                               ByVal strPREESTNO As String) As Integer

        Dim intRtn As Integer '결과값 변수
        Dim CUSER As String

        SetConfig(strInfoXML) '기본정보 Setting
        With mobjSCGLConfig '기본정보를 가지고 있는 Config 개체
            Try
                CUSER = .WRKUSR

                mobjcePD_PRODUCTIONCOMMI_HDRINPUT = New cePD_PRODUCTIONCOMMI_HDRINPUT(mobjSCGLConfig)
                mobjcePD_PRODUCTIONCOMMI_INPUT = New cePD_PRODUCTIONCOMMI_INPUT(mobjSCGLConfig)

                'XML Element 변수 선언 (strMasterXML을 변환)
                Dim xmlRoot As XmlElement
                xmlRoot = XMLGetRoot(strMasterXML) 'XML 데이터

                'DB접속 및 트랜잭션 시작
                .mobjSCGLSql.SQLConnect(.DBConnStr)
                .mobjSCGLSql.SQLBeginTrans()
                'Master 데이터 처리

                '간접비 저장 하기전에 삭제
                mobjcePD_PRODUCTIONCOMMI_HDRINPUT.DeleteDo(strPREESTNO, CUSER)
                '간접비 임시 저장 저장
                intRtn = ProcessRtn_HDR(xmlRoot, strPREESTNO)

                '디테일 내역 저장하기 전에 삭제
                mobjcePD_PRODUCTIONCOMMI_INPUT.DeleteDo(strPREESTNO, CUSER)
                'Detail 데이터 처리
                intRtn = ProcessRtn_DETAIL(vntData, strPREESTNO)


                .mobjSCGLSql.SQLCommitTrans()
                Return intRtn
            Catch err As Exception
                '트랜잭션RollBack 및 오류 전송
                .mobjSCGLSql.SQLRollbackTrans()
                Throw RaiseSysErr(err, CLASS_NAME & ".ProcessRtn")
            Finally
                'Resource해제
                .mobjSCGLSql.SQLDisconnect()
                mobjcePD_PRODUCTIONCOMMI_HDRINPUT.Dispose()
                mobjcePD_PRODUCTIONCOMMI_INPUT.Dispose()
            End Try
        End With
    End Function
