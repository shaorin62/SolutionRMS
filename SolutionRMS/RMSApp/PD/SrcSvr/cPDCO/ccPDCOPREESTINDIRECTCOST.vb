            Finally
                mobjcePD_PRODUCTIONCOMMI_HDRINPUT.Dispose()
            End Try
        End With
    End Function

    '간접비 입력 수정, 삭제
    Public Function ProcessRtn_DETAIL(ByVal vntData As Object, _
                                      ByVal strPREESTNO As String) As Integer
        Dim intRtn As Integer
        Dim i, intColCnt, intRows As Integer
        Dim dblSEQ As Double '자동 ID 를사용할 때만 사용


        Dim strTempSql As String
        With mobjSCGLConfig
            Try
                If IsArray(vntData) Then
                    '''사용할 Entity 개체생성(Config 정보를 넘겨생성)
                    mobjcePD_PRODUCTIONCOMMI_HDRINPUT = New cePD_PRODUCTIONCOMMI_HDRINPUT(mobjSCGLConfig)
                    '''vntData의 컬럼수, 로우수를 변수입력
                    intColCnt = UBound(vntData, 1) : intRows = UBound(vntData, 2)
                    '''해당하는Row 만큼 Loop
                    For i = 1 To intRows

                        dblSEQ = SelectRtn_INPUTSEQNO(strPREESTNO)
                        intRtn = InsertRtn_DetailINPUT(vntData, intColCnt, i, dblSEQ)
                    Next
                End If

                Return intRows
            Catch err As Exception

                Throw RaiseSysErr(err, CLASS_NAME & ".ProcessRtn_DETAIL")
            Finally
                mobjcePD_PRODUCTIONCOMMI_HDRINPUT.Dispose()
            End Try
        End With

    End Function


