    '간접비 상세내역 조회
    Public Function SelectRtn_Detail(ByVal strInfoXML As String, _
                                     ByRef intRowCnt As Integer, _
                                     ByRef intColCnt As Integer, _
                                     ByVal strPREESTNO As String) As Object

        Dim strCols As String         '컬럼변수
        Dim strWhere As String       'Where조건 변수
        Dim strFormat As String      'SQL Format 변수
        Dim strSQL As String          'SQL 변수
        Dim vntData As Object        'Array Return 변수(Array 를사용할 때 선언)
        Dim strXMLData As String    'XML  Return 변수(XML  을 사용할 때 선언)


        SetConfig(strInfoXML) '기본정보 Setting
        With mobjSCGLConfig '기본정보 Config 개체

            'strSQL = " SELECT B.CHK,B.EXECHK,A.PREESTNO,B.SEQ,A.ITEMCODESEQ,A.ITEMCODE,  "
            'strSQL = strSQL & " DBO.PD_ITEMDIVNAME_FUN(A.ITEMCODE) DIVNAME,  "
            'strSQL = strSQL & " DBO.PD_ITEMCLASSNAME_FUN(A.ITEMCODE) CLASSNAME,  "
            'strSQL = strSQL & " DBO.PD_ITEMCODENAME_FUN(A.ITEMCODE) ITEMCODENAME,  "
            'strSQL = strSQL & " ISNULL(A.AMT,0) AMT,  "
            'strSQL = strSQL & " ISNULL(B.EXEAMT,0) EXEAMT,  "
            'strSQL = strSQL & " cast(DBO.PD_SUBITEMSORTSEQ_FUN(A.ITEMCODE) as numeric) SORTSEQ, "
            'strSQL = strSQL & " CASE ISNULL(B.SEQ,0) WHEN 0 THEN 'I' ELSE  "
            'strSQL = strSQL & " ( "
            'strSQL = strSQL & "     CASE WHEN ISNULL(A.AMT,0) <> ISNULL(B.AMT,0) THEN 'U' ELSE '' END "
            'strSQL = strSQL & " )"
            'strSQL = strSQL & " END AS SAVEFLAG  "
            'strSQL = strSQL & " FROM  "
            'strSQL = strSQL & " ( "
            'strSQL = strSQL & "     SELECT "
            'strSQL = strSQL & "     PREESTNO,ITEMCODESEQ,ITEMCODE,AMT,EXEAMT "
            'strSQL = strSQL & "     FROM PD_PRODUCTIONCOMMI_INPUT "
            'strSQL = strSQL & " ) A LEFT JOIN  PD_PRODUCTIONCOMMI_DTL B "
            'strSQL = strSQL & " ON A.PREESTNO = B.PREESTNO and A.ITEMCODESEQ = B.ITEMCODESEQ AND A.ITEMCODE = B.ITEMCODE "
            'strSQL = strSQL & " WHERE A.PREESTNO = '" & strPREESTNO & "' "
            'strSQL = strSQL & " union all "
            'strSQL = strSQL & " SELECT  "
            'strSQL = strSQL & " A.CHK,A.EXECHK,A.PREESTNO,A.SEQ,A.ITEMCODESEQ,A.ITEMCODE, "
            'strSQL = strSQL & " DBO.PD_ITEMDIVNAME_FUN(A.ITEMCODE) DIVNAME,  "
            'strSQL = strSQL & " DBO.PD_ITEMCLASSNAME_FUN(A.ITEMCODE) CLASSNAME,  "
            'strSQL = strSQL & " DBO.PD_ITEMCODENAME_FUN(A.ITEMCODE) ITEMCODENAME,  "
            'strSQL = strSQL & " ISNULL(A.AMT,0) AMT, "
            'strSQL = strSQL & " ISNULL(A.EXEAMT,0) EXEAMT, "
            'strSQL = strSQL & " 999 SORTSEQ,'D' SAVEFLAG "
            'strSQL = strSQL & " FROM PD_PRODUCTIONCOMMI_DTL A, "
            'strSQL = strSQL & " ( "
            'strSQL = strSQL & "     select itemcodeseq,itemcode from PD_PRODUCTIONCOMMI_DTL "
            'strSQL = strSQL & "     EXCEPT    "
            'strSQL = strSQL & "     select itemcodeseq,itemcode from PD_PRODUCTIONCOMMI_INPUT "
            'strSQL = strSQL & " ) B WHERE A.ITEMCODESEQ = B.ITEMCODESEQ AND A.ITEMCODE = B.ITEMCODE "
            'strSQL = strSQL & " AND A.PREESTNO = '" & strPREESTNO & "' "


            strSQL = " SELECT "
            strSQL = strSQL & " CHK,EXECHK,PREESTNO,SEQ,ITEMCODESEQ,ITEMCODE, "
            strSQL = strSQL & " DBO.PD_ITEMDIVNAME_FUN(ITEMCODE) DIVNAME,  "
            strSQL = strSQL & " DBO.PD_ITEMCLASSNAME_FUN(ITEMCODE) CLASSNAME,  "
            strSQL = strSQL & " DBO.PD_ITEMCODENAME_FUN(ITEMCODE) ITEMCODENAME,  "
            strSQL = strSQL & " ISNULL(AMT,0) AMT, "
            strSQL = strSQL & " ISNULL(EXEAMT,0) EXEAMT, "
            strSQL = strSQL & " ATTR06 PRINT_SEQ"
            strSQL = strSQL & " FROM PD_PRODUCTIONCOMMI_DTL "
            strSQL = strSQL & " WHERE PREESTNO = '" & strPREESTNO & "' "
            strSQL = strSQL & " ORDER BY PRINT_SEQ "

            Try
                ' DB 접속
                .mobjSCGLSql.SQLConnect(.DBConnStr)
                ' ------ Array 데이터 조회 (True 일때 헤더정보 포함 조회(Sheet Data Binding 할 경우 사용), False 일때 데이터만 조회)
                vntData = .mobjSCGLSql.SQLSelectArr(strSQL, intRowCnt, intColCnt, , True)
                Return vntData

            Catch err As Exception
                ' 오류 전송
                Throw RaiseSysErr(err, CLASS_NAME & ".SelectRtn_Detail")
            Finally
                ' DB 접속 종료
                .mobjSCGLSql.SQLDisconnect()
            End Try
        End With
    End Function
