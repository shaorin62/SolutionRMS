    Public Function SelectRtn_HDR(ByVal strInfoXML As String, _
                                  ByRef intRowCnt As Integer, _
                                  ByRef intColCnt As Integer, _
                                  ByVal strFROM As String, _
                                  ByVal strTO As String, _
                                  ByVal strJOBNO As String, _
                                  ByVal strJOBNAME As String, _
                                  ByVal strCUSTCODE As String, _
                                  ByVal strCUSTNAME As String) As Object     'XML  데이터 조회시

        Dim strSQL As String
        Dim strFormet, strSelFields, strWhere As String
        Dim Con1, Con2, Con3, Con4, Con5 As String
        Dim vntData As Object
        Dim strFDATE, strTDATE

        With mobjSCGLConfig
            Try

                SetConfig(strInfoXML)   '기본정보 설정

                Con1 = "" : Con2 = "" : Con3 = "" : Con4 = "" : Con5 = ""

                If strJOBNO <> "" Then Con1 = String.Format(" AND (JOBNO = '{0}')", strJOBNO)
                If strJOBNAME <> "" Then Con1 = String.Format(" AND (dbo.PD_JOBNAME_FUN(JOBNO) LIKE '%{0}%')", strJOBNAME)
                If strCUSTCODE <> "" Then Con2 = String.Format(" AND (CUSTCODE = '{0}')", strCUSTCODE)
                If strCUSTNAME <> "" Then Con3 = String.Format(" AND (dbo.MD_GET_CUSTNAME_FUN(CUSTCODE) LIKE '%{0}%')", strCUSTNAME)
                If strFROM <> "" Then Con4 = String.Format(" AND (CREDAY >= '{0}')", strFROM)
                If strTO <> "" Then Con5 = String.Format(" AND (CREDAY <= '{0}')", strTO)

                strWhere = BuildFields(" ", Con1, Con2, Con3, Con4, Con5)

                'REVSEQ|JOBNO|OUTSCODE|OUTSNAME|CUSTCODE|CUSTNAME|BTN|AMT|CREDAY|CONFIRMFLAG|BIGO
                strFormet = " SELECT "
                strFormet = strFormet & " 0 CHK ,"
                strFormet = strFormet & " REVSEQ ,"
                strFormet = strFormet & " JOBNO ,"
                strFormet = strFormet & " dbo.PD_JOBNAME_FUN(JOBNO) JOBNAME, "
                strFormet = strFormet & " OUTSCODE ,"
                strFormet = strFormet & " OUTSNAME ,"
                strFormet = strFormet & " CUSTCODE ,"
                strFormet = strFormet & " dbo.MD_GET_CUSTNAME_FUN(CUSTCODE) CUSTNAME, "
                strFormet = strFormet & " AMT ,"
                strFormet = strFormet & " CREDAY ,"
                strFormet = strFormet & " CONFIRMFLAG, "
                strFormet = strFormet & " BIGO "
                strFormet = strFormet & " FROM PD_PREBUDGET_HDR WHERE 1=1 {0}"

                strSQL = String.Format(strFormet, strWhere)

                .mobjSCGLSql.SQLConnect(.DBConnStr)
                vntData = .mobjSCGLSql.SQLSelectArr(strSQL, intRowCnt, intColCnt, , True)
                Return vntData
            Catch err As Exception
                Throw RaiseSysErr(err, CLASS_NAME & ".SelectRtn_HDR")
            Finally
                .mobjSCGLSql.SQLDisconnect()
            End Try
        End With
    End Function


