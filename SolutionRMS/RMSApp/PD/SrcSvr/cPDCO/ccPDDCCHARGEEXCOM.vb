    '============== ProcessRtn
    Public Function ProcessRtn(ByVal strInfoXML As String, _
                               ByVal vntData_PREBUDGET_HDR As Object, _
                               ByVal vntData_PREBUDGET_DTL As Object, _
                               ByVal strJOBNO As String, _
                               ByVal strOLDSEQ As Integer) As Integer                      '데이터 INSERT/UPDATE

        Dim intRtn As Integer
        Dim i, intColCnt, intRows, intCnt As Integer
        Dim strSQL As String
        Dim dblSEQ, dblREVSEQ_HDR, dblREVSEQ_DTL As Double
        Dim strCREDAY As String
        Dim strOUTSCODE As String
        Dim strITEMCODE As String
        Dim strNOW As String
        Dim strCheck As Integer

        strCheck = 0
        SetConfig(strInfoXML)

        With mobjSCGLConfig
            Try
                .mobjSCGLSql.SQLConnect(.DBConnStr)
                .mobjSCGLSql.SQLBeginTrans()

                '''사용할 Entity 개체생성(Config 정보를 넘겨생성)
                mobjcePD_PREBUDGET_HDR = New cePD_PREBUDGET_HDR(mobjSCGLConfig)
                mobjcePD_PREBUDGET_DTL = New cePD_PREBUDGET_DTL(mobjSCGLConfig)

                '현재 쓰던 로직과 동일한 헤더 저장
                '=========================================================================================
                'MST 
                '''vntData_PREBUDGET_HDR
                If IsArray(vntData_PREBUDGET_HDR) Then
                    intColCnt = UBound(vntData_PREBUDGET_HDR, 1) : intRows = UBound(vntData_PREBUDGET_HDR, 2)
                    For i = 1 To intRows
                        If GetElement(vntData_PREBUDGET_HDR, "REVSEQ", intColCnt, i, NULL_NUM, True) = -999999 Then
                            dblREVSEQ_HDR = SelectRtn_HDR_SEQNO("PD_PREBUDGET_HDR")
                            strOUTSCODE = SelectRtn_CHARGEBASICLIST_PRICENO("PD_PREBUDGET_HDR")

                            If GetElement(vntData_PREBUDGET_HDR, "CREDAY", intColCnt, i, OPTIONAL_STR) <> "" Then strCREDAY = GetElement(vntData_PREBUDGET_HDR, "CREDAY", intColCnt, i, OPTIONAL_STR).SUBSTRING(0, 4) & GetElement(vntData_PREBUDGET_HDR, "CREDAY", intColCnt, i, OPTIONAL_STR).SUBSTRING(5, 2) & GetElement(vntData_PREBUDGET_HDR, "CREDAY", intColCnt, i, OPTIONAL_STR).SUBSTRING(8, 2)
                            intRtn = InsertRtn_PD_PREBUDGET_HDR(vntData_PREBUDGET_HDR, intColCnt, i, strJOBNO, strOUTSCODE, dblREVSEQ_HDR, strCREDAY)
                        Else
                            strNOW = Now
                            If GetElement(vntData_PREBUDGET_HDR, "CREDAY", intColCnt, i, OPTIONAL_STR) <> "" Then strCREDAY = strNOW.Substring(0, 4) & strNOW.Substring(5, 2) & strNOW.Substring(8, 2)
                            intRtn = UpdateRtn_PD_PREBUDGET_HDR(vntData_PREBUDGET_HDR, intColCnt, i, strCREDAY)
                            '업데이트없음
                            strCheck = 0
                        End If
                    Next

                End If

                '헤더의SEQ (OLDSEQ)에따른 DTL에 저장하는 로직.
                '=========================================================================================
                'DTL
                '''vntData_PREBUDGET_DTL
                If IsArray(vntData_PREBUDGET_DTL) Then
                    intColCnt = UBound(vntData_PREBUDGET_DTL, 1) : intRows = UBound(vntData_PREBUDGET_DTL, 2)
                    For i = 1 To intRows

                        '헤더에서 신규저장시 생성한 코드를 가져온다. 이렇게 해도되는이유는 단일 저장 밖에 되지 않기 때문
                        dblREVSEQ_DTL = dblREVSEQ_HDR
                        '만약에 신규저장시 생선한게 없다면  UPDATE이기 때문에 MSTSEQ에 가져온 헤더SEQ를 넣는다
                        If dblREVSEQ_DTL = 0 Then
                            dblREVSEQ_DTL = strOLDSEQ
                        End If

                        If GetElement(vntData_PREBUDGET_DTL, "SEQ", intColCnt, i, NULL_NUM, True) = -999999 Then
                            dblSEQ = SelectRtn_DTL_SEQNO("PD_PREBUDGET_DTL", dblREVSEQ_DTL)
                            intRtn = InsertRtn_PD_PREBUDGET_DTL(vntData_PREBUDGET_DTL, intColCnt, i, strOUTSCODE, dblREVSEQ_DTL, dblSEQ)
                        Else
                            intRtn = UpdateRtn_PD_PREBUDGET_DTL(vntData_PREBUDGET_DTL, intColCnt, i)
                            strCheck = 1
                        End If
                    Next
                End If


                '하위가 업데이트 되었으면 헤더날짜 오늘날짜로 변경 , 헤더의 금액 업데이트
                If strCheck = 1 Then
                    UpdateRtn_PREBUDGET_HDR_CREDAY(dblREVSEQ_DTL, intColCnt, 1)
                    UpdateRtn_PREBUDGET_HDR_AMT(dblREVSEQ_DTL)
                End If


                .mobjSCGLSql.SQLCommitTrans()
                Return intRows
            Catch err As Exception
                .mobjSCGLSql.SQLRollbackTrans()
                Throw RaiseSysErr(err, CLASS_NAME & ".ProcessRtn")
            Finally
                .mobjSCGLSql.SQLDisconnect()
                mobjcePD_PREBUDGET_HDR.Dispose()
                mobjcePD_PREBUDGET_DTL.Dispose()
            End Try

        End With

    End Function


    

    '============== SelectRtn_PRICE_SEQNO 생성
    Public Function SelectRtn_HDR_SEQNO(ByVal strEntity As String) As Integer

        '여기부터 단순조회
        Dim strSQL, strFormat As String
        Dim strRtn As Integer

        With mobjSCGLConfig '기본정보 Config 개체
            Try
                strSQL = "SELECT ISNULL(MAX(REVSEQ),0) +1 FROM " & strEntity & ""
                strRtn = .mobjSCGLSql.SQLSelectOneScalar(strSQL)

                Return strRtn
            Catch err As Exception
                ' 오류 전송
                Throw RaiseSysErr(err, CLASS_NAME & ".SelectRtn_HDR_SEQNO")
            Finally
            End Try
        End With
        '여기까지 단순조회
    End Function


