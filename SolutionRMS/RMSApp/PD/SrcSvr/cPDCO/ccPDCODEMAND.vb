    Public Function Data_Confirm(ByVal strInfoXML As String, _
                                 ByVal vntData As Object, _
                                 ByVal strConfirmFlag As String) As Integer '데이터 INSERT/UPDATE

        Dim intRtn As Integer
        Dim i, intColCnt, intRows As Integer
        Dim dblID As Double
        Dim strPREESTNO As String
        Dim strJOBNO As String
        Dim strYEARMON As String
        Dim dblSUBNO As Double
        Dim strDTLSQL As String
        Dim intSaveRtn As Integer
        Dim strUSER As String
        Dim strTXTSQL As String
        Dim strUSENO As String
        Dim strMANAGER As String
        Dim intTempRtn As Integer
        Dim intDIVROW As Integer
        Dim intDIVROWCNT As Integer
        Dim strJOBNOCHEK As String

        SetConfig(strInfoXML)

        strUSER = mobjSCGLConfig.WRKUSR

        With mobjSCGLConfig
            Try
                .mobjSCGLSql.SQLConnect(.DBConnStr)
                .mobjSCGLSql.SQLBeginTrans()
                If IsArray(vntData) Then
                    '''사용할 Entity 개체생성(Config 정보를 넘겨생성)
                    mobjcePD_DIVAMT = New cePD_DIVAMT(mobjSCGLConfig)
                    '''vntData의 컬럼수, 로우수를 변수입력
                    intColCnt = UBound(vntData, 1) : intRows = UBound(vntData, 2)
                    For i = 1 To intRows
                        If GetElement(vntData, "CHK", intColCnt, i) = "" Or GetElement(vntData, "CHK", intColCnt, i) = "0" Then

                        Else
                            dblSUBNO = GetElement(vntData, "SEQ", intColCnt, i, NULL_NUM, True)
                            strPREESTNO = GetElement(vntData, "PREESTNO", intColCnt, i)
                            strJOBNO = GetElement(vntData, "JOBNO", intColCnt, i)
                            strYEARMON = GetElement(vntData, "YEARMON", intColCnt, i)
                            strUSENO = GetElement(vntData, "USENO", intColCnt, i)
                            strMANAGER = GetElement(vntData, "MANAGER", intColCnt, i)

                            If strConfirmFlag = "0" Then

                                intDIVROW = SelectRtn_DIVAMT_DIVROW(strYEARMON, strJOBNO)
                                intDIVROWCNT = SelectRtn_DIVAMT_CONFIRMCOUNT(strYEARMON, strJOBNO)
                                '찢어진게 없다면
                                If intDIVROW = 1 Then
                                    If intDIVROWCNT = 0 Then
                                        strDTLSQL = "UPDATE PD_DIVAMT SET CONFIRMFLAG = '1',MANAGER= '" & strUSER & "',ADJAMT = 0,CHARGE=0 WHERE PREESTNO = '" & strPREESTNO & "' "
                                        strDTLSQL = strDTLSQL & " AND YEARMON = '" & strYEARMON & "' AND JOBNO = '" & strJOBNO & "' AND SEQ =" & dblSUBNO
                                        strDTLSQL = strDTLSQL & " ; DELETE FROM PD_DIVAMT WHERE JOBNO = '" & strJOBNO & "' AND SEQ > 100 AND SEQ%100 =" & dblSUBNO
                                    End If
                                Else
                                    If intDIVROWCNT = 0 Then
                                        strDTLSQL = strDTLSQL & " update a set"
                                        strDTLSQL = strDTLSQL & " a.DIVAMT = b.DIVAMT, CONFIRMFLAG = '1', attr10 = 99999, MANAGER= '" & strUSER & "', JOBNAME = DBO.PD_JOBNAME_FUN(B.JOBNO), ADJAMT = 0, CHARGE=0 "
                                        strDTLSQL = strDTLSQL & " from pd_divamt a , (select yearmon,jobno, sum(isnull(divamt,0)) divamt from pd_divamt"
                                        strDTLSQL = strDTLSQL & " where yearmon = '" & strYEARMON & "' and jobno = '" & strJOBNO & "'"
                                        strDTLSQL = strDTLSQL & " group by yearmon,jobno) b"
                                        strDTLSQL = strDTLSQL & " where a.jobno = b.jobno and a.seq =1"
                                        strDTLSQL = strDTLSQL & " ; DELETE FROM PD_DIVAMT WHERE JOBNO = '" & strJOBNO & "' AND SEQ <> 1 "
                                    End If
                                End If

                            Else
                                strDTLSQL = "UPDATE PD_DIVAMT SET CONFIRMFLAG = '" & strConfirmFlag & "',MANAGER= '" & strUSER & "' ,attr10 = null WHERE PREESTNO = '" & strPREESTNO & "' "
                                strDTLSQL = strDTLSQL & " AND YEARMON = '" & strYEARMON & "' AND JOBNO = '" & strJOBNO & "' AND SEQ =" & dblSUBNO
                            End If
                            '명령실행 PD_DIVAMT CONFIRMFLAG 를 교체한다.

                            intSaveRtn = mobjcePD_DIVAMT.SqlExe(strDTLSQL)

                            'SMS 발송리스트 임시저장
                            strTXTSQL = "INSERT INTO PD_SMS_TEMP (JOBNO,USENO,MANAGER,CONFIRMFLAG) VALUES ('" & strJOBNO & "','" & strUSENO & "','" & strMANAGER & "','" & strConfirmFlag & "')"
                            intTempRtn = mobjcePD_DIVAMT.SqlExe(strTXTSQL)
                        End If
                    Next
                End If
                .mobjSCGLSql.SQLCommitTrans()
                Return intRows
            Catch err As Exception
                .mobjSCGLSql.SQLRollbackTrans()
                Throw RaiseSysErr(err, CLASS_NAME & ".Data_Confirm")
            Finally
                .mobjSCGLSql.SQLDisconnect()
                mobjcePD_DIVAMT.Dispose()
            End Try
        End With
    End Function

