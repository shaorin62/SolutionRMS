                              ByRef intColCnt As Integer, _
                              ByVal strFROM As String, _
                              ByVal strTO As String, _
                              ByVal strDEPTCD As String, _
                              ByVal strMANAGER As String) As Object

        Dim strCols As String        '컬럼변수
        Dim strWhere As String       'Where조건 변수 
        Dim strFormat As String      'SQL Format 변수
        Dim strSQL As String         'SQL 변수
        Dim vntData As Object        'Array Return 변수(Array 를사용할 때 선언)
        Dim strXMLData As String     'XML  Return 변수(XML  을 사용할 때 선언)
        Dim Con1 As String           '조건 String 변수
        Dim strUSER As String


        SetConfig(strInfoXML) '기본정보 Setting

        strUSER = mobjSCGLConfig.WRKUSR
        strCols = strCols & "CASE ISNULL(C.PREESTNO,'') WHEN '' THEN 0 ELSE 1 END CHK, "
        strCols = strCols & "A.YEARMON, "
        strCols = strCols & "A.PREESTNO, "
        strCols = strCols & "A.JOBNAME, "
        strCols = strCols & "A.JOBNO, "
        strCols = strCols & "A.SEQ SUBNO, "
        strCols = strCols & "A.CLIENTCODE, "
        strCols = strCols & "DBO.SC_GET_HIGHCUSTNAME_FUN(A.CLIENTCODE) CLIENTNAME, "
        strCols = strCols & "ISNULL(A.DIVAMT,0) DIVAMT, "
        strCols = strCols & "CASE ISNULL(C.ADJAMT,0) WHEN 0 THEN ISNULL(A.ADJAMT,0)  ELSE ISNULL(C.ADJAMT,0) END AS ADJAMT, "
        strCols = strCols & "CASE ISNULL(C.CHARGE,9999999999999) WHEN 9999999999999 THEN ISNULL(A.DIVAMT,0)-ISNULL(A.ADJAMT,0) ELSE C.CHARGE END AS CHARGE,  "
        strCols = strCols & "ISNULL(A.DIVAMT,0)-ISNULL(A.ADJAMT,0) OLDCHARGE, "
        strCols = strCols & "CASE ISNULL(C.DEMANDFLAG,'') WHEN '' THEN '' ELSE C.DEMANDFLAG END AS DEMANDFLAG, "
        strCols = strCols & "DBO.PD_JOBNAME_FUN(A.JOBNO) PRIORITYJOB, "
        strCols = strCols & "CASE ISNULL(C.MEMO,'') WHEN '' THEN '' ELSE C.MEMO END AS MEMO, "
        strCols = strCols & "CASE ISNULL(C.TAXCODE,'') WHEN '' THEN '' ELSE ISNULL(C.TAXCODE,'')  END AS TAXCODE,A.TIMCODE,DBO.SC_GET_CUSTNAME_FUN(A.TIMCODE) TIMNAME, "
        strCols = strCols & "A.SUBSEQ,DBO.SC_GET_SUBSEQNAME_FUN(A.SUBSEQ) SUBSEQNAME,A.CREDAY,CASE ISNULL(C.PREESTNO,'') WHEN '' THEN 'N' ELSE 'Y' END AS SAVEFLAG,'" & strUSER & "' USENO,"
        strCols = strCols & "CASE ISNULL(C.CONFIRMFLAG,'') WHEN '' THEN A.CONFIRMFLAG ELSE C.CONFIRMFLAG END AS CONFIRMFLAG,A.YEARMON DATAYEARMON "

        If strFROM <> "" And strTO <> "" Then
            Con1 = String.Format(" AND (A.YEARMON  BETWEEN '{0}' AND  '{1}')", strFROM, strTO)
        End If
        If strFROM <> "" And strTO = "" Then
            Con1 = String.Format(" AND (A.YEARMON  >= '{0}')", strFROM)
        End If
        If strFROM = "" And strTO <> "" Then
            Con1 = String.Format(" AND (A.YEARMON  <= '{0}')", strTO)

        End If
        strWhere = BuildFields(" ", Con1)
        If strMANAGER = "Y" Then
            strFormat = "SELECT {0} FROM PD_DIVAMT A LEFT JOIN PD_DEMAND_DTL C ON A.PREESTNO = C.PREESTNO AND A.JOBNO = C.JOBNO AND A.SEQ = C.SUBNO AND A.YEARMON = C.YEARMON WHERE A.CONFIRMFLAG = '1' {1}"
        Else
            strFormat = "SELECT {0} FROM (SELECT JOBNO FROM PD_JOBNO WHERE DEPTCD = '" & strDEPTCD & "' Or EMPNO = '" & strUSER & "') B,PD_DIVAMT A LEFT JOIN PD_DEMAND_DTL C ON A.PREESTNO = C.PREESTNO AND A.JOBNO = C.JOBNO AND A.SEQ = C.SUBNO AND A.YEARMON = C.YEARMON WHERE A.JOBNO = B.JOBNO AND A.CONFIRMFLAG = '1' {1}"
        End If
        With mobjSCGLConfig '기본정보 Config 개체
            strSQL = String.Format(strFormat, strCols, strWhere)
            Try
                .mobjSCGLSql.SQLConnect(.DBConnStr)
                vntData = .mobjSCGLSql.SQLSelectArr(strSQL, intRowCnt, intColCnt, , True)
                Return vntData

            Catch err As Exception
                ' 오류 전송
                Throw RaiseSysErr(err, CLASS_NAME & ".SelectRtn")
            Finally
                ' DB 접속 종료
                .mobjSCGLSql.SQLDisconnect()
            End Try
        End With
    End Function

