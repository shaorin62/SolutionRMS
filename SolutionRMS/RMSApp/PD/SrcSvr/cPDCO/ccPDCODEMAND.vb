    '============== 청구요청 하단그리드 작성 저장
    Public Function ProcessRtn(ByVal strInfoXML As String, _
                               ByVal vntData As Object, _
                               ByVal vntData_Hdr As Object, _
                               ByVal strPREESTNOHDR As String) As Integer '데이터 INSERT/UPDATE
        Dim intRtn As Integer
        Dim i, intColCnt, intRows As Integer
        Dim j, intColCnt2, intRows2 As Integer
        Dim dblID As Double
        Dim strPREESTNO As String
        Dim strJOBNO As String
        Dim intUpdateRtn As Integer
        Dim strCREDAY As String
        Dim strCREDAYHDR As String
        Dim intCnt As Integer
        Dim strMEMO As String

        SetConfig(strInfoXML)
        With mobjSCGLConfig
            Try
                .mobjSCGLSql.SQLConnect(.DBConnStr)
                .mobjSCGLSql.SQLBeginTrans()
                '하단그리드 저장처리
                If IsArray(vntData) Then
                    '''사용할 Entity 개체생성(Config 정보를 넘겨생성)
                    mobjcePD_DEMAND_MST = New cePD_DEMAND_MST(mobjSCGLConfig)
                    '''vntData의 컬럼수, 로우수를 변수입력
                    intColCnt = UBound(vntData, 1) : intRows = UBound(vntData, 2)
                    intCnt = 0
                    For i = 1 To intRows
                        '인서트
                        strCREDAY = GetElement(vntData, "CREDAY", intColCnt, i)
                        strCREDAY = Mid(strCREDAY, 1, 4) & Mid(strCREDAY, 6, 2) & Mid(strCREDAY, 9, 2)
                        If GetElement(vntData, "SEQ", intColCnt, i, NULL_NUM, True) = -999999 Then
                            strPREESTNO = GetElement(vntData, "PREESTNO", intColCnt, i)
                            strJOBNO = GetElement(vntData, "JOBNO", intColCnt, i)

                            dblID = SelectRtn_SEQNO(strPREESTNO, strJOBNO)
                            intRtn = InsertRtn(vntData, intColCnt, i, dblID, strCREDAY)
                            intCnt = intCnt + 1
                        Else
                            intRtn = UpdateRtn(vntData, intColCnt, i, strCREDAY)
                        End If
                    Next
                End If

                '상단그리드 1행 처리
                If IsArray(vntData_Hdr) Then
                    '''사용할 Entity 개체생성(Config 정보를 넘겨생성)
                    mobjcePD_DEMAND_DTL = New cePD_DEMAND_DTL(mobjSCGLConfig)
                    '''vntData의 컬럼수, 로우수를 변수입력
                    intColCnt2 = UBound(vntData_Hdr, 1) : intRows2 = UBound(vntData_Hdr, 2)
                    For j = 1 To intRows2
                        '인서트
                        strCREDAYHDR = GetElement(vntData_Hdr, "CREDAY", intColCnt2, j)
                        strCREDAYHDR = Mid(strCREDAYHDR, 1, 4) & Mid(strCREDAYHDR, 6, 2) & Mid(strCREDAYHDR, 9, 2)
                        If strPREESTNOHDR = GetElement(vntData_Hdr, "PREESTNO", intColCnt2, j) Then
                            strMEMO = intCnt & " 건"
                            If GetElement(vntData_Hdr, "SAVEFLAG", intColCnt2, j) = "N" Then
                                intRtn = InsertRtn_DTL(vntData_Hdr, intColCnt2, j, strCREDAYHDR, strMEMO)
                            Else
                                intRtn = UpdateRtn_DTL(vntData_Hdr, intColCnt2, j, strCREDAYHDR)
                            End If
                        End If
                    Next
                End If

                .mobjSCGLSql.SQLCommitTrans()
                Return intRows
            Catch err As Exception
                .mobjSCGLSql.SQLRollbackTrans()
                Throw RaiseSysErr(err, CLASS_NAME & ".ProcessRtn")
            Finally
                .mobjSCGLSql.SQLDisconnect()
                mobjcePD_DEMAND_MST.Dispose()
                mobjcePD_DEMAND_DTL.Dispose()
            End Try
        End With
    End Function


