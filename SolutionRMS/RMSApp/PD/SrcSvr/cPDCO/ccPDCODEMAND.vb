    '실제 청구요청 부분
    Public Function ProcessRtn_Demand(ByVal strInfoXML As String, _
                                      ByVal vntData As Object) As Integer '데이터 INSERT/UPDATE
        Dim intRtn As Integer
        Dim i, intColCnt, intRows As Integer
        Dim dblID As Double
        Dim strPREESTNO As String
        Dim strJOBNO As String
        Dim strYEARMON As String
        Dim intUpdateRtn As Integer
        Dim dblSUBNO As Double
        Dim strDTLSQL As String
        Dim strMSTSQL As String
        Dim dblOLDSEQ As Double
        Dim strUSENO As String
        Dim dblHISTORYSEQ As Double
        Dim strHISTORY As String

        Dim intSaveRtn As Integer
        Dim intHISTORYRtn As Integer

        SetConfig(strInfoXML)

        strUSENO = mobjSCGLConfig.WRKUSR
        With mobjSCGLConfig
            Try

                .mobjSCGLSql.SQLConnect(.DBConnStr)
                .mobjSCGLSql.SQLBeginTrans()
                If IsArray(vntData) Then
                    '''사용할 Entity 개체생성(Config 정보를 넘겨생성)
                    mobjcePD_DIVAMT = New cePD_DIVAMT(mobjSCGLConfig)

                    '승인 요청 이력번호 생성
                    dblHISTORYSEQ = SelectRtn_DEMANDSEQ(strUSENO)


                    '''vntData의 컬럼수, 로우수를 변수입력
                    intColCnt = UBound(vntData, 1) : intRows = UBound(vntData, 2)
                    For i = 1 To intRows

                        '이력테이블 PD_DEMAND_DTL,PD_DEMAND_MST 의 내역 CONFIRMFLAG 교체 - 반려 0 이므로 반려건을 대비하여 0 으로 확정
                        dblSUBNO = GetElement(vntData, "SEQ", intColCnt, i, NULL_NUM, True)
                        dblOLDSEQ = GetElement(vntData, "OLDSEQ", intColCnt, i, NULL_NUM, True)
                        strPREESTNO = GetElement(vntData, "PREESTNO", intColCnt, i)
                        strJOBNO = GetElement(vntData, "JOBNO", intColCnt, i)
                        strYEARMON = GetElement(vntData, "YEARMON", intColCnt, i)





                        'PD_DIVAMT 인서트
                        If GetElement(vntData, "SEQ", intColCnt, i, NULL_NUM, True) = -999999 Then
                            dblID = SelectRtn_SUBNO(strPREESTNO, strJOBNO, strYEARMON)
                            '당월데이터 인서트
                            intRtn = InsertRtn_DIVAMT(vntData, intColCnt, i, dblID, dblHISTORYSEQ)
                        ElseIf GetElement(vntData, "SEQ", intColCnt, i, NULL_NUM, True) >= 100 Then
                            '차월데이터 인서트
                            dblID = GetElement(vntData, "SEQ", intColCnt, i, NULL_NUM, True)
                            intRtn = InsertRtn_DIVAMT(vntData, intColCnt, i, dblID, dblHISTORYSEQ)
                        Else
                            '업데이트
                            intRtn = UpdateRtn_DIVAMT(vntData, intColCnt, i, dblHISTORYSEQ)
                        End If

                        '이력은 PD_DIVAMT 에서 추출할수 있으니, PD_DEMAND_DTL 및 PD_DEMAND_MST 테이블의 활용은 여기까지,, 청구요청후 지운다. PD_DIVAMT 에 인서트 후 삭제
                        strDTLSQL = "DELETE FROM PD_DEMAND_DTL WHERE PREESTNO = '" & strPREESTNO & "' "
                        strDTLSQL = strDTLSQL & " AND YEARMON = '" & strYEARMON & "' AND JOBNO = '" & strJOBNO & "' AND SUBNO =" & dblSUBNO
                        intSaveRtn = mobjcePD_DIVAMT.SqlExe(strDTLSQL)

                        strMSTSQL = "DELETE FROM PD_DEMAND_MST WHERE PREESTNO = '" & strPREESTNO & "' "
                        strMSTSQL = strMSTSQL & "AND JOBNO = '" & strJOBNO & "' AND SEQ =" & dblOLDSEQ

                        intSaveRtn = mobjcePD_DIVAMT.SqlExe(strMSTSQL)
                    Next

                    strHISTORY = "INSERT INTO PD_DEMAND(USENO,SEQ,LOGDATE) VALUES('" & strUSENO & "'," & dblHISTORYSEQ & ",'" & Now & "' )"
                    intHISTORYRtn = mobjcePD_DIVAMT.SqlExe(strHISTORY)
                End If
                .mobjSCGLSql.SQLCommitTrans()
                Return intRows
            Catch err As Exception
                .mobjSCGLSql.SQLRollbackTrans()
                Throw RaiseSysErr(err, CLASS_NAME & ".ProcessRtn")
            Finally
                .mobjSCGLSql.SQLDisconnect()
                mobjcePD_DIVAMT.Dispose()
            End Try
        End With
    End Function

