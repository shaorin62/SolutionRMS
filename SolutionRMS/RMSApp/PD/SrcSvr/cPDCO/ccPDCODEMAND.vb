    Public Function ProcessRtn_HDR(ByVal strInfoXML As String, _
                                   ByVal vntData As Object) As Integer '데이터 INSERT/UPDATE
        Dim intRtn As Integer
        Dim i, intColCnt, intRows As Integer
        Dim strCREDAY As String
        Dim strMEMO As String
        SetConfig(strInfoXML)
        With mobjSCGLConfig
            Try

                .mobjSCGLSql.SQLConnect(.DBConnStr)
                .mobjSCGLSql.SQLBeginTrans()
                If IsArray(vntData) Then
                    '''사용할 Entity 개체생성(Config 정보를 넘겨생성)
                    mobjcePD_DEMAND_DTL = New cePD_DEMAND_DTL(mobjSCGLConfig)
                    '''vntData의 컬럼수, 로우수를 변수입력
                    intColCnt = UBound(vntData, 1) : intRows = UBound(vntData, 2)

                    For i = 1 To intRows
                        strCREDAY = GetElement(vntData, "CREDAY", intColCnt, i)
                        strCREDAY = Mid(strCREDAY, 1, 4) & Mid(strCREDAY, 6, 2) & Mid(strCREDAY, 9, 2)
                        '인서트
                        If GetElement(vntData, "CHK", intColCnt, i) = "" Then
                        Else
                            If GetElement(vntData, "DEMANDFLAG", intColCnt, i) = "DI01" Or GetElement(vntData, "DEMANDFLAG", intColCnt, i) = "DI02" Then
                                If GetElement(vntData, "SAVEFLAG", intColCnt, i) = "N" Then
                                    strMEMO = GetElement(vntData, "MEMO", intColCnt, i)
                                    intRtn = InsertRtn_DTL(vntData, intColCnt, i, strCREDAY, strMEMO)
                                Else
                                    intRtn = UpdateRtn_DTL(vntData, intColCnt, i, strCREDAY)
                                End If
                            End If
                        End If
                    Next
                End If
                .mobjSCGLSql.SQLCommitTrans()
                Return intRows
            Catch err As Exception
                .mobjSCGLSql.SQLRollbackTrans()
                Throw RaiseSysErr(err, CLASS_NAME & ".ProcessRtn_HDR")
            Finally
                .mobjSCGLSql.SQLDisconnect()
                mobjcePD_DEMAND_DTL.Dispose()
            End Try
        End With
    End Function

