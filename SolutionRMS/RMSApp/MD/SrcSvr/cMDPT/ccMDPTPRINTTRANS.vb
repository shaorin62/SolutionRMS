    ' =============== DeleteRtn Sample Code
    Public Function DeleteRtn(ByVal strInfoXML As String, _
                              ByVal vntData As Object) As Integer   '데이터 DELETE

        Dim intRtn As Integer      'Return변수( 처리건수 또는 0 )
        Dim intColCnt, intRows
        Dim strTRANSNO
        Dim strTRANSYEARMON
        Dim i

        SetConfig(strInfoXML)    '기본정보 Setting
        With mobjSCGLConfig    '기본정보 Config 개체
            Try
                ' 사용할Entity 개체생성(Config 정보를 넘겨생성)
                mobjceMD_BOOKING_MEDIUM = New ceMD_BOOKING_MEDIUM(mobjSCGLConfig)
                mobjceMD_PRINTTRANS_HDR = New ceMD_PRINTTRANS_HDR(mobjSCGLConfig)
                mobjceMD_PRINTTRANS_DTL = New ceMD_PRINTTRANS_DTL(mobjSCGLConfig)
                ' DB 접속 및 트랜잭션 시작
                .mobjSCGLSql.SQLConnect(.DBConnStr)
                .mobjSCGLSql.SQLBeginTrans()
                ' 엔티티 오브젝트의 Delete 메소드 호출

                If IsArray(vntData) Then
                    intColCnt = UBound(vntData, 1) : intRows = UBound(vntData, 2)
                    '''해당하는Row 만큼 Loop
                    For i = 1 To intRows
                        If GetElement(vntData, "CHK", intColCnt, i, OPTIONAL_STR) = 1 Then
                            strTRANSYEARMON = GetElement(vntData, "TRANSYEARMON", intColCnt, i, OPTIONAL_STR)
                            strTRANSNO = GetElement(vntData, "TRANSNO", intColCnt, i, OPTIONAL_STR)

                            intRtn = mobjceMD_PRINTTRANS_HDR.DeleteDo(strTRANSYEARMON, strTRANSNO)
                            intRtn = mobjceMD_BOOKING_MEDIUM.TRANSDeleteUpdateDo(strTRANSYEARMON, strTRANSNO)
                            intRtn = mobjceMD_PRINTTRANS_DTL.DeleteDo(strTRANSYEARMON, strTRANSNO)
                        End If
                    Next
                End If

                ' 트랜잭션 Commit
                .mobjSCGLSql.SQLCommitTrans()
                Return intRtn
            Catch err As Exception
                '트랜잭션 RollBack 및 오류 전송
                .mobjSCGLSql.SQLRollbackTrans()
                Throw RaiseSysErr(err, CLASS_NAME & "DeleteRtn")
            Finally
                'DB접속 종료
                .mobjSCGLSql.SQLDisconnect()
                '사용한 Entity(개체Dispose)
                mobjceMD_BOOKING_MEDIUM.Dispose()
                mobjceMD_PRINTTRANS_DTL.Dispose()
                mobjceMD_PRINTTRANS_HDR.Dispose()
            End Try
        End With
    End Function

