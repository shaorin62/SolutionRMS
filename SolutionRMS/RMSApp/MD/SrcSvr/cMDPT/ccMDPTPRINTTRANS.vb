    ' =============== SelectRtn_HDR 거래명세헤더조회
    Public Function SelectRtn_HDR(ByVal strInfoXML As String, _
                                  ByRef intRowCnt As Integer, _
                                  ByRef intColCnt As Integer, _
                                  ByVal strYEARMON As String, _
                                  ByVal strCLIENTCODE As String, _
                                  ByVal strTIMCODE As String, _
                                  ByVal strMED_FLAG As String) As Object

        Dim strSQL As String            'SQL문
        Dim strFormat As String         '임시 SQL문
        Dim strWhere As String
        Dim vntData As Object
        Dim Con1, Con2, Con3, Con4 As String

        SetConfig(strInfoXML)   '기본정보 설정

        Con1 = ""
        Con2 = ""
        Con3 = ""
        Con4 = ""

        If strYEARMON <> "" Then Con1 = String.Format(" AND (TRANSYEARMON = '{0}')", strYEARMON)
        If strCLIENTCODE <> "" Then Con2 = String.Format(" AND (CLIENTCODE = '{0}')", strCLIENTCODE)
        If strTIMCODE <> "" Then Con3 = String.Format(" AND (TIMCODE = '{0}')", strTIMCODE)
        If strMED_FLAG <> "" Then Con4 = String.Format(" AND (MED_FLAG = '{0}')", strMED_FLAG)

        strWhere = BuildFields(" ", Con1, Con2, Con3, Con4)

        strFormat = " SELECT"
        strFormat = strFormat & " 0 CHK, "
        strFormat = strFormat & " B.CONFIRMFLAG CONFIRMGBN, "
        strFormat = strFormat & " CASE DBO.MD_TAXTRU_YN_FUN(A.TRANSYEARMON,A.TRANSNO, 'B') "
        strFormat = strFormat & " WHEN 'Y' THEN 'O'"
        strFormat = strFormat & " ELSE '' END AS CONFIRMFLAG,"
        strFormat = strFormat & " DBO.SC_GET_HIGHCUSTNAME_FUN(H.CLIENTCODE) CLIENTNAME,"
        strFormat = strFormat & " CASE "
        strFormat = strFormat & " WHEN H.MED_FLAG = 'MP01' THEN '신문'"
        strFormat = strFormat & " WHEN H.MED_FLAG = 'MP02' THEN '잡지' END AS MED_FLAGNAME,"
        strFormat = strFormat & " ISNULL(H.AMT,0) AMT, "
        strFormat = strFormat & " ISNULL(H.VAT,0) VAT, "
        strFormat = strFormat & " ISNULL(H.AMT,0)+ISNULL(H.VAT,0) SUMAMTVAT, "
        strFormat = strFormat & " H.DEMANDDAY, "
        strFormat = strFormat & " H.PRINTDAY, "
        strFormat = strFormat & " A.TRANSYEARMON, "
        strFormat = strFormat & " A.TRANSNO, "
        strFormat = strFormat & " DBO.SC_EMPNAME_FUN(H.CONFIRM_USER) CONFIRM_USER,"
        strFormat = strFormat & " H.MEMO, A.CNT, H.MED_FLAG"
        strFormat = strFormat & " FROM"
        strFormat = strFormat & " ("
        strFormat = strFormat & " 	SELECT TRANSYEARMON, TRANSNO, COUNT(*) CNT "
        strFormat = strFormat & " 	FROM MD_PRINTTRANS_DTL"
        strFormat = strFormat & " 	WHERE 1=1  {0} "
        strFormat = strFormat & " 	GROUP BY TRANSYEARMON, TRANSNO"
        strFormat = strFormat & " ) A"
        strFormat = strFormat & " INNER JOIN MD_PRINTTRANS_HDR H"
        strFormat = strFormat & " ON A.TRANSYEARMON = H.TRANSYEARMON"
        strFormat = strFormat & " AND A. TRANSNO = H.TRANSNO"
        strFormat = strFormat & " INNER JOIN "
        strFormat = strFormat & " ("
        strFormat = strFormat & " 	SELECT TRANSYEARMON,TRANSNO,CONFIRMFLAG "
        strFormat = strFormat & " 	FROM ("
        strFormat = strFormat & " 		SELECT TRANSYEARMON,TRANSNO,"
        strFormat = strFormat & " 		CASE SUM(ISNULL(CONFIRMFLAG,0)) "
        strFormat = strFormat & " 		WHEN 0 THEN '' "
        strFormat = strFormat & " 		ELSE 'O' END CONFIRMFLAG "
        strFormat = strFormat & " 		FROM MD_PRINTTRANS_DTL "
        strFormat = strFormat & " 		WHERE 1=1  {0} "
        strFormat = strFormat & " 		GROUP BY TRANSYEARMON,TRANSNO"
        strFormat = strFormat & " 	) DATA"
        strFormat = strFormat & " ) B"
        strFormat = strFormat & " ON A.TRANSYEARMON = B.TRANSYEARMON"
        strFormat = strFormat & " AND A.TRANSNO = B.TRANSNO"

        strSQL = String.Format(strFormat, strWhere)
        '기본정보 Setting
        With mobjSCGLConfig '기본정보 Config 개체
            Try
                ' DB 접속
                .mobjSCGLSql.SQLConnect(.DBConnStr)
                vntData = .mobjSCGLSql.SQLSelectArr(strSQL, intRowCnt, intColCnt, , True)

                Return vntData
            Catch err As Exception
                ' 오류 전송
                Throw RaiseSysErr(err, CLASS_NAME & ".SelectRtn_HDR")
            Finally
                ' DB 접속 종료
                .mobjSCGLSql.SQLDisconnect()
            End Try
        End With
    End Function

