    ' ============== 승인처리 관련 조회
    Public Function SelectRtn_TransList(ByVal strInfoXML As String, _
                                        ByRef intRowCnt As Integer, _
                                        ByRef intColCnt As Integer) As Object

        Dim vntData As Object
        Dim strSQL As String

        SetConfig(strInfoXML)

        With mobjSCGLConfig '기본정보 Config 개체

            strSQL = " SELECT  "
            strSQL = strSQL & " 0 CHK,  "
            strSQL = strSQL & " B.CONFIRMFLAG CONFIRMGBN,  "
            strSQL = strSQL & " CASE DBO.MD_TAX_YN2_FUN(A.TRANSYEARMON,A.TRANSNO,'B') "
            strSQL = strSQL & " WHEN 'Y' THEN 'O'"
            strSQL = strSQL & " ELSE '' END AS CONFIRMFLAG,"
            strSQL = strSQL & " DBO.SC_GET_HIGHCUSTNAME_FUN(A.CLIENTCODE) CLIENTNAME,  "
            strSQL = strSQL & " CASE A.MED_FLAG WHEN 'MP01' THEN '신문'  "
            strSQL = strSQL & " WHEN 'MP02' THEN '잡지' END AS MED_FLAGNAME,  "
            strSQL = strSQL & " ISNULL(A.AMT,0) AMT,  "
            strSQL = strSQL & " ISNULL(A.VAT,0) VAT,  "
            strSQL = strSQL & " ISNULL(A.AMT,0)+ISNULL(A.VAT,0) SUMAMTVAT,  "
            strSQL = strSQL & " A.DEMANDDAY DEMANDDAY,  "
            strSQL = strSQL & " A.PRINTDAY PRINTDAY,  "
            strSQL = strSQL & " A.TRANSYEARMON,"
            strSQL = strSQL & " A.TRANSNO,"
            strSQL = strSQL & " A.MEMO MEMO "
            strSQL = strSQL & " FROM MD_PRINTTRANS_HDR A, "
            strSQL = strSQL & " ("
            strSQL = strSQL & " 	SELECT TRANSYEARMON,TRANSNO,CONFIRMFLAG "
            strSQL = strSQL & " 	FROM ("
            strSQL = strSQL & " 		SELECT TRANSYEARMON,TRANSNO,"
            strSQL = strSQL & " 		CASE SUM(ISNULL(CONFIRMFLAG,0)) WHEN 0 THEN '' ELSE 'O' END CONFIRMFLAG "
            strSQL = strSQL & " 		FROM MD_PRINTTRANS_DTL "
            strSQL = strSQL & " 		WHERE 1=1   "
            strSQL = strSQL & " 		GROUP BY TRANSYEARMON,TRANSNO"
            strSQL = strSQL & " 	) DATA "
            strSQL = strSQL & " ) B "
            strSQL = strSQL & " WHERE A.TRANSYEARMON = B.TRANSYEARMON  "
            strSQL = strSQL & " AND A.TRANSNO = B.TRANSNO AND CONFIRMFLAG = '' "

            Try
                ' DB 접속
                .mobjSCGLSql.SQLConnect(.DBConnStr)
                ' ------ Array 데이터 조회 (True 일때 헤더정보 포함 조회(Sheet Data Binding 할 경우 사용), False 일때 데이터만 조회)
                vntData = .mobjSCGLSql.SQLSelectArr(strSQL, intRowCnt, intColCnt, , True)
                Return vntData
            Catch err As Exception
                ' 오류 전송
                '.mobjSCGLSql.SQLRollbackTrans()
                Throw RaiseSysErr(err, CLASS_NAME & ".SelectRtn_TransList")
            Finally
                ' DB 접속 종료
                .mobjSCGLSql.SQLDisconnect()
            End Try
        End With

    End Function

