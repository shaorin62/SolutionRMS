    '==============MD_MATTER_MST 테이블의 신규 MATTERCODE 가져오기
    Public Function Get_NewSEQ(ByVal strYEARMON As String) As String
        Dim strSQL, strFormat, strRtn As String

        With mobjSCGLConfig '기본정보 Config 개체
            Try
                strSQL = "select ISNULL(Max(SEQ),0) +1 From MD_PROJECTION_DTL WHERE YEARMON = '" & strYEARMON & "'"
                strRtn = .mobjSCGLSql.SQLSelectOneScalar(strSQL)
                Return strRtn
            Catch err As Exception
                ' 오류 전송
                Throw RaiseSysErr(err, CLASS_NAME & ".Get_NewMatterCode")
            End Try
        End With
    End Function

    ' =============== ProcessRtn    거래처 디테일 저장
    Public Function ProcessRtn(ByVal strInfoXML As String, _
                               ByVal vntData As Object, _
                               ByVal vntData2 As Object, _
                               ByVal strCLIENTCODE As String) As Object

        Dim intRtn As Integer
        Dim i, intColCnt, intRows As Integer
        Dim intColCnt2, intRows2 As Integer
        Dim strYEARMON
        Dim strSEQ
        Dim strSQL, strSQL1
        Dim strEXCLIENTCODE
        Dim strAOR_RATE

        SetConfig(strInfoXML)
        With mobjSCGLConfig
            Try
                .mobjSCGLSql.SQLConnect(.DBConnStr)
                .mobjSCGLSql.SQLBeginTrans()

                '''사용할 Entity 개체생성(Config 정보를 넘겨생성)
                mobjceMD_PROJECTION_DTL = New ceMD_PROJECTION_DTL(mobjSCGLConfig)
                mobjceMD_PROJECTION_AOR = New ceMD_PROJECTION_AOR(mobjSCGLConfig)

                If IsArray(vntData) Then
                    '''vntData의 컬럼수, 로우수를 변수입력
                    intColCnt = UBound(vntData, 1) : intRows = UBound(vntData, 2)

                    For i = 1 To intRows
                        strYEARMON = ""
                        strYEARMON = GetElement(vntData, "YEARMON", intColCnt, i, OPTIONAL_STR)

                        strSQL = ""
                        strSQL = " DELETE FROM  MD_PROJECTION_DTL "
                        strSQL = strSQL & "  WHERE YEARMON = '" & strYEARMON & "' AND CLIENTCODE = '" & strCLIENTCODE & "'"

                        .mobjSCGLSql.SQLDo(strSQL)
                    Next

                    For i = 1 To intRows
                        strYEARMON = ""
                        strYEARMON = GetElement(vntData, "YEARMON", intColCnt, i, OPTIONAL_STR)

                        strSEQ = ""
                        strSEQ = Get_NewSEQ(strYEARMON)
                        intRtn = InsertRtn_MD_PROJECTION_DTL(vntData, intColCnt, i, strYEARMON, strSEQ, strCLIENTCODE)
                    Next
                End If

                If IsArray(vntData2) Then
                    '''vntData2의 컬럼수, 로우수를 변수입력
                    intColCnt2 = UBound(vntData2, 1) : intRows2 = UBound(vntData2, 2)

                    For i = 1 To intRows2
                        If GetElement(vntData2, "SAVEFLAG", intColCnt2, i) = "Y" Then
                            strYEARMON = ""
                            strYEARMON = GetElement(vntData2, "GBN", intColCnt2, i, OPTIONAL_STR)
                            strSQL1 = ""
                            strSQL1 = " DELETE FROM  MD_PROJECTION_AOR "
                            strSQL1 = strSQL1 & "  WHERE YEARMON = '" & strYEARMON & "' AND CLIENTCODE = '" & strCLIENTCODE & "'"

                            .mobjSCGLSql.SQLDo(strSQL1)
                        End If
                    Next

                    For i = 1 To intRows2
                        If GetElement(vntData2, "SAVEFLAG", intColCnt2, i) = "Y" Then
                            strYEARMON = ""
                            strEXCLIENTCODE = ""
                            strAOR_RATE = ""

                            strYEARMON = GetElement(vntData2, "GBN", intColCnt2, i, OPTIONAL_STR)
                            strEXCLIENTCODE = GetElement(vntData2, "EXCLIENTCODE", intColCnt2, i, OPTIONAL_STR)
                            strAOR_RATE = GetElement(vntData2, "AMT", intColCnt2, i, NULL_NUM, True)

                            intRtn = InsertRtn_MD_PROJECTION_AOR(vntData2, intColCnt2, i, strYEARMON, strEXCLIENTCODE, strAOR_RATE, strCLIENTCODE)
                        End If
                    Next

                End If
                .mobjSCGLSql.SQLCommitTrans()
                Return intRows
            Catch err As Exception
                .mobjSCGLSql.SQLRollbackTrans()
                Throw RaiseSysErr(err, CLASS_NAME & ".ProcessRtn_CUSTDTL")
            Finally
                .mobjSCGLSql.SQLDisconnect()
                mobjceMD_PROJECTION_DTL.Dispose()
                mobjceMD_PROJECTION_AOR.Dispose()
            End Try
        End With
    End Function
