                strFormet = strFormet & "  PUB_DATE,"
                strFormet = strFormet & "  COL_DEG,"
                strFormet = strFormet & "  EXECUTE_FACE,"
                strFormet = strFormet & "  CASE MED_FLAG WHEN 'MP01' THEN CAST(STD_STEP AS VARCHAR(10))+ '/' + CAST(STD_CM AS VARCHAR(10)) + '/' + CAST(STD_FACE AS VARCHAR(10))"
