                strFormet = strFormet & "  '' BTN,"
                strFormet = strFormet & "  DBO.SC_GET_HIGHCUSTNAME_FUN(EXCLIENTCODE) EXCLIENTNAME ,"
                strFormet = strFormet & "  ISNULL(EXSUSURATE,0) EXSUSURATE,"
                strFormet = strFormet & "  ISNULL(EXSUSU,0) EXSUSU,ISNULL(COMMISSION,0)-ISNULL(EXSUSU,0) MCSUSU,"
