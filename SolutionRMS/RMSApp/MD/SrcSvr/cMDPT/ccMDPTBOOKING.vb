    ' =============== 인쇄 개별청약 조회
    Public Function SelectRtn(ByVal strInfoXML As String, ByRef intRowCnt As Integer, ByRef intColCnt As Integer, _
                              ByVal strYEARMON As String, _
                              ByVal strCLIENTCODE As String, ByVal strCLIENTNAME As String, _
                              ByVal strREAL_MED_CODE As String, ByVal strREAL_MED_NAME As String, _
                              ByVal strTIMCODE As String, ByVal strTIMNAME As String, _
                              ByVal strMEDCODE As String, ByVal strMEDNAME As String, _
                              ByVal strSUBSEQ As String, ByVal strSUBSEQNAME As String, _
                              ByVal strMEDFLAG As String, ByVal strGFLAG As String, ByVal strVOCH_TYPE As String) As Object

        Dim strSQL As String
        Dim strFormet, strWhere As String
        Dim strChkDate As String = ""
        Dim Con1, Con2, Con3, Con4, Con5, Con6, Con7, Con8, Con9, Con10, Con11, Con12, Con13, Con14 As String
        Dim vntData As Object
        Dim vntData2 As Object

        With mobjSCGLConfig
            Try
                SetConfig(strInfoXML)   '기본정보 설정

                Con1 = "" : Con2 = "" : Con3 = "" : Con4 = "" : Con5 = "" : Con6 = ""
                Con7 = "" : Con8 = "" : Con9 = "" : Con10 = "" : Con11 = "" : Con12 = "" : Con13 = "" : Con14 = ""

                If strYEARMON <> "" Then Con1 = String.Format(" AND (YEARMON = '{0}')", strYEARMON) '년월
                If strCLIENTCODE <> "" Then Con2 = String.Format(" AND (CLIENTCODE = '{0}')", strCLIENTCODE) '광고주코드
                If strCLIENTNAME <> "" Then Con3 = String.Format(" AND (DBO.SC_GET_HIGHCUSTNAME_FUN(CLIENTCODE) LIKE '%{0}%')", strCLIENTNAME) '광고주명
                If strREAL_MED_CODE <> "" Then Con4 = String.Format(" AND (REAL_MED_CODE = '{0}')", strREAL_MED_CODE) '매체사코드
                If strREAL_MED_NAME <> "" Then Con5 = String.Format(" AND (DBO.SC_GET_HIGHCUSTNAME_FUN(REAL_MED_CODE) LIKE '%{0}%')", strREAL_MED_NAME) '매체사명
                If strTIMCODE <> "" Then Con6 = String.Format(" AND (TIMCODE = '{0}')", strTIMCODE) '팀코드
                If strTIMNAME <> "" Then Con7 = String.Format(" AND (DBO.SC_GET_CUSTNAME_FUN(TIMCODE) LIKE '%{0}%')", strTIMNAME) '팀명
                If strMEDCODE <> "" Then Con8 = String.Format(" AND (MEDCODE = '{0}')", strMEDCODE) '매체코드
                If strMEDNAME <> "" Then Con9 = String.Format(" AND (DBO.SC_GET_CUSTNAME_FUN(MEDCODE) LIKE '%{0}%')", strMEDNAME) '매체명
                If strSUBSEQ <> "" Then Con10 = String.Format(" AND (SUBSEQ = '{0}')", strSUBSEQ) '브랜드코드
                If strSUBSEQNAME <> "" Then Con11 = String.Format(" AND (DBO.SC_GET_SUBSEQNAME_FUN(SUBSEQ) LIKE '%{0}%')", strSUBSEQNAME) '브랜드명
                If strMEDFLAG <> "" Then Con12 = String.Format(" AND (MED_FLAG = '{0}')", strMEDFLAG) '매체구분
                If strGFLAG <> "" Then Con13 = String.Format(" AND (GFLAG = '{0}')", strGFLAG) '집행구분
                If strVOCH_TYPE <> "" Then
                    If strVOCH_TYPE = "PROJECTION" Then
                        Con14 = " AND (PROJECTION = 'Y')" '돌출
                    Else
                        Con14 = String.Format(" AND (VOCH_TYPE = '{0}')", strVOCH_TYPE) '전표구분
                    End If
                End If

                strWhere = BuildFields(" ", Con1, Con2, Con3, Con4, Con5, Con6, Con7, Con8, Con9, Con10, Con11, Con12, Con13, Con14)

                strFormet = "  SELECT"
                strFormet = strFormet & "  CHK, GFLAGNAME, YEARMON, SEQ, "
                strFormet = strFormet & "  MED_FLAG, DIVMEDIA, "
                strFormet = strFormet & "  PUB_DATE, DEMANDDAY, "
                strFormet = strFormet & "  CLIENTCODE, CLIENTNAME, MEDCODE, MEDNAME,"
                strFormet = strFormet & "  REAL_MED_CODE, REAL_MED_NAME, REAL_MED_BUSINO, "
                strFormet = strFormet & "  SUBSEQ, SUBSEQNAME, TIMCODE, TIMNAME, MATTERCODE, MATTERNAME,MATTERUSER, "
                strFormet = strFormet & "  DEPT_CD, DEPT_NAME, MED_FLAG, MED_FLAG_NAME, PUB_FACE, "
                strFormet = strFormet & "  EXECUTE_FACE,"
                strFormet = strFormet & "  STD_STEP, STD_CM, STD_FACE, "
                strFormet = strFormet & "  STD, STD_PAGE, COL_DEG, "
                strFormet = strFormet & "  PROJECTION, "
                strFormet = strFormet & "  PRICE, AMT, COMMI_RATE, COMMISSION, "
                strFormet = strFormet & "  VOCH_TYPE, RECEIPT_GUBUN, "
                strFormet = strFormet & "  TRU_TAX_FLAG, DUTYFLAG, "
                strFormet = strFormet & "  MEMO, TRU_TRANS_NO , COMMI_TRANS_NO, GFLAG,  "
                strFormet = strFormet & "  EXCLIENTCODE, EXCLIENTNAME,"
                strFormet = strFormet & "  REAL_MED_BUSINO AS REAL_MED_BUSINO1,"
                strFormet = strFormet & "  REAL_MED_NAME AS REAL_MED_NAME1,"
                strFormet = strFormet & "  MEDNAME AS MEDNAME1, "
                strFormet = strFormet & "  MATTERNAME AS MATTERNAME1, "
                strFormet = strFormet & "  TIMNAME AS TIMNAME1, "
                strFormet = strFormet & "  SUBSEQNAME AS SUBSEQNAME1, "
                strFormet = strFormet & "  DEPT_NAME AS DEPT_NAME1,"
                strFormet = strFormet & "  EXCLIENTNAME AS EXCLIENTNAME1, "
                strFormet = strFormet & "  AMT AS AMT1,"
                strFormet = strFormet & "  COMMISSION AS COMMISSION1"
                strFormet = strFormet & "  "
                strFormet = strFormet & "  FROM ("
                strFormet = strFormet & "  	select "
                strFormet = strFormet & "  	0 CHK, "
                strFormet = strFormet & "  	CASE WHEN GFLAG = 'M' THEN '미정' WHEN GFLAG = 'B' THEN '배정' WHEN GFLAG = 'J' THEN '집행' WHEN GFLAG = 'S' THEN '실적'END GFLAGNAME,"
                strFormet = strFormet & "  	YEARMON, SEQ, "
                strFormet = strFormet & "  	MED_FLAG, DIVMEDIA, "
                strFormet = strFormet & "  	PUB_DATE, DEMANDDAY, "
                strFormet = strFormet & "  	CLIENTCODE, "
                strFormet = strFormet & "  	DBO.SC_GET_HIGHCUSTNAME_FUN(CLIENTCODE) CLIENTNAME,"
                strFormet = strFormet & "  	MEDCODE, "
                strFormet = strFormet & "  	DBO.SC_GET_CUSTNAME_FUN(MEDCODE) MEDNAME,"
                strFormet = strFormet & "  	REAL_MED_CODE,"
                strFormet = strFormet & "  	DBO.SC_GET_HIGHCUSTNAME_FUN(REAL_MED_CODE) REAL_MED_NAME, "
                strFormet = strFormet & "  	DBO.SC_GET_BUSINO_FUN(REAL_MED_CODE) REAL_MED_BUSINO, "
                strFormet = strFormet & "  	SUBSEQ,"
                strFormet = strFormet & "  	DBO.SC_GET_SUBSEQNAME_FUN(SUBSEQ) SUBSEQNAME, "
                strFormet = strFormet & "  	TIMCODE,"
                strFormet = strFormet & "  	DBO.SC_GET_CUSTNAME_FUN(TIMCODE) TIMNAME, "
                strFormet = strFormet & "  	MATTERCODE,"
                strFormet = strFormet & "  	DBO.MD_GET_MATTERNAME_FUN(MATTERCODE) MATTERNAME, "
                strFormet = strFormet & "  	DBO.MD_GET_MATTERUSER_FUN(MATTERCODE) MATTERUSER, "
                strFormet = strFormet & "  	DEPT_CD,"
                strFormet = strFormet & "  	DBO.SC_DEPT_NAME_FUN(DEPT_CD) DEPT_NAME,"
                strFormet = strFormet & "  	DBO.MD_GET_MEDNAME_FUN(MED_FLAG) MED_FLAG_NAME,"
                strFormet = strFormet & "  	PUB_FACE, "
                strFormet = strFormet & "  	EXECUTE_FACE,"
                strFormet = strFormet & "  	STD_STEP, STD_CM, STD_FACE, "
                strFormet = strFormet & "  	STD, STD_PAGE, COL_DEG, "
                strFormet = strFormet & "  	PROJECTION, "
                strFormet = strFormet & "  	PRICE, AMT, COMMI_RATE, COMMISSION, "
                strFormet = strFormet & "  	VOCH_TYPE, RECEIPT_GUBUN, "
                strFormet = strFormet & "  	TRU_TAX_FLAG, DUTYFLAG, "
                strFormet = strFormet & "  	MEMO, "
                strFormet = strFormet & "  	CASE WHEN ISNULL(TRU_TRANS_NO,'') <> '' THEN "
                strFormet = strFormet & "  	SUBSTRING(TRU_TRANS_NO, 0, DBO.INSTR(0, TRU_TRANS_NO,'-')) + '-' + "
                strFormet = strFormet & "  	dbo.lpad(SUBSTRING(TRU_TRANS_NO, DBO.INSTR(0, TRU_TRANS_NO,'-')+1, DBO.INSTR(0, SUBSTRING(TRU_TRANS_NO,8,LEN(TRU_TRANS_NO)),'-')-1), 4, 0) + '-' + "
                strFormet = strFormet & "  	dbo.lpad(SUBSTRING(TRU_TRANS_NO, DBO.INSTR(0, TRU_TRANS_NO,'-')+1 + DBO.INSTR(0, SUBSTRING(TRU_TRANS_NO,8,LEN(TRU_TRANS_NO)),'-'), LEN(TRU_TRANS_NO)),4,0) "
                strFormet = strFormet & "  	ELSE "
                strFormet = strFormet & "  	TRU_TRANS_NO "
                strFormet = strFormet & "  	END AS TRU_TRANS_NO , "
                strFormet = strFormet & "  	CASE WHEN ISNULL(COMMI_TRANS_NO,'') <> '' THEN "
                strFormet = strFormet & "  	SUBSTRING(COMMI_TRANS_NO, 0, DBO.INSTR(0, COMMI_TRANS_NO,'-')) + '-' + "
                strFormet = strFormet & "  	dbo.lpad(SUBSTRING(COMMI_TRANS_NO, DBO.INSTR(0, COMMI_TRANS_NO,'-')+1, DBO.INSTR(0, SUBSTRING(COMMI_TRANS_NO,8,LEN(COMMI_TRANS_NO)),'-')-1), 4, 0) + '-' + "
                strFormet = strFormet & "  	dbo.lpad(SUBSTRING(COMMI_TRANS_NO, DBO.INSTR(0, COMMI_TRANS_NO,'-')+1 + DBO.INSTR(0, SUBSTRING(COMMI_TRANS_NO,8,LEN(COMMI_TRANS_NO)),'-'), LEN(COMMI_TRANS_NO)),4,0) "
                strFormet = strFormet & "  	ELSE "
                strFormet = strFormet & "  	COMMI_TRANS_NO "
                strFormet = strFormet & "  	END AS COMMI_TRANS_NO, "
                strFormet = strFormet & "  	GFLAG,  "
                strFormet = strFormet & "  	EXCLIENTCODE, "
                strFormet = strFormet & "  	DBO.SC_GET_HIGHCUSTNAME_FUN(EXCLIENTCODE) EXCLIENTNAME "
                strFormet = strFormet & "  	from MD_BOOKING_MEDIUM "
                strFormet = strFormet & "  	where 1=1  "
                strFormet = strFormet & "  	{0} "
                strFormet = strFormet & "  ) A"
                strFormet = strFormet & "  ORDER BY CASE WHEN SUBSTRING(CLIENTNAME,1,3) = '(주)' THEN"
                strFormet = strFormet & "  SUBSTRING(CLIENTNAME,4,100)"
                strFormet = strFormet & "  ELSE"
                strFormet = strFormet & "  CLIENTNAME END, PUB_DATE"

                strSQL = String.Format(strFormet, strWhere)

                .mobjSCGLSql.SQLConnect(.DBConnStr)

                vntData = .mobjSCGLSql.SQLSelectArr(strSQL, intRowCnt, intColCnt, , True)

                Return vntData
            Catch err As Exception
                Throw RaiseSysErr(err, CLASS_NAME & ".SelectRtn")
            Finally
                .mobjSCGLSql.SQLDisconnect()
            End Try
        End With
    End Function

    Public Function DataValidation(ByVal strInfoXML As String, ByRef intRowCnt As Integer, ByRef intColCnt As Integer, _
                           ByVal strCLIENTCODE As String, _
                           ByVal strFLAG As String) As Object                                      'XML  데이터 조회시

        Dim strSQL As String
        Dim strFormet, strSelFields, strWhere As String
        Dim strChkDate As String = ""
        Dim vntData As Object

        With mobjSCGLConfig
            Try
                SetConfig(strInfoXML)   '기본정보 설정


                If strFLAG = "CUST" Then
                    If strCLIENTCODE <> "" Then strWhere = String.Format(" AND (CUSTCODE = '{0}')", strCLIENTCODE)
                    strFormet = "SELECT CUSTCODE FROM SC_CUST_TEMP WHERE 1=1 AND CUSTCODE LIKE 'A%' {0}"
                    strSQL = String.Format(strFormet, strWhere)
                ElseIf strFLAG = "MED" Then
                    If strCLIENTCODE <> "" Then strWhere = String.Format(" AND (CUSTCODE = '{0}')", strCLIENTCODE)
                    strFormet = "SELECT CUSTCODE FROM SC_CUST_TEMP WHERE 1=1 AND CUSTCODE LIKE 'B%' {0}"
                    strSQL = String.Format(strFormet, strWhere)
                ElseIf strFLAG = "REAL" Then
                    If strCLIENTCODE <> "" Then strWhere = String.Format(" AND (HIGHCUSTCODE = '{0}')", strCLIENTCODE)
                    strFormet = "SELECT CUSTCODE FROM SC_CUST_TEMP WHERE 1=1 AND HIGHCUSTCODE IS NOT NULL {0}"
                    strSQL = String.Format(strFormet, strWhere)
                End If

                .mobjSCGLSql.SQLConnect(.DBConnStr)
                vntData = .mobjSCGLSql.SQLSelectArr(strSQL, intRowCnt, intColCnt)

                Return vntData
            Catch err As Exception
                Throw RaiseSysErr(err, CLASS_NAME & ".SelectRtn")
            Finally
                .mobjSCGLSql.SQLDisconnect()
            End Try
        End With
    End Function

    ' =============== ProcessRtn    부킹 저장
    Public Function ProcessRtn(ByVal strInfoXML As String, _
                               ByVal vntData As Object) As Object

        Dim intRtn As Integer
        Dim i, intColCnt, intRows As Integer
        Dim strSEQ, strSEQ1
        Dim strYEARMON
        Dim strPUB_DATE, strDEMANDDAY
        Dim strPROJECTION
        Dim strTRU_TAX_FLAG

        SetConfig(strInfoXML)
        With mobjSCGLConfig
            Try
                .mobjSCGLSql.SQLConnect(.DBConnStr)
                .mobjSCGLSql.SQLBeginTrans()
                If IsArray(vntData) Then
                    '''사용할 Entity 개체생성(Config 정보를 넘겨생성)
                    mobjceMD_BOOKING_MEDIUM = New ceMD_BOOKING_MEDIUM(mobjSCGLConfig)
                    '''vntData의 컬럼수, 로우수를 변수입력
                    intColCnt = UBound(vntData, 1) : intRows = UBound(vntData, 2)
                    For i = 1 To intRows
                        strSEQ = ""
                        strPROJECTION = ""
                        strYEARMON = GetElement(vntData, "YEARMON", intColCnt, i, OPTIONAL_STR)
                        If GetElement(vntData, "PUB_DATE", intColCnt, i, OPTIONAL_STR) <> "" Then strPUB_DATE = GetElement(vntData, "PUB_DATE", intColCnt, i, OPTIONAL_STR).Substring(0, 4) & GetElement(vntData, "PUB_DATE", intColCnt, i, OPTIONAL_STR).Substring(5, 2) & GetElement(vntData, "PUB_DATE", intColCnt, i, OPTIONAL_STR).Substring(8, 2)
                        If GetElement(vntData, "DEMANDDAY", intColCnt, i, OPTIONAL_STR) <> "" Then strDEMANDDAY = GetElement(vntData, "DEMANDDAY", intColCnt, i, OPTIONAL_STR).Substring(0, 4) & GetElement(vntData, "DEMANDDAY", intColCnt, i, OPTIONAL_STR).Substring(5, 2) & GetElement(vntData, "DEMANDDAY", intColCnt, i, OPTIONAL_STR).Substring(8, 2)
                        If GetElement(vntData, "PROJECTION", intColCnt, i, OPTIONAL_STR) = "1" Then
                            strPROJECTION = "Y"
                        Else
                            strPROJECTION = "N"
                        End If

                        If GetElement(vntData, "TRU_TAX_FLAG", intColCnt, i, OPTIONAL_STR) = "1" Then
                            strTRU_TAX_FLAG = "Y"
                        Else
                            strTRU_TAX_FLAG = "N"
                        End If
                        If GetElement(vntData, "SEQ", intColCnt, i, NULL_NUM, True) = -999999 Then
                            strSEQ = SelectRtn_SEQ(strYEARMON)
                            intRtn = InsertRtn_MD_BOOKING_MEDIUM(vntData, intColCnt, i, strYEARMON, strSEQ, strPUB_DATE, strDEMANDDAY, strPROJECTION, strTRU_TAX_FLAG)
                        Else
                            strSEQ = GetElement(vntData, "SEQ", intColCnt, i, OPTIONAL_STR)
                            intRtn = UpdateRtn_MD_BOOKING_MEDIUM(vntData, intColCnt, i, strYEARMON, strSEQ, strPUB_DATE, strDEMANDDAY, strPROJECTION, strTRU_TAX_FLAG)
                        End If
                    Next
                End If
                .mobjSCGLSql.SQLCommitTrans()
                Return intRows
            Catch err As Exception
                .mobjSCGLSql.SQLRollbackTrans()
                Throw RaiseSysErr(err, CLASS_NAME & ".ProcessRtn")
            Finally
                .mobjSCGLSql.SQLDisconnect()
                mobjceMD_BOOKING_MEDIUM.Dispose()
            End Try
        End With
    End Function

