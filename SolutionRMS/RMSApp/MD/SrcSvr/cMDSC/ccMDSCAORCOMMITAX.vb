    '============== ProcessRtn_Group 합산생성
    Public Function ProcessRtn_Group(ByVal strInfoXML As String, _
                                     ByVal vntData As Object, _
                                     ByVal intTAXNO As Integer, _
                                     ByVal intColFlag As Integer) As Integer

        Dim intRtn, intRtn1, intRtn2, intRtn3 As Integer '결과값 변수
        Dim i, intColCnt, intRows As Integer '루프, 컬럼Cnt, 로우Cnt 변수
        Dim intTAXNOSEQ As Integer
        Dim strDEMANDDAY As String
        Dim strPRINTDAY As String
        Dim strTAXYEARMON As String
        Dim strSUMM As String
        Dim strTRANSYEARMON As String
        Dim lngTRANSNO As Double
        Dim lngSEQ As Double
        Dim strTAXNO As String
        Dim strTRANSNO As String
        Dim intCnt As Integer
        Dim intCnt2 As Integer
        Dim strMEDCODE
        Dim strCNT
        Dim strMEDCODE2

        '합계 변수
        Dim lngCOMMISSION As Double
        Dim lngSUMCOMMISSION As Double
        Dim lngVAT As Double
        Dim lngSUMVAT As Double

        '헤더 변수
        Dim strCLIENTCODE, strCLIENTACCCODE, strREAL_MED_CODE, strREAL_MED_ACCODE As String
        Dim lngTOTALSUMAMT, lngTOTALAMT As Double
        '디테일변수
        Dim lngCOMMI_RATE As String

        SetConfig(strInfoXML) '기본정보 Setting
        With mobjSCGLConfig '기본정보를 가지고 있는 Config 개체
            Try
                Dim xmlRoot As XmlElement

                .mobjSCGLSql.SQLConnect(.DBConnStr)
                .mobjSCGLSql.SQLBeginTrans()

                mobjceMD_COMMITAX_HDR = New ceMD_COMMITAX_HDR(mobjSCGLConfig)
                mobjceMD_COMMITAX_DTL = New ceMD_COMMITAX_DTL(mobjSCGLConfig)
                mobjceMD_AOR_MEDIUM = New eMDCO.ceMD_AOR_MEDIUM(mobjSCGLConfig)
                mobjceMD_AORCOMMI_DTL = New eMDCO.ceMD_AORCOMMI_DTL(mobjSCGLConfig)

                intColCnt = UBound(vntData, 1) : intRows = UBound(vntData, 2)
                For intCnt = 1 To intColFlag
                    intCnt2 = 0
                    strCNT = 0
                    If IsArray(vntData) Then
                        intColCnt = UBound(vntData, 1) : intRows = UBound(vntData, 2)
                        '''해당하는Row 만큼 Loop
                        lngCOMMISSION = 0
                        lngSUMCOMMISSION = 0
                        lngVAT = 0
                        lngSUMVAT = 0
                        lngTOTALAMT = 0
                        lngTOTALSUMAMT = 0
                        strMEDCODE2 = ""
                        strSUMM = ""
                        '쉬트 내 헤더값 검출
                        For i = 1 To intRows
                            '청구합계
                            If GetElement(vntData, "CHK", intColCnt, i, OPTIONAL_STR) = 1 And GetElement(vntData, "RANKTRANS", intColCnt, i, OPTIONAL_STR) = intCnt Then
                                lngCOMMISSION = GetElement(vntData, "COMMISSION", intColCnt, i, OPTIONAL_STR)
                                lngTOTALAMT = GetElement(vntData, "AMT", intColCnt, i, OPTIONAL_STR)
                                lngSUMCOMMISSION = lngSUMCOMMISSION + lngCOMMISSION
                                lngTOTALSUMAMT = lngTOTALSUMAMT + lngTOTALAMT

                                '계산서년월
                                strTAXYEARMON = GetElement(vntData, "TAXYEARMON", intColCnt, i, OPTIONAL_STR)
                                '계산서번호
                                intTAXNO = SelectRtn_COMMITAXNO(strTAXYEARMON)
                                '헤더정보
                                strDEMANDDAY = GetElement(vntData, "DEMANDDAY", intColCnt, i, OPTIONAL_STR).Substring(0, 4) & GetElement(vntData, "DEMANDDAY", intColCnt, i, OPTIONAL_STR).Substring(5, 2) & GetElement(vntData, "DEMANDDAY", intColCnt, i, OPTIONAL_STR).Substring(8, 2)
                                strPRINTDAY = GetElement(vntData, "PRINTDAY", intColCnt, i, OPTIONAL_STR).Substring(0, 4) & GetElement(vntData, "PRINTDAY", intColCnt, i, OPTIONAL_STR).Substring(5, 2) & GetElement(vntData, "PRINTDAY", intColCnt, i, OPTIONAL_STR).Substring(8, 2)

                                strMEDCODE = ""
                                strMEDCODE = GetElement(vntData, "MEDCODE", intColCnt, i, OPTIONAL_STR)
                                If GetElement(vntData, "MEDCODE", intColCnt, i, OPTIONAL_STR) <> strMEDCODE2 Then
                                    strCNT = strCNT + 1
                                    strMEDCODE2 = strMEDCODE
                                End If

                                If strCNT <> 1 Then
                                    strSUMM = GetElement(vntData, "SUMM", intColCnt, i, OPTIONAL_STR)
                                Else
                                    strSUMM = GetElement(vntData, "SUMM", intColCnt, i, OPTIONAL_STR) & " (" & GetElement(vntData, "MEDNAME", intColCnt, i, OPTIONAL_STR) & ")"
                                End If
                            End If
                        Next

                        lngSUMVAT = Int(lngSUMCOMMISSION / 10)
                        If lngTOTALSUMAMT <> "0.0" Or lngTOTALSUMAMT <> "0" Then
                            lngCOMMI_RATE = gRound(((lngSUMCOMMISSION / lngTOTALSUMAMT) * 100), 2)
                        Else
                            lngCOMMI_RATE = 0
                        End If

                        '검출된 헤더값 실제저장
                        For i = 1 To intRows
                            If GetElement(vntData, "CHK", intColCnt, i, OPTIONAL_STR) = 1 And GetElement(vntData, "RANKTRANS", intColCnt, i, OPTIONAL_STR) = intCnt Then
                                If intCnt2 = intCnt Then
                                Else
                                    '헤더저장
                                    intRtn = InsertRtn_COMMITAX_HDR(vntData, _
                                                                  intColCnt, _
                                                                  i, _
                                                                  strTAXYEARMON, _
                                                                  intTAXNO, _
                                                                  lngSUMCOMMISSION, _
                                                                  lngSUMVAT, _
                                                                  strDEMANDDAY, _
                                                                  strPRINTDAY, _
                                                                  strSUMM, _
                                                                  lngCOMMI_RATE, _
                                                                  lngTOTALSUMAMT)

                                    '헤더 저장과 동시에 통합 청구로 보내기 위함. AOR 전표는 통합 전표로 생성됨.
                                    intRtn = mobjceMD_COMMITAX_HDR.UpdateRtn_COMMITAX_HDR_AORMERGE(strTAXYEARMON, intTAXNO, "AOR")

                                End If
                                '무한업데이트.
                                intCnt2 = intCnt
                                '디테일저장
                                If IsArray(vntData) Then
                                    '거래명세서 번호는 위에서 참조
                                    strTRANSYEARMON = GetElement(vntData, "TRANSYEARMON", intColCnt, i, OPTIONAL_STR)
                                    lngTRANSNO = GetElement(vntData, "TRANSNO", intColCnt, i, OPTIONAL_STR)
                                    lngSEQ = GetElement(vntData, "SEQ", intColCnt, i, OPTIONAL_STR)
                                    strTRANSNO = strTRANSYEARMON & "-" & lngTRANSNO & "-" & lngSEQ
                                    strDEMANDDAY = GetElement(vntData, "DEMANDDAY", intColCnt, i, OPTIONAL_STR).Substring(0, 4) & GetElement(vntData, "DEMANDDAY", intColCnt, i, OPTIONAL_STR).Substring(5, 2) & GetElement(vntData, "DEMANDDAY", intColCnt, i, OPTIONAL_STR).Substring(8, 2)
                                    strPRINTDAY = GetElement(vntData, "PRINTDAY", intColCnt, i, OPTIONAL_STR).Substring(0, 4) & GetElement(vntData, "PRINTDAY", intColCnt, i, OPTIONAL_STR).Substring(5, 2) & GetElement(vntData, "PRINTDAY", intColCnt, i, OPTIONAL_STR).Substring(8, 2)

                                    strTAXNO = strTAXYEARMON & "-" & intTAXNO
                                    intTAXNOSEQ = SelectRtn_COMMITAXNOSEQ(strTAXYEARMON, intTAXNO)

                                    '디테일저장
                                    intRtn1 = InsertRtn_COMMITAX_DTL(vntData, intColCnt, i, strTAXYEARMON, intTAXNO, intTAXNOSEQ, strTRANSNO)

                                    '거래명세서 디테일의 세금계산서번호 저장
                                    intRtn2 = mobjceMD_AORCOMMI_DTL.Update_CommiTax(strTRANSYEARMON, lngTRANSNO, lngSEQ, strTAXYEARMON, intTAXNO)

                                    '신탁의 세금계산서번호 저장
                                    intRtn3 = mobjceMD_AOR_MEDIUM.Update_CommiTax(strTRANSNO, strTAXNO)
                                End If
                            End If
                        Next
                    End If
                Next
                .mobjSCGLSql.SQLCommitTrans()
                Return intRtn
            Catch err As Exception
                .mobjSCGLSql.SQLRollbackTrans()
                Throw RaiseSysErr(err, CLASS_NAME & ".ProcessRtn_Group")
            Finally
                'Resource해제
                mobjceMD_COMMITAX_HDR.Dispose()
                mobjceMD_COMMITAX_DTL.Dispose()
                mobjceMD_AOR_MEDIUM.Dispose()
                mobjceMD_AORCOMMI_DTL.Dispose()
                .mobjSCGLSql.SQLDisconnect()
            End Try
        End With
    End Function

    '생성된 세금계산서 삭제
    Public Function DeleteRtn_CommiTax(ByVal strInfoXML As String, _
                                      ByVal strTAXYEARMON As String, _
                                      ByVal intTAXNO As Integer) As Integer
        Dim intRtn As Integer      'Return변수( 처리건수 또는 0 )
        Dim strTAXNO As String

        SetConfig(strInfoXML)    '기본정보 Setting
        With mobjSCGLConfig    '기본정보 Config 개체
            Try
                ' 사용할Entity 개체생성(Config 정보를 넘겨생성)
                strTAXNO = strTAXYEARMON & "-" & CStr(intTAXNO)
                mobjceMD_COMMITAX_HDR = New ceMD_COMMITAX_HDR(mobjSCGLConfig)
                mobjceMD_COMMITAX_DTL = New ceMD_COMMITAX_DTL(mobjSCGLConfig)
                mobjceMD_AOR_MEDIUM = New eMDCO.ceMD_AOR_MEDIUM(mobjSCGLConfig)
                mobjceMD_AORCOMMI_DTL = New eMDCO.ceMD_AORCOMMI_DTL(mobjSCGLConfig)
                ' DB 접속 및 트랜잭션 시작
                .mobjSCGLSql.SQLConnect(.DBConnStr)
                .mobjSCGLSql.SQLBeginTrans()
                ' 엔티티 오브젝트의 Delete 메소드 호출
                '거래명세서의 세금계산서년월,번호 삭제
                intRtn = mobjceMD_AORCOMMI_DTL.CommiTaxDeleteUpdateDo(strTAXYEARMON, intTAXNO)
                '신탁테이블의 세금계산서 번호 삭제
                intRtn = mobjceMD_AOR_MEDIUM.CommiTaxDeleteUpdateDo(strTAXNO)

                intRtn = mobjceMD_COMMITAX_HDR.DeleteMark(strTAXYEARMON, intTAXNO)

                intRtn = mobjceMD_COMMITAX_DTL.DeleteMark(strTAXYEARMON, intTAXNO)

                ' 트랜잭션 Commit
                .mobjSCGLSql.SQLCommitTrans()
                Return intRtn
            Catch err As Exception
                .mobjSCGLSql.SQLRollbackTrans()
                Throw RaiseSysErr(err, CLASS_NAME & "DeleteRtn_CommiTax")
            Finally
                'DB접속 종료
                .mobjSCGLSql.SQLDisconnect()
                '사용한 Entity(개체Dispose)
                mobjceMD_COMMITAX_HDR.Dispose()
                mobjceMD_COMMITAX_DTL.Dispose()
                mobjceMD_AOR_MEDIUM.Dispose()
                mobjceMD_AORCOMMI_DTL.Dispose()
            End Try
        End With

    End Function

    Public Function SelectRtn_COMMITAXNO(ByVal strTAXYEARMON As String) As String
        Dim strSQL As String
        Dim strFormat As String
        Dim strRtn As String

        With mobjSCGLConfig '기본정보 Config 개체
            Try
                strSQL = String.Format("select ISNULL(Max(TAXNO),0) +1 From MD_COMMITAX_HDR WHERE TAXYEARMON = '" & strTAXYEARMON & "'")
                strRtn = .mobjSCGLSql.SQLSelectOneScalar(strSQL)
                Return strRtn
            Catch err As Exception
                Throw RaiseSysErr(err, CLASS_NAME & ".SelectRtn_COMMITAXNO")
            Finally
            End Try
        End With
    End Function

