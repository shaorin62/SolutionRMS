            strFormat = strFormat & "   REAL_MED_BISNO, "
            strFormat = strFormat & "   VOCHNO, "
            strFormat = strFormat & "   dbo.SC_REAL_MED_EMPNAME_FUN(data2.taxyearmon, data2.taxno, data2.medflag) suppnm, "
            strFormat = strFormat & "   dbo.SC_REAL_MED_EMPEMAIL_FUN(data2.taxyearmon, data2.taxno, data2.medflag) suppemail "
            strFormat = strFormat & "   from "
            strFormat = strFormat & "   ("
            strFormat = strFormat & "       SELECT "
            strFormat = strFormat & "       0 CHK,"
            strFormat = strFormat & "       TAXYEARMON,"
            strFormat = strFormat & "       TAXNO,"
            strFormat = strFormat & "       TAXYEARMON + '-' + MJFLAG + '-' + dbo.LPAD(TAXNO,4,'0') TAXMANAGE,"
            strFormat = strFormat & "       REAL_MED_NAME,"
            strFormat = strFormat & "       REAL_AMT,"
            strFormat = strFormat & "       COMMI_RATE,"
            strFormat = strFormat & "       AMT,"
            strFormat = strFormat & "       VAT,"
            strFormat = strFormat & "       AMT + VAT SUMAMT,"
            strFormat = strFormat & "       DEMANDDAY,"
            strFormat = strFormat & "       SUMM,"
            strFormat = strFormat & "       PRINTDAY,"
            strFormat = strFormat & "       DBO.SC_DEPT_NAME_FUN(DEPT_CD) DEPT_NAME,"
            strFormat = strFormat & "       REAL_MED_BISNO, REAL_MEDOWNER, REAL_MEDADDR1, REAL_MEDADDR2,"
            strFormat = strFormat & "       VOCHNO, medflag"
            strFormat = strFormat & "       FROM MD_COMMITAX_HDR"
            strFormat = strFormat & "       WHERE 1 = 1 AND ISNULL(ATTR10,0) <> 999999 {0} "
            strFormat = strFormat & "       AND ISNULL(ATTR03,'') = 'AOR'"
            strFormat = strFormat & "   ) DATA2"
            strFormat = strFormat & "   LEFT JOIN "
            strFormat = strFormat & "   ("
            strFormat = strFormat & "       SELECT "
            strFormat = strFormat & "       A.TAXYEARMON, A.TAXNO, A.MAXAMT, A.MAXCNT, B.MEDCODE "
            strFormat = strFormat & "  	 FROM "
            strFormat = strFormat & "       ("
            strFormat = strFormat & "           SELECT  TAXYEARMON, "
            strFormat = strFormat & "   	            TAXNO, "
            strFormat = strFormat & "   	            MAX(AMT) MAXAMT, "
            strFormat = strFormat & "   	            COUNT(TAXNO) MAXCNT ,"
            strFormat = strFormat & "   	            MAX(TAXNOSEQ) TAXNOSEQ "
            strFormat = strFormat & "                FROM MD_COMMITAX_DTL"
            strFormat = strFormat & "                WHERE 1=1 AND ISNULL(ATTR10,0) <> 999999 " & Con1
            strFormat = strFormat & "                AND ISNULL(ATTR03,'') = 'AOR' "
            strFormat = strFormat & "                GROUP BY TAXYEARMON, TAXNO "
            strFormat = strFormat & "      ) A LEFT JOIN MD_COMMITAX_DTL B ON A.TAXYEARMON = B.TAXYEARMON"
            strFormat = strFormat & "      AND  A.TAXNO = B.TAXNO"
            strFormat = strFormat & "      AND A.TAXNOSEQ = B.TAXNOSEQ"
            strFormat = strFormat & "   ) DATA1 "
            strFormat = strFormat & "   ON DATA2.TAXYEARMON = DATA1.TAXYEARMON AND  DATA2.TAXNO = DATA1.TAXNO"

            strSQL = String.Format(strFormat, strWhere)
            '데이터 조회
            Try
                .mobjSCGLSql.SQLConnect(.DBConnStr)
                vntData = .mobjSCGLSql.SQLSelectArr(strSQL, intRowCnt, intColCnt, , True)
                Return vntData
            Catch err As Exception
                Throw RaiseSysErr(err, CLASS_NAME & ".Get_AOR_TAX")
            Finally
                .mobjSCGLSql.SQLDisconnect()
            End Try
        End With

    End Function

