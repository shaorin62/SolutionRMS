    ' ============== ProcessRtn_CUST 광고주별 거래명세서 생성
    Public Function ProcessRtn_CUST(ByVal strInfoXML As String, _
                                    ByVal strMasterXML As String, _
                                    ByVal vntData As Object, _
                                    ByRef strTRANSYEARMON As String, _
                                    ByRef intColFlag As Integer) As Integer

        Dim intRtn, intRtn1 As Integer  '결과값 변수
        Dim i, intColCnt, intRows As Integer '루프, 컬럼Cnt, 로우Cnt 변수
        Dim intTRANSNOSEQ As Integer
        Dim intCOMMINOSEQ As Integer
        Dim intCnt, intCnt2
        Dim lngAMT, lngSUMAMT
        Dim lngVAT, lngSUMVAT
        Dim strCUSTCODE
        Dim strMEDFLAG
        Dim strREAL_MED_CODE
        Dim strMEMO
        Dim strDEMANDDAY
        Dim strPRINTDAY
        Dim lngTRANSNO
        Dim strDEPT_CD

        SetConfig(strInfoXML) '기본정보 Setting
        With mobjSCGLConfig '기본정보를 가지고 있는 Config 개체
            Try
                'XML Element 변수 선언 (strMasterXML을 변환)
                Dim xmlRoot As XmlElement
                xmlRoot = XMLGetRoot(strMasterXML) 'XML 데이터

                'DB접속 및 트랜잭션 시작
                .mobjSCGLSql.SQLConnect(.DBConnStr)
                .mobjSCGLSql.SQLBeginTrans()
                'Master 데이터 처리
                For intCnt = 1 To intColFlag
                    intCnt2 = 0
                    '청구지그룹별 청구지코드 가져오기
                    If IsArray(vntData) Then
                        intColCnt = UBound(vntData, 1) : intRows = UBound(vntData, 2)
                        '''해당하는Row 만큼 Loop
                        lngAMT = 0 : lngSUMAMT = 0 : lngVAT = 0 : lngSUMVAT = 0 : strMEMO = ""

                        For i = 1 To intRows
                            If GetElement(vntData, "CHK", intColCnt, i, OPTIONAL_STR) = 1 Then
                                '청구합계
                                If GetElement(vntData, "TRANSCUSTRANK", intColCnt, i, OPTIONAL_STR) = intCnt Then
                                    lngAMT = CDbl(GetElement(vntData, "COMMISSION", intColCnt, i, OPTIONAL_STR))
                                    lngSUMAMT = lngSUMAMT + lngAMT
                                    lngVAT = CDbl(GetElement(vntData, "VAT", intColCnt, i, OPTIONAL_STR))
                                    lngSUMVAT = lngSUMVAT + lngVAT
                                End If
                            End If
                        Next

                        For i = 1 To intRows
                            If GetElement(vntData, "CHK", intColCnt, i, OPTIONAL_STR) = 1 Then
                                If GetElement(vntData, "TRANSCUSTRANK", intColCnt, i, OPTIONAL_STR) = intCnt Then
                                    '청구합계,부가세합계,청구지는 헤더에 변수로 저장
                                    If intCnt2 = intCnt Then
                                    Else
                                        strCUSTCODE = GetElement(vntData, "CLIENTCODE", intColCnt, i, OPTIONAL_STR)
                                        strREAL_MED_CODE = GetElement(vntData, "REAL_MED_CODE", intColCnt, i, OPTIONAL_STR)
                                        strMEDFLAG = GetElement(vntData, "MED_FLAG", intColCnt, i, OPTIONAL_STR)
                                        strDEPT_CD = GetElement(vntData, "DEPT_CD", intColCnt, i, OPTIONAL_STR)

                                        If GetElement(vntData, "MEMO", intColCnt, i, OPTIONAL_STR) <> "" Then
                                            strMEMO = GetElement(vntData, "MEMO", intColCnt, i, OPTIONAL_STR)
                                        End If

                                        If XMLGetElement(xmlRoot, "DEMANDDAY") <> "" Then strDEMANDDAY = XMLGetElement(xmlRoot, "DEMANDDAY").Substring(0, 4) & XMLGetElement(xmlRoot, "DEMANDDAY").Substring(5, 2) & XMLGetElement(xmlRoot, "DEMANDDAY").Substring(8, 2)
                                        If XMLGetElement(xmlRoot, "PRINTDAY") <> "" Then strPRINTDAY = XMLGetElement(xmlRoot, "PRINTDAY").Substring(0, 4) & XMLGetElement(xmlRoot, "PRINTDAY").Substring(5, 2) & XMLGetElement(xmlRoot, "PRINTDAY").Substring(8, 2)

                                        '헤더저장
                                        intRtn = ProcessRtn_CUSTHdr(xmlRoot, strTRANSYEARMON, lngTRANSNO, lngSUMAMT, lngSUMVAT, strDEPT_CD, strCUSTCODE, strREAL_MED_CODE, strMEDFLAG, strDEMANDDAY, strPRINTDAY, strMEMO)
                                        '디테일저장(거래명세서 번호는 위에서 참조)
                                        intRtn = ProcessRtn_CUSTDtl(strInfoXML, vntData, strTRANSYEARMON, lngTRANSNO, intTRANSNOSEQ, intCnt, strDEMANDDAY, strPRINTDAY) ', strDETAIL_ONE_IDs 라인의 시퀀스
                                    End If
                                    '무한업데이트.
                                    intCnt2 = intCnt
                                End If
                            End If
                        Next
                    End If
                Next

                .mobjSCGLSql.SQLCommitTrans()
                Return intRtn
            Catch err As Exception
                '트랜잭션RollBack 및 오류 전송
                .mobjSCGLSql.SQLRollbackTrans()
                Throw RaiseSysErr(err, CLASS_NAME & ".ProcessRtn_CUST")
            Finally
                'Resource해제
                .mobjSCGLSql.SQLDisconnect()
            End Try
        End With
    End Function

    '============== ProcessRtn_CUSTHdr 광고주별 거래명세서 생성 헤더 저장
    Public Function ProcessRtn_CUSTHdr(ByVal xmlRoot As XmlElement, _
                                       ByRef strTRANSYEARMON As String, _
                                       ByRef intTRANSNO As Integer, _
                                       ByRef lngSUMAMT As Double, _
                                       ByRef lngSUMVAT As Double, _
                                       ByRef strDEPT_CD As String, _
                                       ByRef strCUSTCODE As String, _
                                       ByRef strREAL_MED_CODE As String, _
                                       ByRef strMEDFLAG As String, _
                                       ByRef strDEMANDDAY As String, _
                                       ByRef strPRINTDAY As String, _
                                       ByRef strMEMO As String) As Double
        Dim intRtn As Integer
        With mobjSCGLConfig
            Try
                mobjceMD_AORCOMMI_HDR = New ceMD_AORCOMMI_HDR(mobjSCGLConfig)
                Select Case GetFlagMode(XMLGetElement(xmlRoot, meDATA_FLAG))
                    Case meINS_TRANS
                        intTRANSNO = SelectRtn_TRANSNO(strTRANSYEARMON)
                        intRtn = InsertRtn_TRANS_HDR(xmlRoot, intTRANSNO, strTRANSYEARMON, strDEMANDDAY, strPRINTDAY, lngSUMAMT, lngSUMVAT, strDEPT_CD, strCUSTCODE, strREAL_MED_CODE, strMEDFLAG, strMEMO)
                    Case meUPD_TRANS
                End Select
                Return intRtn
            Catch err As Exception
                Throw RaiseSysErr(err, CLASS_NAME & ".ProcessRtn_CUSTHdr")
            Finally
                mobjceMD_AORCOMMI_HDR.Dispose()
            End Try
        End With
    End Function

    '============== ProcessRtn_CUSTDtl 광고주별 거래명세서 생성 디테일 저장
    Public Function ProcessRtn_CUSTDtl(ByVal strInfoXML As String, _
                                       ByVal vntData As Object, _
                                       ByRef strTRANSYEARMON As String, _
                                       ByRef intTRANSNO As Integer, _
                                       ByRef intTRANSNOSEQ As Integer, _
                                       ByRef intCnt As Integer, _
                                       ByRef strDEMANDDAY As String, _
                                       ByRef strPRINTDAY As String) As Integer '데이터 INSERT/UPDATE

        Dim intRtn As Integer
        Dim i, intColCnt, intRows As Integer
        Dim lngAMT_DTL, lngVAT_DTL, IngSumAmtVat
        Dim strTBRDSTDATE, strTBRDEDDATE
        Dim strMEMO
        Dim lngTrust_seq
        Dim strTRUTRANS
        Dim lngTrust_YEARMON
        SetConfig(strInfoXML)
        With mobjSCGLConfig
            Try
                If IsArray(vntData) Then
                    '''사용할 Entity 개체생성(Config 정보를 넘겨생성)
                    mobjceMD_AORCOMMI_DTL = New ceMD_AORCOMMI_DTL(mobjSCGLConfig)
                    mobjceMD_AOR_MEDIUM = New eMDCO.ceMD_AOR_MEDIUM(mobjSCGLConfig)
                    '''vntData의 컬럼수, 로우수를 변수입력
                    intColCnt = UBound(vntData, 1) : intRows = UBound(vntData, 2)
                    '''해당하는Row 만큼 Loop
                    For i = 1 To intRows
                        ''' FLAG 조사 - INSERT/UPDATE 조사
                        Select Case GetFlagMode(vntData(0, i))
                            Case meINS_TRANS
                                If GetElement(vntData, "CHK", intColCnt, i, OPTIONAL_STR) = 1 Then
                                    If GetElement(vntData, "TRANSCUSTRANK", intColCnt, i, OPTIONAL_STR) = intCnt Then
                                        intTRANSNOSEQ = SelectRtn_TRANSNOSEQ(intTRANSNO, strTRANSYEARMON)
                                        strMEMO = GetElement(vntData, "MEMO", intColCnt, i, OPTIONAL_STR)
                                        intRtn = InsertRtn_TRANS_DTL(vntData, intColCnt, i, strTRANSYEARMON, intTRANSNO, intTRANSNOSEQ, strDEMANDDAY, strPRINTDAY, strMEMO)

                                        '신탁테이블에 TRU_TRANS_NO 업데이트
                                        lngTrust_seq = GetElement(vntData, "SEQ", intColCnt, i, OPTIONAL_STR)
                                        lngTrust_YEARMON = GetElement(vntData, "YEARMON", intColCnt, i, OPTIONAL_STR)
                                        strTRUTRANS = strTRANSYEARMON & "-" & intTRANSNO & "-" & intTRANSNOSEQ

                                        intRtn = mobjceMD_AOR_MEDIUM.Update_TruTrans(lngTrust_YEARMON, lngTrust_seq, strTRUTRANS, strDEMANDDAY)
                                    End If
                                End If
                        End Select
                    Next
                End If
                Return intRows
            Catch err As Exception
                Throw RaiseSysErr(err, CLASS_NAME & ".ProcessRtn_CUSTDTL")
            Finally
                mobjceMD_AORCOMMI_DTL.Dispose()
            End Try
        End With
    End Function

