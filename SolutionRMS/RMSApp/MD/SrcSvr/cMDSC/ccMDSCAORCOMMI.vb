    '============== 승인 처리 1
    Public Function ProcessRtn_Confirm(ByVal strInfoXML As String, _
                                       ByVal vntData As Object) As Integer

        Dim intRtn As Integer
        Dim i, intColCnt, intRows As Integer

        SetConfig(strInfoXML) '기본정보 Setting

        With mobjSCGLConfig '기본정보를 가지고 있는 Config 개체

            Try
                'DB접속 및 트랜잭션 시작
                .mobjSCGLSql.SQLConnect(.DBConnStr)
                .mobjSCGLSql.SQLBeginTrans()

                If IsArray(vntData) Then
                    '''사용할 Entity 개체생성(Config 정보를 넘겨생성)
                    mobjceMD_AORCOMMI_HDR = New ceMD_AORCOMMI_HDR(mobjSCGLConfig)
                    mobjceMD_AORCOMMI_DTL = New ceMD_AORCOMMI_DTL(mobjSCGLConfig)
                    '''vntData의 컬럼수, 로우수를 변수입력
                    intColCnt = UBound(vntData, 1) : intRows = UBound(vntData, 2)

                    '''해당하는Row 만큼 Loop
                    For i = 1 To intRows
                        If GetElement(vntData, "CHK", intColCnt, i, OPTIONAL_STR) = "1" Then
                            intRtn = UpdateRtn_TRANSHDRCONFIRM(vntData, intColCnt, i)
                            intRtn = UpdateRtn_TRANSDTLCONFIRM(vntData, intColCnt, i)
                        End If
                    Next
                End If

                '트랜잭션Commit
                .mobjSCGLSql.SQLCommitTrans()
                Return intRtn
            Catch err As Exception
                '트랜잭션RollBack 및 오류 전송
                .mobjSCGLSql.SQLRollbackTrans()
                Throw RaiseSysErr(err, CLASS_NAME & ".ProcessRtn_Confirm_OK")
            Finally
                mobjceMD_AORCOMMI_HDR.Dispose()
                mobjceMD_AORCOMMI_DTL.Dispose()
                'Resource해제
                .mobjSCGLSql.SQLDisconnect()
            End Try
        End With
    End Function

