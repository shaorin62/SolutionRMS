                strFormet = strFormet & " A.TBRDSTDATE, TBRDEDDATE, "
                strFormet = strFormet & " A.ROLLSTDATE, ROLLEDDATE, "
                strFormet = strFormet & " A.BRDSTTIME, BRDEDTIME, "
