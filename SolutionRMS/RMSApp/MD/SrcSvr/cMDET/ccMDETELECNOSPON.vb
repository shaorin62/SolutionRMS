        strSelFields = strSelFields & "PROGNAME ATTR02, "
        strSelFields = strSelFields & "INPUT_MEDFLAG, "
        strSelFields = strSelFields & "DBO.MD_GET_ELECMEDNAME_FUN(INPUT_MEDFLAG) INPUT_MEDNAME , "
