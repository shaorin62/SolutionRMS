                                    ByRef strCUSTCODE As String, _
                                    ByRef strMEDCODE As String, _
                                    ByRef strREAL_MED_CODE As String, _
                                    ByRef strDEPT_CD As String, _
                                    ByRef strMED_FLAG As String, _
                                    ByRef strCLIENTNAME As String, _
