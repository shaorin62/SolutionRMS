        strSelFields = strSelFields & "MEDCODE ,"
        strSelFields = strSelFields & "REAL_MED_CODE , "
        strSelFields = strSelFields & "DENSE_RANK() OVER (ORDER BY CLIENTCODE,REAL_MED_CODE,ISNULL(GFLAG,'') DESC) TRANSRANK,GFLAG,SUBSEQ,DBO.PD_JOBCUST_NAME_FUN(SUBSEQ) BRANDNAME,CLIENTSUBCODE,DBO.SC_GET_CUSTNAME_FUN(CLIENTSUBCODE) CLIENTSUBNAME"
        'SUBSEQ|BRANDNAME|CLIENTSUBCODE|CLIENTSUBNAME
