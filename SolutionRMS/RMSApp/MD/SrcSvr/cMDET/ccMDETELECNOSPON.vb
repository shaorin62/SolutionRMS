        strSelFields = strSelFields & "CASE ISNULL(BRDFRI,'') WHEN -1 THEN '금' ELSE '' END +"
        strSelFields = strSelFields & "CASE ISNULL(BRDSAT,'') WHEN -1 THEN '토' ELSE '' END AS WEEKDAY,"
        strSelFields = strSelFields & "AMT, "
        strSelFields = strSelFields & "CASE ISNULL(TRU_TAX_FLAG,'') WHEN 'Y' THEN ROUND((AMT*0.1),0) ELSE 0 END AS VAT,"
        strSelFields = strSelFields & "AMT + CASE ISNULL(TRU_TAX_FLAG,'') WHEN  'Y'  THEN ROUND((AMT*0.1),0) ELSE 0 END AS SUMAMTVAT,"
