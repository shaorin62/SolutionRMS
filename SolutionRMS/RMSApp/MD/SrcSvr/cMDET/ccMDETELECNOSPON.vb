    ' ============== ProcessRtn (Master & Detail) Sample Code 
    Public Function ProcessRtn(ByVal strInfoXML As String, _
                               ByVal strMasterXML As String, _
                               ByVal vntData As Object, _
                               ByRef intTRANSNO As Integer, _
                               ByRef strTRANSYEARMON As String, _
                               ByRef intColFlag As Integer) As Integer
        Dim intRtn, intRtn1 As Integer  '결과값 변수
        Dim i, intColCnt, intRows As Integer '루프, 컬럼Cnt, 로우Cnt 변수
        Dim intTRANSNOSEQ As Integer
        Dim intCOMMINOSEQ As Integer
        Dim intCnt, intCnt2
        Dim lngAMT, lngSUMAMT
        Dim lngVAT, lngSUMVAT
        Dim strCUSTCODE
        Dim strMEDCODE
        Dim strREAL_MED_CODE
        Dim strDEPT_CD
        Dim strMED_FLAG
        Dim strMEMO
        Dim strDEMANDDAY
        Dim strPRINTDAY
        Dim intTAXNO
        Dim strSEQ
        Dim strCLIENTNAME, strREAL_MED_NAME
        Dim strGFLAG

        SetConfig(strInfoXML) '기본정보 Setting
        With mobjSCGLConfig '기본정보를 가지고 있는 Config 개체
            Try
                'XML Element 변수 선언 (strMasterXML을 변환)
                Dim xmlRoot As XmlElement
                xmlRoot = XMLGetRoot(strMasterXML) 'XML 데이터

                'DB접속 및 트랜잭션 시작
                .mobjSCGLSql.SQLConnect(.DBConnStr)
                .mobjSCGLSql.SQLBeginTrans()
                'Master 데이터 처리
                For intCnt = 1 To intColFlag
                    intCnt2 = 0
                    '청구지그룹별 청구지코드 가져오기
                    If IsArray(vntData) Then
                        intColCnt = UBound(vntData, 1) : intRows = UBound(vntData, 2)
                        '''해당하는Row 만큼 Loop
                        lngAMT = 0
                        lngSUMAMT = 0
                        lngVAT = 0
                        lngSUMVAT = 0

                        For i = 1 To intRows
                            '청구합계
                            If GetElement(vntData, "TRANSRANK", intColCnt, i, OPTIONAL_STR) = intCnt And GetElement(vntData, "CHK", intColCnt, i, OPTIONAL_STR) = "1" Then
                                lngAMT = GetElement(vntData, "AMT", intColCnt, i, OPTIONAL_STR)
                                lngSUMAMT = lngSUMAMT + lngAMT
                            End If
                        Next
                        lngSUMVAT = Int(lngSUMAMT * 0.1)
                        For i = 1 To intRows
                            If GetElement(vntData, "TRANSRANK", intColCnt, i, OPTIONAL_STR) = intCnt And GetElement(vntData, "CHK", intColCnt, i, OPTIONAL_STR) = "1" Then
                                '청구합계,부가세합계,청구지는 헤더에 변수로 저장
                                If intCnt2 = intCnt Then
                                Else
                                    strCUSTCODE = GetElement(vntData, "CLIENTCODE", intColCnt, i, OPTIONAL_STR)
                                    strMEDCODE = GetElement(vntData, "MEDCODE", intColCnt, i, OPTIONAL_STR)
                                    strREAL_MED_CODE = GetElement(vntData, "REAL_MED_CODE", intColCnt, i, OPTIONAL_STR)
                                    strDEPT_CD = GetElement(vntData, "DEPTCD", intColCnt, i, OPTIONAL_STR)
                                    strMED_FLAG = GetElement(vntData, "INPUT_MEDFLAG", intColCnt, i, OPTIONAL_STR)
                                    strSEQ = GetElement(vntData, "SEQ", intColCnt, i, OPTIONAL_STR)
                                    strCLIENTNAME = GetElement(vntData, "CLIENTNAME", intColCnt, i, OPTIONAL_STR)
                                    strREAL_MED_NAME = GetElement(vntData, "REAL_MED_NAME", intColCnt, i, OPTIONAL_STR)
                                    strGFLAG = GetElement(vntData, "GFLAG", intColCnt, i, OPTIONAL_STR)
                                    '위수탁 거래명세서 투입
                                    intRtn = ProcessRtn_Hdr(xmlRoot, intTRANSNO, strTRANSYEARMON, lngSUMAMT, lngSUMVAT, strCUSTCODE, strMEDCODE, strREAL_MED_CODE, strDEPT_CD, strMED_FLAG, strGFLAG)

                                    '세금계산서 생성
                                    'intRtn = ProcessRtn_TRUTAX(xmlRoot, strTRANSYEARMON, intTAXNO, lngSUMAMT, lngSUMVAT, strCUSTCODE, strMEDCODE, strREAL_MED_CODE, strDEPT_CD, strMED_FLAG, strCLIENTNAME, strREAL_MED_NAME)
                                    '디테일저장
                                    If IsArray(vntData) Then
                                        If XMLGetElement(xmlRoot, "DEMANDDAY") <> "" Then strDEMANDDAY = XMLGetElement(xmlRoot, "DEMANDDAY").Substring(0, 4) & XMLGetElement(xmlRoot, "DEMANDDAY").Substring(5, 2) & XMLGetElement(xmlRoot, "DEMANDDAY").Substring(8, 2)
                                        If XMLGetElement(xmlRoot, "PRINTDAY") <> "" Then strPRINTDAY = XMLGetElement(xmlRoot, "PRINTDAY").Substring(0, 4) & XMLGetElement(xmlRoot, "PRINTDAY").Substring(5, 2) & XMLGetElement(xmlRoot, "PRINTDAY").Substring(8, 2)

                                        '위수탁 거래명세서 디테일
                                        intRtn = ProcessRtn_Dtl(strInfoXML, vntData, intTRANSNO, strTRANSYEARMON, intTRANSNOSEQ, intCnt, strDEMANDDAY, strPRINTDAY) ', strDETAIL_ONE_IDs 라인의 시퀀스
                                    End If
                                    '위수탁세금계산서 번호를 위수탁거래명세서에 UPDATE
                                    'intRtn = ProcessRtn_TRANSTAXTRUST(strTRANSYEARMON, intTRANSNO, strTRANSYEARMON, intTAXNO)

                                End If
                                '무한업데이트.
                                intCnt2 = intCnt
                            End If
                        Next
                    End If
                    '헤더저장
                Next

                .mobjSCGLSql.SQLCommitTrans()
                Return intRtn
            Catch err As Exception
                '트랜잭션RollBack 및 오류 전송
                .mobjSCGLSql.SQLRollbackTrans()
                Throw RaiseSysErr(err, CLASS_NAME & ".ProcessRtn")
            Finally
                'Resource해제
                mobjceMD_ELEC_TRANS_DTL.Dispose()
                .mobjSCGLSql.SQLDisconnect()
            End Try
        End With
    End Function

    '============== ProcessRtn ( Sheet ) Sample Code
    Public Function ProcessRtn_Dtl(ByVal strInfoXML As String, _
                                   ByVal vntData As Object, _
                                   ByRef intTRANSNO As Integer, _
                                   ByRef strTRANSYEARMON As String, _
                                   ByRef intTRANSNOSEQ As Integer, _
                                   ByRef intCnt As Integer, _
                                   ByRef strDEMANDDAY As String, _
                                   ByRef strPRINTDAY As String) As Integer '데이터 INSERT/UPDATE
        Dim intRtn As Integer
        Dim intRtn1 As Integer
        Dim i, intColCnt, intRows As Integer
        Dim lngAMT_DTL, lngVAT_DTL
        Dim strTRANSNO As String
        'Dim strSUBSEQ, strCLIENTSUBCODE
        Dim strYEARMON As String
        Dim strSEQ As String
        SetConfig(strInfoXML)
        With mobjSCGLConfig
            Try
                If IsArray(vntData) Then
                    '''사용할 Entity 개체생성(Config 정보를 넘겨생성)
                    mobjceMD_ELEC_TRANS_DTL = New ceMD_ELEC_TRANS_DTL(mobjSCGLConfig)
                    mobjceMD_ELEC_TRANS_DTL = New ceMD_ELEC_TRANS_DTL(mobjSCGLConfig)
                    '''vntData의 컬럼수, 로우수를 변수입력
                    intColCnt = UBound(vntData, 1) : intRows = UBound(vntData, 2)
                    '''해당하는Row 만큼 Loop
                    For i = 1 To intRows
                        ''' FLAG 조사 - INSERT/UPDATE 조사
                        Select Case GetFlagMode(vntData(0, i))
                            Case meINS_TRANS
                                If GetElement(vntData, "TRANSRANK", intColCnt, i, OPTIONAL_STR) = intCnt And GetElement(vntData, "CHK", intColCnt, i, OPTIONAL_STR) = 1 Then
                                    intTRANSNOSEQ = SelectRtn_TRANSNOSEQ(intTRANSNO, strTRANSYEARMON)
                                    lngAMT_DTL = GetElement(vntData, "AMT", intColCnt, i, OPTIONAL_STR)
                                    lngVAT_DTL = GetElement(vntData, "VAT", intColCnt, i, OPTIONAL_STR)
                                    strYEARMON = GetElement(vntData, "YEARMON", intColCnt, i, OPTIONAL_STR)
                                    strSEQ = GetElement(vntData, "SEQ", intColCnt, i, OPTIONAL_STR)
                                    strTRANSNO = strTRANSYEARMON & "-" & intTRANSNO
                                    intRtn = InsertRtn_TRANS_DTL(vntData, intColCnt, i, intTRANSNO, strTRANSYEARMON, intTRANSNOSEQ, strDEMANDDAY, strPRINTDAY, lngVAT_DTL)
                                    intRtn1 = mobjceMD_ELEC_TRANS_DTL.UPDATE_TRANS_DO(strTRANSNO, strYEARMON, strSEQ)
                                End If
                                'Case meUPD_TRANS
                                '  intRtn = UpdateRtn_TRANS_DTL(vntData, intColCnt, i, intTRANSNO, strTRANSYEARMON, intTRANSNOSEQ, strDEMANDDAY, strPRINTDAY, lngVAT_DTL)
                        End Select
                    Next
                End If
