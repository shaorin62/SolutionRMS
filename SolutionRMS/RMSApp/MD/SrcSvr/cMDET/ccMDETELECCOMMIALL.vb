                mobjceMD_COMMITAX_HDR = New ceMD_COMMITAX_HDR(mobjSCGLConfig)
                mobjceMD_COMMITAX_DTL = New ceMD_COMMITAX_DTL(mobjSCGLConfig)
                mobjceMD_ELECCOMMI_DTL = New ceMD_ELECCOMMI_DTL(mobjSCGLConfig)

                strCLIENTACCODE = "" 'GetCLIENTACCODE(strCUSTCODE)
                strREAL_MED_ACCODE = "" 'GetCLIENTACCODE(strREAL_MED_CODE)
                strMEDBUSINO = GetBUSINO(strMEDCODE)

                strREAL_MEDOWNER = GetOWNER(strREAL_MED_CODE)
                strREAL_MEDADDR1 = GetADDR1(strREAL_MED_CODE)
                strREAL_MEDADDR2 = GetADDR2(strREAL_MED_CODE)

                intTAXNO = SelectRtn_COMMITAXNO(strCOMMIYEARMON)
                intTAXNOSEQ = SelectRtn_COMMITAXSEQ(intTAXNO, strCOMMIYEARMON)
                strCOMMITAXNO = strCOMMIYEARMON & "-" & intTAXNO
                intRtn = InsertRtn_COMMITAX_HDR(xmlRoot, strCOMMIYEARMON, intTAXNO, strCUSTCODE, strCLIENTACCODE, strREAL_MED_CODE, strREAL_MED_ACCODE, strDEPT_CD, lngSUMAMT, lngSUMVAT, strDEMANDDAY, strPRINTDAY, strCLIENTBISNO, strREAL_MED_BISNO, strREAL_MED_NAME, strREAL_MEDOWNER, strREAL_MEDADDR1, strREAL_MEDADDR2, lngSUMREAL_AMT, lngSUSURATESUM)
                intRtn = InsertRtn_COMMITAX_DTL(xmlRoot, strCOMMIYEARMON, intTAXNO, intTAXNOSEQ, strREAL_MED_CODE, strMEDCODE, strMEDBUSINO, lngSUMAMT, lngSUMVAT, lngSUMREAL_AMT, lngSUSURATESUM)
                intRtn = mobjceMD_ELECCOMMI_DTL.UPDATE_COMMITAXNO_DO(strCOMMIYEARMON, strMED_FLAG, strCUSTCODE, strREAL_MED_CODE, strTRANSRANK, strCOMMITAXNO)
                Return intRtn
            Catch err As Exception
                Throw RaiseSysErr(err, CLASS_NAME & ".ProcessRtn_COMMITAX")
            Finally
                mobjceMD_COMMITAX_HDR.Dispose()
                mobjceMD_COMMITAX_DTL.Dispose()
            End Try
        End With
    End Function

    '============== ProcessRtn ( Sheet ) Sample Code
    Public Function ProcessRtn_COMMI_Dtl(ByVal strInfoXML As String, _
                                         ByVal vntData1 As Object, _
                                         ByRef intCOMMINO As Integer, _
                                         ByRef strCOMMIYEARMON As String, _
                                         ByRef intCOMMINOSEQ As Integer, _
                                         ByRef intCnt As Integer, _
                                         ByRef strDEMANDDAY As String, _
                                         ByRef strPRINTDAY As String, _
                                         ByRef strMED_FLAG As String, _
                                         ByRef strTRANSRANK As String) As Integer '데이터 INSERT/UPDATE

        Dim intRtn As Integer
        Dim intRtn1 As Integer
        Dim intRtn2 As Integer
        Dim i, intColCnt, intRows As Integer
        Dim lngSUSU_DTL, lngVAT_DTL
        Dim strCLIENTCODE, strREAL_MED_CODE, strTRUSTSEQ
        Dim strCOMMTRANSNO As String
        SetConfig(strInfoXML)
        With mobjSCGLConfig
            Try
                If IsArray(vntData1) Then
                    '''사용할 Entity 개체생성(Config 정보를 넘겨생성)
                    mobjceMD_ELECCOMMI_DTL = New ceMD_ELECCOMMI_DTL(mobjSCGLConfig)
                    '''vntData의 컬럼수, 로우수를 변수입력
                    intColCnt = UBound(vntData1, 1) : intRows = UBound(vntData1, 2)
                    '''해당하는Row 만큼 Loop
                    For i = 1 To intRows
                        ''' FLAG 조사 - INSERT/UPDATE 조사
                        Select Case GetFlagMode(vntData1(0, i))
                            Case meINS_TRANS
                                If GetElement(vntData1, "TRANSRANK", intColCnt, i, OPTIONAL_STR) = intCnt Then
                                    strCLIENTCODE = GetElement(vntData1, "CLIENTCODE", intColCnt, i, OPTIONAL_STR)
                                    strREAL_MED_CODE = GetElement(vntData1, "REAL_MED_CODE", intColCnt, i, OPTIONAL_STR)
                                    'strTRUSTSEQ = SelectRtn_TRUSTSEQ(strCOMMIYEARMON, strCLIENTCODE, strREAL_MED_CODE)

                                    lngSUSU_DTL = GetElement(vntData1, "SUSU", intColCnt, i, OPTIONAL_STR)
                                    strMED_FLAG = GetElement(vntData1, "INPUT_MEDFLAG", intColCnt, i, OPTIONAL_STR)
                                    intCOMMINOSEQ = SelectRtn_COMMINOSEQ(intCOMMINO, strCOMMIYEARMON)
                                    lngVAT_DTL = GetElement(vntData1, "SUSUVAT", intColCnt, i, OPTIONAL_STR)
                                    strCOMMTRANSNO = strCOMMIYEARMON & "-" & intCOMMINO ' & "-" & intCOMMINOSEQ 거래명세 디테일내역 가져오지 않음
                                    intRtn = InsertRtn_COMMI_DTL(vntData1, intColCnt, i, intCOMMINO, strCOMMIYEARMON, intCOMMINOSEQ, strDEMANDDAY, strPRINTDAY, lngVAT_DTL)
                                    '수수료거래명세서 번호 업데이트
                                    intRtn = mobjceMD_ELECCOMMI_DTL.UPDATE_COMMI_DO(strCOMMIYEARMON, strCLIENTCODE, strREAL_MED_CODE, strMED_FLAG, intCnt, strCOMMTRANSNO)
                                End If

                        End Select
                    Next
                End If
                Return intRows
            Catch err As Exception
                Throw RaiseSysErr(err, CLASS_NAME & ".ProcessRtn_Dtl")
            Finally
                mobjceMD_ELECCOMMI_DTL.Dispose()
            End Try
        End With
    End Function

