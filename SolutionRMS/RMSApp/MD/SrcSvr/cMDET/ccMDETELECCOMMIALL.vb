                                        ByRef strCOMMIYEARMON As String, _
                                        ByRef intTAXNO As String, _
                                        ByRef lngSUMAMT As Double, _
                                        ByRef lngSUMVAT As Double, _
                                        ByRef strCUSTCODE As String, _
                                        ByRef strMEDCODE As String, _
                                        ByRef strREAL_MED_CODE As String, _
                                        ByRef strDEPT_CD As String, _
                                        ByRef strMED_FLAG As String, _
                                        ByRef strCLIENTBISNO As String, _
                                        ByRef strREAL_MED_BISNO As String, _
