    ' ============== ProcessRtn (Master & Detail) Sample Code 
    Public Function ProcessRtn(ByVal strInfoXML As String, _
                               ByVal strMasterXML As String, _
                               ByVal vntData1 As Object, _
                               ByRef intCOMMINO As Integer, _
                               ByRef strCOMMIYEARMON As String, _
                               ByRef intColFlag1 As Integer, _
                               ByRef strDESCRIPTION As String) As Integer
        Dim intRtn, intRtn1, intRtnTRANS As Integer '결과값 변수
        Dim i, intColCnt, intRows As Integer '루프, 컬럼Cnt, 로우Cnt 변수
        Dim intTRANSNOSEQ As Integer
        Dim intCOMMINOSEQ As Integer
        Dim intCnt, intCnt2
        Dim lngAMT, lngSUMAMT
        Dim lngVAT, lngSUMVAT
        Dim strCUSTCODE
        Dim strMEDCODE
        Dim strREAL_MED_CODE
        Dim strDEPT_CD
        Dim strMED_FLAG
        Dim strMEMO
        Dim strDEMANDDAY
        Dim strPRINTDAY
        Dim intTAXNO
        Dim strSEQ As String
        Dim strCLIENTBISNO As String
        Dim strREAL_MED_BISNO As String
        Dim lngREAL_AMT As Double
        Dim lngSUSURATE As Double
        Dim strCOMMISEQ As String
        Dim strTRANSRANK As String
        Dim strCLIENTNAME, strREAL_MED_NAME
        Dim strREAL_MED_NAME2
        Dim lngRtv As Long, lngCnt As Long, strDesc As String '수수료번호 신탁업데이트 Procedure 처리시 사용
        Dim lngSUMREAL_AMT
        Dim lngSUSURATESUM

        SetConfig(strInfoXML) '기본정보 Setting
        With mobjSCGLConfig '기본정보를 가지고 있는 Config 개체
            Try
                'XML Element 변수 선언 (strMasterXML을 변환)
                Dim xmlRoot As XmlElement
                xmlRoot = XMLGetRoot(strMasterXML) 'XML 데이터

                'DB접속 및 트랜잭션 시작
                .mobjSCGLSql.SQLConnect(.DBConnStr)
                .mobjSCGLSql.SQLBeginTrans()
                'Master 데이터 처리

                '********************************************
                '여기부터 수수료 테이블로 저장
                '********************************************
                For intCnt = 1 To intColFlag1
                    intCnt2 = 0
                    '청구지그룹별 청구지코드 가져오기
                    If IsArray(vntData1) Then
                        intColCnt = UBound(vntData1, 1) : intRows = UBound(vntData1, 2)
                        '''해당하는Row 만큼 Loop
                        lngAMT = 0
                        lngSUMAMT = 0
                        lngVAT = 0
                        lngSUMVAT = 0
                        lngREAL_AMT = 0
                        lngSUMREAL_AMT = 0
                        strCUSTCODE = ""
                        strMEDCODE = ""
                        strREAL_MED_CODE = ""
                        strDEPT_CD = ""
                        strMED_FLAG = ""
                        For i = 1 To intRows
                            '청구합계
                            If GetElement(vntData1, "TRANSRANK", intColCnt, i, OPTIONAL_STR) = intCnt Then
                                lngAMT = GetElement(vntData1, "SUSU", intColCnt, i, OPTIONAL_STR)
                                lngREAL_AMT = GetElement(vntData1, "AMT", intColCnt, i, OPTIONAL_STR)
                                lngSUMAMT = lngSUMAMT + lngAMT
                                lngSUMREAL_AMT = lngSUMREAL_AMT + lngREAL_AMT
                            End If
                        Next
                        lngSUMVAT = Int((lngSUMAMT * 0.1))
                        lngSUSURATESUM = gRound(((lngSUMAMT / lngSUMREAL_AMT) * 100), 2)
                        For i = 1 To intRows
                            If GetElement(vntData1, "TRANSRANK", intColCnt, i, OPTIONAL_STR) = intCnt Then
                                '청구합계,부가세합계,청구지는 헤더에 변수로 저장

                                If intCnt2 = intCnt Then
                                Else
                                    strCUSTCODE = GetElement(vntData1, "CLIENTCODE", intColCnt, i, OPTIONAL_STR)
                                    strMEDCODE = GetElement(vntData1, "MEDCODE", intColCnt, i, OPTIONAL_STR)
                                    strREAL_MED_CODE = GetElement(vntData1, "REAL_MED_CODE", intColCnt, i, OPTIONAL_STR)
                                    strDEPT_CD = GetElement(vntData1, "DEPTCD", intColCnt, i, OPTIONAL_STR)
                                    strMED_FLAG = GetElement(vntData1, "INPUT_MEDFLAG", intColCnt, i, OPTIONAL_STR)
                                    strCLIENTBISNO = GetElement(vntData1, "CLIENTBISNO", intColCnt, i, OPTIONAL_STR)
                                    strREAL_MED_BISNO = GetElement(vntData1, "REAL_MED_BISNO", intColCnt, i, OPTIONAL_STR)
                                    strCOMMISEQ = GetElement(vntData1, "SEQ", intColCnt, i, OPTIONAL_STR)
                                    strTRANSRANK = GetElement(vntData1, "TRANSRANK", intColCnt, i, OPTIONAL_STR)
                                    strCOMMIYEARMON = GetElement(vntData1, "YEARMON", intColCnt, i, OPTIONAL_STR)
                                    strREAL_MED_NAME2 = GetElement(vntData1, "REAL_MED_NAME", intColCnt, i, OPTIONAL_STR)
                                    '수수료 거래명세서 투입
