                                    intRtn = ProcessRtn_COMMITAXTRUST(strCOMMIYEARMON, intCOMMINO, strCOMMIYEARMON, intTAXNO)
                                End If
                                '무한업데이트.
                                intCnt2 = intCnt
                            End If
                        Next
                    End If
                    '헤더저장
                Next
                '신탁의 수수료거래명세서,수수료세금계산서 일괄업데이트
                'lngRtv = .mobjSCGLSql.SQLStoredProc("MD_ELECTRICMEDIUM_UPDATE_PRO", strCOMMIYEARMON, "", "", "", lngCnt, strDesc)
                'If lngRtv <> 0 Then
                'strDESCRIPTION = "거래명세서 업데이트 오류확인!"
                'End If
                intRtnTRANS = ProcessRtn_COMMITRANS_BAT(strCOMMIYEARMON)
                .mobjSCGLSql.SQLCommitTrans()
                Return intRtn
            Catch err As Exception
