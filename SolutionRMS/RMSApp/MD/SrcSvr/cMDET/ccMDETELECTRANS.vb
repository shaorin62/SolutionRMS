    ' ===============거래명세서 생성내역.. 광고주에따른 헤더조회
    Public Function SelectRtn_HDR(ByVal strInfoXML As String, _
                                  ByRef intRowCnt As Integer, _
                                  ByRef intColCnt As Integer, _
                                  ByVal strTRANSYEARMON As String, _
                                  ByVal strCLIENTCODE As String) As Object


        Dim strSQL As String            'SQL문
        Dim strFormat As String         '임시 SQL문
        Dim strWhere As String
        Dim vntData As Object
        Dim Con1, Con2 As String


        SetConfig(strInfoXML)

        If strTRANSYEARMON <> "" Then Con1 = String.Format(" AND (TRANSYEARMON = '{0}')", strTRANSYEARMON) '년월
        If strCLIENTCODE <> "" Then Con2 = String.Format(" AND (CLIENTCODE = '{0}')", strCLIENTCODE) '광고주코드

        strWhere = BuildFields(" ", Con1, Con2)


        'strFormat = "SELECT "
        'strFormat = strFormat & " TRANSYEARMON,"
        'strFormat = strFormat & " TRANSNO,"
        'strFormat = strFormat & " CASE VOCH_TYPE WHEN '0' THEN '위수탁'  "
        'strFormat = strFormat & "                WHEN '1' THEN '협찬' "
        'strFormat = strFormat & " ELSE '일반' END AS VOCH_TYPE , "
        'strFormat = strFormat & " CASE CONFIRMFLAG WHEN '1' THEN '승인' "
        'strFormat = strFormat & " ELSE '미승인' END AS CONFIRMFLAGNAME, "
        'strFormat = strFormat & " REAL_MED_CODE,DBO.SC_GET_HIGHCUSTNAME_FUN(REAL_MED_CODE) REAL_MED_NAME,  "
        'strFormat = strFormat & " AMT, VAT, (AMT+VAT) SUMAMTVAT, 0  SUMAMT  "
        'strFormat = strFormat & " FROM MD_ELEC_TRANS_HDR WHERE 1=1  {0} "

        strFormat = " SELECT "
        strFormat = strFormat & " H.TRANSYEARMON,"
        strFormat = strFormat & " H.TRANSNO,"
        strFormat = strFormat & " CASE H.VOCH_TYPE WHEN '0' THEN '위수탁'  "
        strFormat = strFormat & "                WHEN '1' THEN '협찬' "
        strFormat = strFormat & " ELSE '일반' END AS VOCH_TYPE , "
        strFormat = strFormat & " CASE H.CONFIRMFLAG WHEN '1' THEN '승인' "
        strFormat = strFormat & " ELSE '미승인' END AS CONFIRMFLAGNAME, "
        strFormat = strFormat & " H.REAL_MED_CODE,DBO.SC_GET_HIGHCUSTNAME_FUN(H.REAL_MED_CODE) REAL_MED_NAME,  "
        strFormat = strFormat & " H.AMT, H.VAT, (H.AMT+H.VAT) SUMAMTVAT, A.CNT "
        strFormat = strFormat & " FROM "
        strFormat = strFormat & " 	(	SELECT TRANSYEARMON, TRANSNO, COUNT(*) CNT "
        strFormat = strFormat & " 		FROM MD_ELEC_TRANS_DTL"
        strFormat = strFormat & " 		WHERE 1=1 {0}"
        strFormat = strFormat & " 		GROUP BY TRANSYEARMON, TRANSNO"
        strFormat = strFormat & " 	 ) A"
        strFormat = strFormat & " INNER JOIN MD_ELEC_TRANS_HDR H"
        strFormat = strFormat & " ON A.TRANSYEARMON = H.TRANSYEARMON"
        strFormat = strFormat & " AND A. TRANSNO = H.TRANSNO"
        strFormat = strFormat & " INNER JOIN  "
        strFormat = strFormat & " (	SELECT TRANSYEARMON,TRANSNO,CONFIRMFLAG "
        strFormat = strFormat & " 	FROM ("
        strFormat = strFormat & " 			SELECT TRANSYEARMON,TRANSNO,"
        strFormat = strFormat & " 			CASE SUM(ISNULL(CONFIRMFLAG,0)) "
        strFormat = strFormat & " 			WHEN 0 THEN '' "
        strFormat = strFormat & " 			ELSE 'O' END CONFIRMFLAG "
        strFormat = strFormat & " 			FROM MD_ELEC_TRANS_DTL "
        strFormat = strFormat & " 			WHERE 1=1  {0}"
        strFormat = strFormat & " 			GROUP BY TRANSYEARMON,TRANSNO"
        strFormat = strFormat & " 		) DATA"
        strFormat = strFormat & " ) B"
        strFormat = strFormat & " ON A.TRANSYEARMON = B.TRANSYEARMON"


        strFormat = strFormat & " AND A.TRANSNO = B.TRANSNO"
        strSQL = String.Format(strFormat, strWhere)

        '기본정보 Setting
        With mobjSCGLConfig '기본정보 Config 개체
            Try
                ' DB 접속
                .mobjSCGLSql.SQLConnect(.DBConnStr)
                ' ------ Array 데이터 조회 (True 일때 헤더정보 포함 조회(Sheet Data Binding 할 경우 사용), False 일때 데이터만 조회)
                vntData = .mobjSCGLSql.SQLSelectArr(strSQL, intRowCnt, intColCnt, , True)

                Return vntData
            Catch err As Exception
                ' 오류 전송
                '.mobjSCGLSql.SQLRollbackTrans()
                Throw RaiseSysErr(err, CLASS_NAME & ".SelectRtn")
            Finally
                ' DB 접속 종료
                .mobjSCGLSql.SQLDisconnect()
            End Try
        End With
