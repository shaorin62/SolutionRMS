                mobjceMD_ELECTRIC_MEDIUM = New ceMD_ELECTRIC_MEDIUM(mobjSCGLConfig)
                mobjceMD_ELEC_TRANS_HDR = New ceMD_ELEC_TRANS_HDR(mobjSCGLConfig)
                mobjceMD_ELEC_TRANS_DTL = New ceMD_ELEC_TRANS_DTL(mobjSCGLConfig)

                ' DB 접속 및 트랜잭션 시작
                .mobjSCGLSql.SQLConnect(.DBConnStr)
                .mobjSCGLSql.SQLBeginTrans()
                ' 엔티티 오브젝트의 Delete 메소드 호출
                '청구지그룹별 청구지코드 가져오기
                If IsArray(vntData) Then
                    intColCnt = UBound(vntData, 1) : intRows = UBound(vntData, 2)
                    '''해당하는Row 만큼 Loop
                    For i = 1 To intRows
                        If GetElement(vntData, "CHK", intColCnt, i, OPTIONAL_STR) = 1 Then
