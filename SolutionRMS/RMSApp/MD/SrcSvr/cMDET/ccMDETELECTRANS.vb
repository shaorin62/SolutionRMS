                            intRtn = mobjceMD_ELECTRIC_MEDIUM.TRANSDeleteUpdateDo(strTRANSYEARMON, strTRANSNO)
                            intRtn = mobjceMD_ELEC_TRANS_DTL.DeleteDo(strTRANSYEARMON, strTRANSNO)
                        End If
                    Next
                End If

                ' 트랜잭션 Commit
                .mobjSCGLSql.SQLCommitTrans()
