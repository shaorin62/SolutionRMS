            Finally
                'DB접속 종료
                .mobjSCGLSql.SQLDisconnect()
                '사용한 Entity(개체Dispose)
                mobjceMD_ELECTRIC_MEDIUM.Dispose()
                mobjceMD_ELEC_TRANS_HDR.Dispose()
                mobjceMD_ELEC_TRANS_DTL.Dispose()
            End Try
        End With
    End Function

