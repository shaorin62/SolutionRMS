                                        intRtn = mobjceMD_INTERNET_MEDIUM.Update_TruTrans(lngTrust_YEARMON, lngTrust_seq, strTRUTRANS, strDEMANDDAY)
                                    End If
                                End If
                            Case meUPD_TRANS
                                'intRtn = UpdateRtn_TRANS_DTL(vntData, intColCnt, i, intTRANSNO, strTRANSYEARMON, intTRANSNOSEQ, strDEMANDDAY, strPRINTDAY, lngVAT_DTL, IngSumAmtVat, strPUB_DATE, strMEMO)
                        End Select
