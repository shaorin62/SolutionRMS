    '상세페이지의 부가세/적요 수정시 시트 부가세 수정시 dtl 변경금액이 hdr 에 합산
    Public Function ProcessRtn_VAT(ByVal strInfoXML As String, _
                                   ByVal vntData As Object, _
                                   ByVal strTRANSYEARMON As String, _
                                   ByVal strTRANSNO As Integer) As Object

        Dim intRtn As Integer
        Dim i, intColCnt, intRows As Integer
        Dim intSEQ
        Dim lngVAT
        Dim lngSUMVAT

        SetConfig(strInfoXML)
        With mobjSCGLConfig
            Try
                .mobjSCGLSql.SQLConnect(.DBConnStr)
                .mobjSCGLSql.SQLBeginTrans()

                If IsArray(vntData) Then
                    '''사용할 Entity 개체생성(Config 정보를 넘겨생성)
                    mobjceMD_INTERNETTRANS_DTL = New ceMD_INTERNETTRANS_DTL(mobjSCGLConfig)
                    mobjceMD_INTERNETTRANS_HDR = New ceMD_INTERNETTRANS_HDR(mobjSCGLConfig)

                    '''vntData의 컬럼수, 로우수를 변수입력
                    intColCnt = UBound(vntData, 1) : intRows = UBound(vntData, 2)

                    lngSUMVAT = 0

                    For i = 1 To intRows
                        intSEQ = 0
                        lngVAT = 0
                        intSEQ = GetElement(vntData, "SEQ", intColCnt, i, OPTIONAL_NUM)
                        lngVAT = GetElement(vntData, "VAT", intColCnt, i, OPTIONAL_NUM)
                        '거래명세서 DTL 업데이트
                        intRtn = mobjceMD_INTERNETTRANS_DTL.Update_Vat(strTRANSYEARMON, strTRANSNO, intSEQ, lngVAT)

                        '거래명세서 부가세 합산
                        lngSUMVAT = lngSUMVAT + lngVAT
                    Next

                    '변경된 세부내역의 부가세의 합을 거래명세서 헤더에 합산 Update
                    intRtn = mobjceMD_INTERNETTRANS_HDR.Update_VatHDR(strTRANSYEARMON, strTRANSNO, lngSUMVAT)
                End If

                .mobjSCGLSql.SQLCommitTrans()
                Return intRows
            Catch err As Exception
                .mobjSCGLSql.SQLRollbackTrans()
                Throw RaiseSysErr(err, CLASS_NAME & ".ProcessRtn_VAT")
            Finally
                .mobjSCGLSql.SQLDisconnect()
                mobjceMD_INTERNETTRANS_HDR.Dispose()
                mobjceMD_INTERNETTRANS_DTL.Dispose()
            End Try
        End With
    End Function

