            If strYEARMON <> "" And strEDYEARMON <> "" Then
                Con1 = String.Format(" AND (A.YEARMON BETWEEN '{0}' AND  '{1}')", strYEARMON, strEDYEARMON)
            End If
            If strYEARMON <> "" And strEDYEARMON = "" Then
                Con1 = String.Format(" AND (A.YEARMON >= '{0}')", strYEARMON)
            End If
            If strYEARMON = "" And strEDYEARMON <> "" Then
                Con1 = String.Format(" AND (A.YEARMON <= '{0}')", strEDYEARMON)
            End If

            If strCLIENTCODE <> "" Then Con2 = String.Format(" AND A.CLIENTCODE = '{0}'", strCLIENTCODE)
