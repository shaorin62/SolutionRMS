    Public Function ProcessRtn(ByVal strInfoXML As String, _
                               ByVal vntData As Object, _
                               ByVal strCAMPAIGN_CODE As String, _
                               ByVal strCLIENTCODE As String, _
                               ByVal strTIMCODE As String, _
                               ByVal strSUBSEQ As String, _
                               ByVal strTBRDSTDATE As String, _
                               ByVal strTBRDEDDATE As String, _
                               ByVal strDEPT_CD As String) As Object

        Dim intRtn As Integer
        Dim i, intColCnt, intRows As Integer
        Dim strYEARMON, strOLDYEARMON
        Dim strSEQ, strOLDSEQ
        Dim strSQL
        Dim strTRU_TAX_FLAG
        Dim strDEMANDDAY


        SetConfig(strInfoXML)
        With mobjSCGLConfig
            Try
                .mobjSCGLSql.SQLConnect(.DBConnStr)
                .mobjSCGLSql.SQLBeginTrans()
                If IsArray(vntData) Then

                    '''사용할 Entity 개체생성(Config 정보를 넘겨생성)
                    mobjceMD_INTERNET_MEDIUM = New ceMD_INTERNET_MEDIUM(mobjSCGLConfig)

                    '''vntData의 컬럼수, 로우수를 변수입력
                    intColCnt = UBound(vntData, 1) : intRows = UBound(vntData, 2)

                    If strTBRDSTDATE <> "" Then strTBRDSTDATE = Trim(Replace(strTBRDSTDATE, "-", ""))
                    If strTBRDEDDATE <> "" Then strTBRDEDDATE = Trim(Replace(strTBRDEDDATE, "-", ""))

                    For i = 1 To intRows
                        If GetElement(vntData, "SEQ", intColCnt, i, NULL_NUM, True) = -999999 Then
                            '년월 정리
                            If Len(GetElement(vntData, "YEARMON", intColCnt, i, OPTIONAL_STR)) <> 6 Then
                                strYEARMON = GetElement(vntData, "YEARMON", intColCnt, i, OPTIONAL_STR).Substring(0, 4) & GetElement(vntData, "YEARMON", intColCnt, i, OPTIONAL_STR).Substring(5, 2)
                            Else
                                strYEARMON = GetElement(vntData, "YEARMON", intColCnt, i, OPTIONAL_STR)
                            End If

                            If GetElement(vntData, "TRU_TAX_FLAG", intColCnt, i, OPTIONAL_STR) = "1" Then
                                strTRU_TAX_FLAG = "Y"
                            Else
                                strTRU_TAX_FLAG = "N"
                            End If

                            strDEMANDDAY = Trim(Replace(GetElement(vntData, "DEMANDDAY", intColCnt, i, OPTIONAL_STR), "-", ""))
                            strSEQ = SelectRtn_SEQ(strYEARMON)
                                '바뀐년월로 신규저장
                            intRtn = InsertRtn_MD_INTERNET_MEDIUM(vntData, intColCnt, i, _
                                                                  strYEARMON, strSEQ, _
                                                                  strCAMPAIGN_CODE, strCLIENTCODE, strTIMCODE, strSUBSEQ, _
                                                                  strTBRDSTDATE, strTBRDEDDATE, _
                                                                  strDEPT_CD, strTRU_TAX_FLAG, strDEMANDDAY)
                        Else
                            '년월 정리
                            If Len(GetElement(vntData, "YEARMON", intColCnt, i, OPTIONAL_STR)) <> 6 Then
                                strYEARMON = GetElement(vntData, "YEARMON", intColCnt, i, OPTIONAL_STR).Substring(0, 4) & GetElement(vntData, "YEARMON", intColCnt, i, OPTIONAL_STR).Substring(5, 2)
                            Else
                                strYEARMON = GetElement(vntData, "YEARMON", intColCnt, i, OPTIONAL_STR)
                            End If

                            If GetElement(vntData, "TRU_TAX_FLAG", intColCnt, i, OPTIONAL_STR) = "1" Then
                                strTRU_TAX_FLAG = "Y"
                            Else
                                strTRU_TAX_FLAG = "N"
                            End If

                            strDEMANDDAY = Trim(Replace(GetElement(vntData, "DEMANDDAY", intColCnt, i, OPTIONAL_STR), "-", ""))
                            '기존년월체크
                            If GetElement(vntData, "OLDYEARMON", intColCnt, i, OPTIONAL_STR) <> strYEARMON Then
                                strOLDYEARMON = GetElement(vntData, "OLDYEARMON", intColCnt, i, OPTIONAL_STR)
                                strOLDSEQ = GetElement(vntData, "OLDSEQ", intColCnt, i, NULL_NUM, True)
                                strSEQ = SelectRtn_SEQ(strYEARMON)
                                '바뀐년월로 신규저장
                                intRtn = InsertRtn_MD_INTERNET_MEDIUM(vntData, intColCnt, i, _
                                                                      strYEARMON, strSEQ, _
                                                                      strCAMPAIGN_CODE, strCLIENTCODE, strTIMCODE, strSUBSEQ, _
                                                                      strTBRDSTDATE, strTBRDEDDATE, _
                                                                      strDEPT_CD, strTRU_TAX_FLAG, strDEMANDDAY)

                                '기존 바뀌기전 청약데이터 삭제
                                strSQL = ""
                                strSQL = " DELETE FROM  MD_INTERNET_MEDIUM "
                                strSQL = strSQL & "  WHERE YEARMON = '" & strOLDYEARMON & "' "
                                strSQL = strSQL & "  AND SEQ = " & strOLDSEQ

                                .mobjSCGLSql.SQLDo(strSQL)
                            Else
                                strSEQ = GetElement(vntData, "SEQ", intColCnt, i, NULL_NUM, True)
                                '바뀐년월로 신규저장
                                intRtn = UpdateRtn_MD_INTERNET_MEDIUM(vntData, intColCnt, i, _
                                                                      strYEARMON, strSEQ, _
                                                                      strCAMPAIGN_CODE, strCLIENTCODE, strTIMCODE, strSUBSEQ, _
                                                                      strTBRDSTDATE, strTBRDEDDATE, _
                                                                      strDEPT_CD, strTRU_TAX_FLAG, strDEMANDDAY)
                            End If
                        End If
                    Next
                End If
                .mobjSCGLSql.SQLCommitTrans()
                Return intRows
            Catch err As Exception
                .mobjSCGLSql.SQLRollbackTrans()
                Throw RaiseSysErr(err, CLASS_NAME & ".ProcessRtn")
            Finally
                .mobjSCGLSql.SQLDisconnect()
                mobjceMD_INTERNET_MEDIUM.Dispose()
            End Try
        End With
    End Function

