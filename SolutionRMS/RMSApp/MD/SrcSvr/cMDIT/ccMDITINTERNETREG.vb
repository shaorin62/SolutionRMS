                    intRtn = DELETE_DIVAMT(strInfoXML, vntData)
                End If

                '트랜잭션Commit
                .mobjSCGLSql.SQLCommitTrans()
                Return intRtn
            Catch err As Exception
                '트랜잭션RollBack 및 오류 전송
                .mobjSCGLSql.SQLRollbackTrans()
                Throw RaiseSysErr(err, CLASS_NAME & ".ProcessRtn")
            Finally
                'Resource해제
                .mobjSCGLSql.SQLDisconnect()
            End Try
        End With
    End Function

    '============== 대대행사 실제 삭제부분
    Public Function DELETE_DIVAMT(ByVal strInfoXML As String, _
                                  ByVal vntData As Object) As Integer '데이터 INSERT/UPDATE

        Dim intRtn As Integer
        Dim i, intColCnt, intRows, intSEQ As Integer
        Dim dblID As Double '자동 ID 를사용할 때만 사용
        Dim strYEARMON As String
        Dim strSEQ As Double

        SetConfig(strInfoXML)
        With mobjSCGLConfig
            Try
                If IsArray(vntData) Then
                    '''사용할 Entity 개체생성(Config 정보를 넘겨생성)
                    mobjceMD_INTERNET_MEDIUM = New ceMD_INTERNET_MEDIUM(mobjSCGLConfig)
                    '''vntData의 컬럼수, 로우수를 변수입력
                    intColCnt = UBound(vntData, 1) : intRows = UBound(vntData, 2)
                    '''해당하는Row 만큼 Loop
                    For i = 1 To intRows
                        If GetElement(vntData, "CHK", intColCnt, i, OPTIONAL_STR) = 1 Then
                            strYEARMON = GetElement(vntData, "YEARMON", intColCnt, i, OPTIONAL_STR)
                            strSEQ = GetElement(vntData, "SEQ", intColCnt, i, OPTIONAL_STR)
                            intRtn = mobjceMD_INTERNET_MEDIUM.DeleteRtn_EXCUTION(strYEARMON, strSEQ)
                        End If
                    Next
                End If
                Return intRtn
            Catch err As Exception
                Throw RaiseSysErr(err, CLASS_NAME & ".strDETAIL_DIVAMT")
            Finally
                mobjceMD_INTERNET_MEDIUM.Dispose()
            End Try
        End With
    End Function

