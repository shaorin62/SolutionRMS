    ' =============== 청약 데이터 삭제
    Public Function DeleteRtn_AMT(ByVal strInfoXML As String, _
                                  ByVal strYEARMON As String, _
                                  ByVal strSEQ As Integer, _
                                  ByVal strCAMPAIGN_CODE As String) As Integer

        Dim intRtn, intRtn2, intRtn3 As Integer      'Return변수( 처리건수 또는 0 )

        SetConfig(strInfoXML)    '기본정보 Setting
        With mobjSCGLConfig    '기본정보 Config 개체
            Try
                ' 사용할Entity 개체생성(Config 정보를 넘겨생성)
                mobjceMD_POINTAD_AMT = New ceMD_POINTAD_AMT(mobjSCGLConfig)
                mobjceMD_POINTAD_OUT = New ceMD_POINTAD_OUT(mobjSCGLConfig)
                mobjceMD_POINTAD_KAKAO = New ceMD_POINTAD_KAKAO(mobjSCGLConfig)
                ' DB 접속 및 트랜잭션 시작
                .mobjSCGLSql.SQLConnect(.DBConnStr)
                .mobjSCGLSql.SQLBeginTrans()

                '청약데이터 삭제 
                intRtn = mobjceMD_POINTAD_AMT.DeleteDo(strYEARMON, strSEQ)
                '사내수익 삭제
                intRtn = mobjceMD_POINTAD_OUT.DeleteDo_ALL(strYEARMON, strCAMPAIGN_CODE)
                'KAKAO 내역 삭제
                intRtn = mobjceMD_POINTAD_KAKAO.DeleteDo_ALL(strYEARMON, strCAMPAIGN_CODE)


                ' 트랜잭션 Commit
                .mobjSCGLSql.SQLCommitTrans()
                Return intRtn
            Catch err As Exception
                '트랜잭션 RollBack 및 오류 전송
                .mobjSCGLSql.SQLRollbackTrans()
                Throw RaiseSysErr(err, CLASS_NAME & "DeleteRtn_AMT")
            Finally
                'DB접속 종료
                .mobjSCGLSql.SQLDisconnect()
                '사용한 Entity(개체Dispose)
                mobjceMD_POINTAD_AMT.Dispose()
                mobjceMD_POINTAD_OUT.Dispose()
                mobjceMD_POINTAD_KAKAO.Dispose()
            End Try
        End With
