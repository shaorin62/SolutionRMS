    Public Function SelectRtn_CLIENTCODE(ByVal strGUBUN As String, _
                                         ByVal strCLIENTCODE As String) As String

        Dim strSQL, strRtn As String
        With mobjSCGLConfig '기본정보 Config 개체
            Try
                If strGUBUN = "1" Then
                    strSQL = " SELECT HIGHCUSTCODE FROM SC_CUST_HDR"
                    strSQL = strSQL & " WHERE BUSINO IN ("
                    strSQL = strSQL & " 	SELECT BUSINO FROM SC_CUST_HDR"
                    strSQL = strSQL & " 	WHERE HIGHCUSTCODE = '" & strCLIENTCODE & "'"
                    strSQL = strSQL & " )"
                    strSQL = strSQL & " AND USE_FLAG = '1'"
                    strSQL = strSQL & " AND MEDFLAG = 'A'"
                Else
                    strSQL = " SELECT HIGHCUSTCODE FROM SC_CUST_HDR"
                    strSQL = strSQL & " WHERE BUSINO IN ("
                    strSQL = strSQL & " 	SELECT BUSINO FROM SC_CUST_HDR"
                    strSQL = strSQL & " 	WHERE HIGHCUSTCODE = '" & strCLIENTCODE & "'"
                    strSQL = strSQL & " ) "
                    strSQL = strSQL & " AND USE_FLAG = '1' "
                    strSQL = strSQL & " AND MEDFLAG = 'G' "

                End If

                strRtn = .mobjSCGLSql.SQLSelectOneScalar(strSQL)
                Return strRtn

            Catch err As Exception
                ' 오류 전송
                Throw RaiseSysErr(err, CLASS_NAME & ".SelectRtn_CLIENTCODE")
            Finally
            End Try
        End With
    End Function

