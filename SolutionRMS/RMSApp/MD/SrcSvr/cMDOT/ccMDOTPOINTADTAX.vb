    '상세페이지의 부가세/적요 수정시 시트 부가세 수정시 dtl 변경금액이 hdr 에 합산
    Public Function ProcessRtn_VAT(ByVal strInfoXML As String, _
                                   ByVal vntData As Object, _
                                   ByVal strTAXYEARMON As String, _
                                   ByVal intTAXNO As Integer, _
                                   ByVal strSUMM As String) As Object

        Dim intRtn As Integer
        Dim intRtnTran As Integer
        Dim i, intColCnt, intRows As Integer
        Dim intTAXNOSEQ
        Dim lngVAT
        Dim lngSUMVAT

        Dim strTRANSSUM
        Dim strTRANSYEARMON
        Dim strTRANSNO
        Dim intTRANSSEQ
        Dim intLEN
        Dim lngTRANSVAT

        SetConfig(strInfoXML)
        With mobjSCGLConfig
            Try
                .mobjSCGLSql.SQLConnect(.DBConnStr)
                .mobjSCGLSql.SQLBeginTrans()

                If IsArray(vntData) Then
                    '''사용할 Entity 개체생성(Config 정보를 넘겨생성)
                    mobjceMD_COMMITAX_HDR = New ceMD_COMMITAX_HDR(mobjSCGLConfig)
                    mobjceMD_COMMITAX_DTL = New ceMD_COMMITAX_DTL(mobjSCGLConfig)
                    mobjceMD_POINTADCOMMI_HDR = New ceMD_POINTADCOMMI_HDR(mobjSCGLConfig)
                    mobjceMD_POINTADCOMMI_DTL = New ceMD_POINTADCOMMI_DTL(mobjSCGLConfig)

                    '''vntData의 컬럼수, 로우수를 변수입력
                    intColCnt = UBound(vntData, 1) : intRows = UBound(vntData, 2)
                    lngSUMVAT = 0

                    For i = 1 To intRows
                        intTAXNOSEQ = ""
                        lngVAT = 0

                        intTAXNOSEQ = GetElement(vntData, "TAXNOSEQ", intColCnt, i, NULL_NUM, True)
                        lngVAT = GetElement(vntData, "VAT", intColCnt, i, NULL_NUM, True)

                        '거래명세서 업데이트를 위해 거래명세서 번호 조회
                        strTRANSSUM = SelectRtn_transno(strTAXYEARMON, intTAXNO, intTAXNOSEQ)

                        '거래명세서 번호를 거래년월, 거래명세서번호, 순번 으로 나누기
                        intLEN = Len(strTRANSSUM)
                        strTRANSYEARMON = Mid(strTRANSSUM, 1, 6)
                        strTRANSNO = Mid(strTRANSSUM, InStr(1, strTRANSSUM, "-") + 1, (InStr(8, strTRANSSUM, "-")) - (InStr(1, strTRANSSUM, "-") + 1))
                        intTRANSSEQ = Mid(strTRANSSUM, InStr(8, strTRANSSUM, "-") + 1, intLEN)

                        '세금계산서 DTL 업데이트
                        intRtn = mobjceMD_COMMITAX_DTL.Update_Vat(strTAXYEARMON, intTAXNO, intTAXNOSEQ, lngVAT)
                        '거래명세서 DTL 업데이트
                        intRtnTran = mobjceMD_POINTADCOMMI_DTL.Update_Vat(strTRANSYEARMON, strTRANSNO, intTRANSSEQ, lngVAT)

                        '세금계산서 부가세 합산
                        lngSUMVAT = lngSUMVAT + lngVAT

                    Next

                    '변경된 세부내역의 부가세의 합을 세금계산서 헤더에 합산 Update
                    intRtn = mobjceMD_COMMITAX_HDR.SUMMUpdateDo(strTAXYEARMON, intTAXNO, strSUMM, lngSUMVAT)

                    '거래명세서 헤더 VAT업데이트를 위해 거래명세서 DTL 금액 조회
                    lngTRANSVAT = SelectRtn_TRANSVAT(strTRANSYEARMON, strTRANSNO)

                    '변경된 세부내역의 부가세의 합을 거래명세서 헤더에 합산 Update
                    intRtnTran = mobjceMD_POINTADCOMMI_HDR.Update_VatHDR(strTRANSYEARMON, strTRANSNO, lngTRANSVAT)
                End If

                .mobjSCGLSql.SQLCommitTrans()
                Return intRows
            Catch err As Exception
                .mobjSCGLSql.SQLRollbackTrans()
                Throw RaiseSysErr(err, CLASS_NAME & ".ProcessRtn_VAT")
            Finally
                .mobjSCGLSql.SQLDisconnect()
                mobjceMD_COMMITAX_HDR.Dispose()
                mobjceMD_COMMITAX_DTL.Dispose()
