    ' =============== SelectRtn 생성조회 [거래명세서 기초데이터]
    Public Function Get_OUTDOOR_TAXBUILD(ByVal strInfoXML As String, _
                                      ByRef intRowCnt As Integer, _
                                      ByRef intColCnt As Integer, _
                                      ByVal strYEARMON As String, _
                                      ByVal strTRANSNO As String, _
                                      ByVal strCLIENTCODE As String, _
                                      ByVal strGUBUN As String) As Object
        Dim strSQL As String            'SQL문
        Dim strFormat As String         '임시 SQL문
        Dim strSelFields As String      '조회필드
        Dim strWhere As String
        Dim vntData As Object
        Dim Con1 As String
        Dim Con2 As String
        Dim Con3 As String

        SetConfig(strInfoXML)   '기본정보 설정
        With mobjSCGLConfig

            Con1 = ""
            Con2 = ""
            Con3 = ""
            If strYEARMON <> "" Then Con1 = String.Format(" AND (TRANSYEARMON = '{0}')", strYEARMON)
            If strTRANSNO <> "" Then Con2 = String.Format(" AND (TRANSNO = '{0}')", strTRANSNO)
            If strCLIENTCODE <> "" Then Con3 = String.Format(" AND (REAL_MED_CODE like '%{0}%')", strCLIENTCODE)

            strWhere = BuildFields(" ", Con1, Con2, Con3)
            strFormat = " SELECT "
            strFormat = strFormat & " 0 CHK,"
            strFormat = strFormat & "  Case TAXNO"
            strFormat = strFormat & "  WHEN NULL THEN ''"
            strFormat = strFormat & "  ELSE TAXYEARMON +'-'+CAST(TAXNO AS NVARCHAR(10))"
            strFormat = strFormat & "  END AS TAXNO,"

            strFormat = strFormat & " '' TAXMANAGE,"
