    '============== ProcessRtn 분할생성
    Public Function ProcessRtn_Div(ByVal strInfoXML As String, _
                                   ByVal vntData As Object, _
                                   ByVal intTAXNO As Integer) As Integer

        Dim intRtn, intRtn1, intRtn2, intRtn3 As Integer '결과값 변수
        Dim i, intColCnt, intRows As Integer '루프, 컬럼Cnt, 로우Cnt 변수
        Dim intTAXNOSEQ As Integer
        Dim strDEMANDDAY As String
        Dim strPRINTDAY As String
        Dim strTAXYEARMON As String
        Dim strSUMM As String
        Dim strTRANSYEARMON As String
        Dim lngTRANSNO As Double
        Dim lngSEQ As Double
        Dim strTAXNO As String
        Dim strTRANSNO As String
        Dim lngAMT As Double
        Dim lngSUSURATE As Double
        Dim lngTOTAL As Double
        Dim strCLIENTCODE As String
        Dim strMEDFLAG
        SetConfig(strInfoXML) '기본정보 Setting
        With mobjSCGLConfig '기본정보를 가지고 있는 Config 개체
            Try

                .mobjSCGLSql.SQLConnect(xConnString)
                .mobjSCGLSql.SQLBeginTrans()
                mobjceMD_COMMITAX_HDR = New ceMD_COMMITAX_HDR(mobjSCGLConfig)
                mobjceMD_COMMITAX_DTL = New ceMD_COMMITAX_DTL(mobjSCGLConfig)
                mobjceMD_OUTDOOR_MEDIUM = New eMDCO.ceMD_OUTDOOR_MEDIUM(mobjSCGLConfig)
                mobjceMD_OUTDOORCOMMI_DTL = New eMDCO.ceMD_OUTDOORCOMMI_DTL(mobjSCGLConfig)
                intColCnt = UBound(vntData, 1) : intRows = UBound(vntData, 2)
                '''해당하는Row 만큼 Loop
                For i = 1 To intRows
                    ''' FLAG 조사 - INSERT/UPDATE 조사
                    If GetElement(vntData, "CHK", intColCnt, i, OPTIONAL_STR) = 1 Then
                        strTRANSYEARMON = GetElement(vntData, "TRANSYEARMON", intColCnt, i, OPTIONAL_STR)
                        lngTRANSNO = GetElement(vntData, "TRANSNO", intColCnt, i, OPTIONAL_STR)
                        lngSEQ = GetElement(vntData, "SEQ", intColCnt, i, OPTIONAL_STR)
                        strTRANSNO = strTRANSYEARMON & "-" & lngTRANSNO & "-" & lngSEQ
                        strDEMANDDAY = GetElement(vntData, "DEMANDDAY", intColCnt, i, OPTIONAL_STR).Substring(0, 4) & GetElement(vntData, "DEMANDDAY", intColCnt, i, OPTIONAL_STR).Substring(5, 2) & GetElement(vntData, "DEMANDDAY", intColCnt, i, OPTIONAL_STR).Substring(8, 2)
                        strPRINTDAY = GetElement(vntData, "PRINTDAY", intColCnt, i, OPTIONAL_STR).Substring(0, 4) & GetElement(vntData, "PRINTDAY", intColCnt, i, OPTIONAL_STR).Substring(5, 2) & GetElement(vntData, "PRINTDAY", intColCnt, i, OPTIONAL_STR).Substring(8, 2)
                        strTAXYEARMON = GetElement(vntData, "TAXYEARMON", intColCnt, i, OPTIONAL_STR)
                        strSUMM = GetElement(vntData, "SUMM", intColCnt, i, OPTIONAL_STR)
                        intTAXNO = SelectRtn_COMMITAXNO(strTAXYEARMON)
                        strTAXNO = strTAXYEARMON & "-" & intTAXNO
                        lngAMT = GetElement(vntData, "SUSU", intColCnt, i, NULL_NUM, True)
                        lngSUSURATE = GetElement(vntData, "SUSURATE", intColCnt, i, NULL_NUM, True)
                        lngTOTAL = GetElement(vntData, "AMT", intColCnt, i, NULL_NUM, True)
                        strCLIENTCODE = GetElement(vntData, "CLIENTCODE", intColCnt, i, OPTIONAL_STR)
                        strMEDFLAG = GetElement(vntData, "MEDFLAG", intColCnt, i, OPTIONAL_STR)
                        '헤더저장    
                        intRtn = DIVInsertRtn_MD_COMMITAX_HDR(vntData, intColCnt, i, strTAXYEARMON, intTAXNO, strDEMANDDAY, strPRINTDAY, strSUMM, lngAMT, lngSUSURATE, lngTOTAL, strMEDFLAG)

                        '디테일저장
                        intRtn1 = DIVInsertRtn_MD_COMMITAX_DTL(vntData, intColCnt, i, strTAXYEARMON, intTAXNO, strTRANSNO, lngSUSURATE, lngAMT, lngTOTAL, strCLIENTCODE, strMEDFLAG)

                        '거래명세서 디테일의 세금계산서번호 저장
                        intRtn2 = mobjceMD_OUTDOORCOMMI_DTL.Update_CommiTax(strTRANSYEARMON, lngTRANSNO, lngSEQ, strTAXYEARMON, intTAXNO)

                        '신탁의 세금계산서번호 저장
                        intRtn3 = mobjceMD_OUTDOOR_MEDIUM.Update_CommiTax(strTRANSNO, strTAXNO)
                    End If
                Next
                .mobjSCGLSql.SQLCommitTrans()
                Return intRtn
            Catch err As Exception
                .mobjSCGLSql.SQLRollbackTrans()
                Throw RaiseSysErr(err, CLASS_NAME & ".ProcessRtn_Div")
            Finally
                'Resource해제
                mobjceMD_COMMITAX_HDR.Dispose()
                mobjceMD_COMMITAX_DTL.Dispose()
                mobjceMD_OUTDOOR_MEDIUM.Dispose()
                mobjceMD_OUTDOORCOMMI_DTL.Dispose()
                .mobjSCGLSql.SQLDisconnect()
            End Try
        End With

    End Function


    '============== ProcessRtn 합산생성
    Public Function ProcessRtn_Group(ByVal strInfoXML As String, _
                                     ByVal vntData As Object, _
                                     ByVal intTAXNO As Integer, _
                                     ByVal intColFlag As Integer) As Integer
        Dim intRtn, intRtn1, intRtn2, intRtn3 As Integer '결과값 변수
        Dim i, intColCnt, intRows As Integer '루프, 컬럼Cnt, 로우Cnt 변수
        Dim intTAXNOSEQ As Integer
        Dim strDEMANDDAY As String
        Dim strPRINTDAY As String
        Dim strTAXYEARMON As String
        Dim strSUMM As String
        Dim strTRANSYEARMON As String
        Dim lngTRANSNO As Double
        Dim lngSEQ As Double
        Dim strTAXNO As String
        Dim strTRANSNO As String
        Dim intCnt As Integer
        Dim intCnt2 As Integer

        '합계 변수
        Dim lngAMT As Double
        Dim lngSUMAMT As Double
        Dim lngVAT As Double
        Dim lngSUMVAT As Double
        Dim lngVALUE As Double
        Dim lngSUMVALUE As Double

        '헤더 변수
        Dim strCLIENTCODE, strCLIENTACCCODE, strREAL_MED_CODE, strREAL_MED_ACCODE, strDEPTCODE, strATTR01, strATTR02, strATTR03 As String
        Dim lngSUSURATESUM, lngTOTALAMT As Double
        Dim lngSUSRATEEA, lngTOTALEA As Double
        '디테일변수
        Dim strSUSURATE As String
        Dim lngAMTDETAIL As Double
        Dim lngTOTALDETAIL As Double
        SetConfig(strInfoXML) '기본정보 Setting
        With mobjSCGLConfig '기본정보를 가지고 있는 Config 개체
            Try
                Dim xmlRoot As XmlElement

                .mobjSCGLSql.SQLConnect(xConnString)
                .mobjSCGLSql.SQLBeginTrans()

                mobjceMD_COMMITAX_HDR = New ceMD_COMMITAX_HDR(mobjSCGLConfig)
                mobjceMD_COMMITAX_DTL = New ceMD_COMMITAX_DTL(mobjSCGLConfig)
                mobjceMD_OUTDOOR_MEDIUM = New eMDCO.ceMD_OUTDOOR_MEDIUM(mobjSCGLConfig)
                mobjceMD_OUTDOORCOMMI_DTL = New eMDCO.ceMD_OUTDOORCOMMI_DTL(mobjSCGLConfig)

                intColCnt = UBound(vntData, 1) : intRows = UBound(vntData, 2)
                For intCnt = 1 To intColFlag
                    intCnt2 = 0
                    If IsArray(vntData) Then
                        intColCnt = UBound(vntData, 1) : intRows = UBound(vntData, 2)
                        '''해당하는Row 만큼 Loop
                        lngAMT = 0
                        lngSUMAMT = 0
                        lngVAT = 0
                        lngSUMVAT = 0
                        lngVALUE = 0
                        lngSUMVALUE = 0
                        lngTOTALEA = 0
                        lngTOTALAMT = 0
                        '쉬트 내 헤더값 검출
                        For i = 1 To intRows
                            '청구합계
                            If GetElement(vntData, "CHK", intColCnt, i, OPTIONAL_STR) = 1 And GetElement(vntData, "RANKTRANS", intColCnt, i, OPTIONAL_STR) = intCnt Then
                                lngAMT = GetElement(vntData, "SUSU", intColCnt, i, OPTIONAL_STR)
                                lngTOTALEA = GetElement(vntData, "AMT", intColCnt, i, OPTIONAL_STR)
                                lngSUMAMT = lngSUMAMT + lngAMT
                                lngTOTALAMT = lngTOTALAMT + lngTOTALEA
                                'lngVAT = gRound((lngAMT / 10), 0)


                                '계산서년월
                                strTAXYEARMON = GetElement(vntData, "TAXYEARMON", intColCnt, i, OPTIONAL_STR)
                                '계산서번호
                                intTAXNO = SelectRtn_COMMITAXNO(strTAXYEARMON)
                                '헤더정보

                                strDEMANDDAY = GetElement(vntData, "DEMANDDAY", intColCnt, i, OPTIONAL_STR).Substring(0, 4) & GetElement(vntData, "DEMANDDAY", intColCnt, i, OPTIONAL_STR).Substring(5, 2) & GetElement(vntData, "DEMANDDAY", intColCnt, i, OPTIONAL_STR).Substring(8, 2)
                                strPRINTDAY = GetElement(vntData, "PRINTDAY", intColCnt, i, OPTIONAL_STR).Substring(0, 4) & GetElement(vntData, "PRINTDAY", intColCnt, i, OPTIONAL_STR).Substring(5, 2) & GetElement(vntData, "PRINTDAY", intColCnt, i, OPTIONAL_STR).Substring(8, 2)
                                strSUMM = GetElement(vntData, "SUMM", intColCnt, i, OPTIONAL_STR)
                            End If
                        Next

                        lngSUMVAT = Int(lngSUMAMT / 10)

                        If lngTOTALAMT <> "0.0" Or lngTOTALAMT <> "0" Then
                            lngSUSURATESUM = gRound(((lngSUMAMT / lngTOTALAMT) * 100), 2)
                        Else
                            lngSUSURATESUM = 0
                        End If

                        '검출된 헤더값 실제저장
                        For i = 1 To intRows
