            Con3 = ""
            Con4 = ""
            If strYEARMON <> "" Then Con1 = String.Format(" AND (TAXYEARMON = '{0}')", strYEARMON)
            If strCLIENTCODE <> "" Then Con2 = String.Format(" AND (REAL_MED_CODE like '%{0}%')", strCLIENTCODE)
