                                    strSUMM = GetElement(vntData, "SUMM", intColCnt, i, OPTIONAL_STR)
                                    strTAXNO = strTAXYEARMON & "-" & intTAXNO
                                    intTAXNOSEQ = SelectRtn_COMMITAXNOSEQ(strTAXYEARMON, intTAXNO)
                                    '취급고
                                    lngTOTALDETAIL = GetElement(vntData, "AMT", intColCnt, i, OPTIONAL_STR)
                                    '수수료율
                                    strSUSURATE = GetElement(vntData, "SUSURATE", intColCnt, i, OPTIONAL_STR)
                                    '수수료
                                    lngAMTDETAIL = GetElement(vntData, "SUSU", intColCnt, i, OPTIONAL_STR)
                                    '광고주코드
                                    strCLIENTCODE = GetElement(vntData, "CLIENTCODE", intColCnt, i, OPTIONAL_STR)
                                    '디테일저장
                                    intRtn1 = InsertRtn_COMMITAX_DTL(vntData, intColCnt, i, strTAXYEARMON, intTAXNO, intTAXNOSEQ, strTRANSNO, strSUSURATE, lngAMTDETAIL, lngTOTALDETAIL, strCLIENTCODE)

                                    '거래명세서 디테일의 세금계산서번호 저장
                                    intRtn2 = mobjceMD_OUTDOORCOMMI_DTL.Update_CommiTax(strTRANSYEARMON, lngTRANSNO, lngSEQ, strTAXYEARMON, intTAXNO)

                                    '신탁의 세금계산서번호 저장
                                    intRtn3 = mobjceMD_OUTDOOR_MEDIUM.Update_CommiTax(strTRANSNO, strTAXNO)
                                End If

                            End If
                        Next
                    End If
                Next


                .mobjSCGLSql.SQLCommitTrans()
                Return intRtn
            Catch err As Exception
                .mobjSCGLSql.SQLRollbackTrans()
                Throw RaiseSysErr(err, CLASS_NAME & ".ProcessRtn_Group")
            Finally
                'Resource해제
                mobjceMD_COMMITAX_HDR.Dispose()
                mobjceMD_COMMITAX_DTL.Dispose()
                mobjceMD_OUTDOOR_MEDIUM.Dispose()
                mobjceMD_OUTDOORCOMMI_DTL.Dispose()
                .mobjSCGLSql.SQLDisconnect()
            End Try
        End With
    End Function


    '생성된 세금계산서 삭제
    Public Function DeleteRtn_CommiTax(ByVal strInfoXML As String, _
                                      ByVal strTAXYEARMON As String, _
                                      ByVal intTAXNO As Integer) As Integer
        Dim intRtn As Integer      'Return변수( 처리건수 또는 0 )
        Dim strTAXNO As String

        SetConfig(strInfoXML)    '기본정보 Setting
        With mobjSCGLConfig    '기본정보 Config 개체
            Try
                ' 사용할Entity 개체생성(Config 정보를 넘겨생성)
                strTAXNO = strTAXYEARMON & "-" & CStr(intTAXNO)
                mobjceMD_COMMITAX_HDR = New ceMD_COMMITAX_HDR(mobjSCGLConfig)
                mobjceMD_COMMITAX_DTL = New ceMD_COMMITAX_DTL(mobjSCGLConfig)
                mobjceMD_OUTDOOR_MEDIUM = New eMDCO.ceMD_OUTDOOR_MEDIUM(mobjSCGLConfig)
                mobjceMD_OUTDOORCOMMI_DTL = New eMDCO.ceMD_OUTDOORCOMMI_DTL(mobjSCGLConfig)
                ' DB 접속 및 트랜잭션 시작
                .mobjSCGLSql.SQLConnect(xConnString)
                .mobjSCGLSql.SQLBeginTrans()
                ' 엔티티 오브젝트의 Delete 메소드 호출
                '거래명세서의 세금계산서년월,번호 삭제
                intRtn = mobjceMD_OUTDOORCOMMI_DTL.CommiTaxDeleteUpdateDo(strTAXYEARMON, intTAXNO)
                '신탁테이블의 세금계산서 번호 삭제
                intRtn = mobjceMD_OUTDOOR_MEDIUM.CommiTaxDeleteUpdateDo(strTAXNO)
                '헤더 삭제
                intRtn = mobjceMD_COMMITAX_HDR.DeleteDo(strTAXYEARMON, intTAXNO)
                '디테일 삭제
                intRtn = mobjceMD_COMMITAX_DTL.DeleteDo(strTAXYEARMON, intTAXNO)

                ' 트랜잭션 Commit
                .mobjSCGLSql.SQLCommitTrans()
                Return intRtn
            Catch err As Exception
                .mobjSCGLSql.SQLRollbackTrans()
                Throw RaiseSysErr(err, CLASS_NAME & "DeleteRtn_CommiTax")
            Finally
                'DB접속 종료
                .mobjSCGLSql.SQLDisconnect()
                '사용한 Entity(개체Dispose)
                mobjceMD_COMMITAX_HDR.Dispose()
                mobjceMD_COMMITAX_DTL.Dispose()
                mobjceMD_OUTDOOR_MEDIUM.Dispose()
                mobjceMD_OUTDOORCOMMI_DTL.Dispose()
            End Try
        End With

    End Function


