    ' =============== SelectRtnSample Code
    Public Function SelectRtn(ByVal strInfoXML As String, _
                              ByRef intRowCnt As Integer, _
                              ByRef intColCnt As Integer, _
                              ByVal strYEARMON As String, _
                              ByVal strCLIENTCODE As String, _
                              ByVal strCLIENTNAME As String, _
                              ByVal strREAL_MED_CODE As String, _
                              ByVal strREAL_MED_NAME As String, _
                              ByVal strTIMCODE As String, _
                              ByVal strTIMNAME As String, _
                              ByVal strTITLE As String, _
                              ByVal strGUBUN As String) As Object     'XML  데이터 조회시

        Dim strSQL As String
        Dim strFormet, strWhere As String
        Dim Con1, Con2, Con3, Con4, Con5, Con6, Con7, Con8, Con9 As String
        Dim vntData As Object

        With mobjSCGLConfig
            Try
                SetConfig(strInfoXML)   '기본정보 설정

                Con1 = "" : Con2 = "" : Con3 = ""
                Con4 = "" : Con5 = "" : Con6 = ""
                Con7 = "" : Con8 = "" : Con9 = ""

                If strYEARMON <> "" Then Con1 = String.Format(" AND (YEARMON = '{0}')", strYEARMON)
                If strCLIENTCODE <> "" Then Con2 = String.Format(" AND (CLIENTCODE = '{0}')", strCLIENTCODE)
                If strCLIENTNAME <> "" Then Con3 = String.Format(" AND (DBO.SC_GET_HIGHCUSTNAME_FUN(CLIENTCODE) LIKE '%{0}%')", strCLIENTNAME) '광고주명
                If strREAL_MED_CODE <> "" Then Con4 = String.Format(" AND (REAL_MED_CODE = '{0}')", strREAL_MED_CODE)
                If strREAL_MED_NAME <> "" Then Con5 = String.Format(" AND (DBO.SC_GET_HIGHCUSTNAME_FUN(REAL_MED_CODE) LIKE '%{0}%')", strREAL_MED_NAME) '매체사명
                If strTIMCODE <> "" Then Con6 = String.Format(" AND (TIMCODE = '{0}')", strTIMCODE)
                If strTIMNAME <> "" Then Con7 = String.Format(" AND (DBO.SC_GET_CUSTNAME_FUN(TIMCODE) LIKE '%{0}%')", strTIMNAME) '팀명
                If strTITLE <> "" Then Con8 = String.Format(" AND (TITLE = '{0}')", strTITLE)

                strWhere = BuildFields(" ", Con1, Con2, Con3, Con4, Con5, Con6, Con7, Con8)

                strFormet = ""

                If strGUBUN = "" Or strGUBUN = "Y" Then
                    strFormet = strFormet & " SELECT "
                    strFormet = strFormet & " 0 CHK, YEARMON, SEQ, '승인' GUBUN,"
                    strFormet = strFormet & " CLIENTCODE, DBO.SC_GET_HIGHCUSTNAME_FUN(CLIENTCODE) CLIENTNAME,"
                    strFormet = strFormet & " TIMCODE, DBO.SC_GET_CUSTNAME_FUN(TIMCODE) TIMNAME,"
                    strFormet = strFormet & " REAL_MED_CODE, DBO.SC_GET_HIGHCUSTNAME_FUN(REAL_MED_CODE) REAL_MED_NAME,"
                    strFormet = strFormet & " MEDCODE, DBO.SC_GET_HIGHCUSTNAME_FUN(MEDCODE) MEDNAME,"
                    strFormet = strFormet & " DBO.SC_GET_BUSINO_FUN(REAL_MED_CODE) REAL_MED_BISNO,"
                    strFormet = strFormet & " DEPT_CD,"
                    strFormet = strFormet & " DBO.SC_DEPT_NAME_FUN(DEPT_CD) DEPT_NAME,"
                    strFormet = strFormet & " MED_FLAG, HIGHSUBSEQ, DBO.SC_HIGHSEQNAME_FUN(HIGHSUBSEQ) SUBSEQNAME, "
                    strFormet = strFormet & " DEMANDDAY, TBRDSTDATE, TBRDEDDATE,"
                    strFormet = strFormet & " GBN_FLAG, TITLE, MATTERNAME, TOTALAMT, AMT, OUT_AMT, COMMI_RATE, COMMISSION,"
                    strFormet = strFormet & " MED_GBN, LOCATION, MEMO, "
                    strFormet = strFormet & " CONTIDX, MDIDX, CYEAR, CMONTH, SIDE, CASE ISNULL(PORTAL_SEQ,'') WHEN '1' THEN 'AOR' ELSE '' END PORTAL_SEQ, CASE ISNULL(COMMI_TAX_FLAG,'N') WHEN 'N' THEN 0 ELSE 1 END COMMI_TAX_FLAG, COMMI_TRANS_NO, "
                    strFormet = strFormet & " CASE ISNULL(ATTR01,'') WHEN '' THEN YEARMON + '-' + CAST(SEQ AS VARCHAR(10)) "
                    strFormet = strFormet & " ELSE ATTR01 END AS ATTR01 "
                    strFormet = strFormet & " FROM MD_OUTDOOR_MEDIUM"
                    strFormet = strFormet & " WHERE 1=1 {0}"

                    If strGUBUN = "" Then
                        strFormet = strFormet & " UNION ALL"
                    End If
                End If

                If strGUBUN = "" Or strGUBUN = "N" Then
                    strFormet = strFormet & " SELECT "
                    strFormet = strFormet & " 0 CHK, YEARMON, null SEQ, '미승인' GUBUN,"
                    strFormet = strFormet & " CLIENTCODE, DBO.SC_GET_HIGHCUSTNAME_FUN(CLIENTCODE) CLIENTNAME,"
                    strFormet = strFormet & " TIMCODE, DBO.SC_GET_CUSTNAME_FUN(TIMCODE) TIMNAME,"
                    strFormet = strFormet & " REAL_MED_CODE, DBO.SC_GET_HIGHCUSTNAME_FUN(REAL_MED_CODE) REAL_MED_NAME,"
                    strFormet = strFormet & " MEDCODE, DBO.SC_GET_HIGHCUSTNAME_FUN(MEDCODE) MEDNAME,"
                    strFormet = strFormet & "  DBO.SC_GET_BUSINO_FUN(REAL_MED_CODE) REAL_MED_BISNO,"
                    strFormet = strFormet & " CASE DBO.MD_GET_OUTDOORDEPTCD_FUN(CLIENTCODE,TIMCODE) WHEN '' THEN '00006145' ELSE DBO.MD_GET_OUTDOORDEPTCD_FUN(CLIENTCODE,TIMCODE) END DEPT_CD,"
                    strFormet = strFormet & " CASE DBO.MD_GET_OUTDOORDEPTNAME_FUN(CLIENTCODE,TIMCODE) WHEN '' THEN 'OOH미디어팀' ELSE DBO.MD_GET_OUTDOORDEPTNAME_FUN(CLIENTCODE,TIMCODE) END DEPT_NAME,"
                    strFormet = strFormet & " 'D' MED_FLAG, HIGHSUBSEQ, DBO.SC_HIGHSEQNAME_FUN(HIGHSUBSEQ) SUBSEQNAME, "
                    strFormet = strFormet & " convert(varchar(8),dateadd(d,-1, dateadd(m,1,yearmon+'01')),112) DEMANDDAY, TBRDSTDATE, TBRDEDDATE,"
                    strFormet = strFormet & " GBN_FLAG, TITLE, MATTERNAME, TOTALAMT, AMT, OUT_AMT, COMMI_RATE, COMMISSION,"
                    strFormet = strFormet & " MED_GBN, LOCATION, MEMO, "
                    strFormet = strFormet & " CONTIDX, MDIDX, CYEAR, CMONTH, SIDE, '' PORTAL_SEQ, 1 COMMI_TAX_FLAG, '' COMMI_TRANS_NO, '' ATTR01 "
                    strFormet = strFormet & " FROM VW_OUTDOOR_DATA"
                    strFormet = strFormet & " WHERE 1=1 {0}"
                    strFormet = strFormet & " AND ISNULL(GFLAG,'') = 'N' AND (ISNULL(AMT,0) <> 0 or ISNULL(COMMISSION,0) <> 0 or  ISNULL(OUT_AMT,0) <> 0) "
                End If

                strFormet = strFormet & " order by GUBUN DESC, DBO.SC_GET_HIGHCUSTNAME_FUN(CLIENTCODE), DBO.SC_GET_CUSTNAME_FUN(TIMCODE), TITLE  "


                strSQL = String.Format(strFormet, strWhere)

                .mobjSCGLSql.SQLConnect(.DBConnStr)
                vntData = .mobjSCGLSql.SQLSelectArr(strSQL, intRowCnt, intColCnt, , True)
                Return vntData
            Catch err As Exception
                Throw RaiseSysErr(err, CLASS_NAME & ".SelectRtn")
            Finally
                .mobjSCGLSql.SQLDisconnect()
            End Try
        End With
    End Function

    '============== ProcessRtn_DETAIL_OK
    Public Function ProcessRtn_DETAIL_OK(ByVal vntData As Object) As String

        Dim intRtn, intRtn2 As Integer
        Dim i, intColCnt, intRows As Integer
        Dim strTBRDSTDATE
        Dim strTBRDEDDATE
        Dim strDEMANDDAY
        Dim strYEARMON
        Dim strSEQ
        Dim strCOMMI_TAX_FLAG
        Dim strMEMO, strGBN_FLAG
        Dim strCONTIDX, strCYEAR, strCMONTH, strMEDCODE

        With mobjSCGLConfig
            Try
                If IsArray(vntData) Then
                    '''사용할 Entity 개체생성(Config 정보를 넘겨생성)
                    mobjceMD_OUTDOOR_MEDIUM = New ceMD_OUTDOOR_MEDIUM(mobjSCGLConfig)
                    mobjceMD_OUTDOOR_REPORT = New ceMD_OUTDOOR_REPORT(mobjSCGLConfig)
                    '''vntData의 컬럼수, 로우수를 변수입력
                    intColCnt = UBound(vntData, 1) : intRows = UBound(vntData, 2)
                    '''해당하는Row 만큼 Loop

                    For i = 1 To intRows
                        If GetElement(vntData, "CHK", intColCnt, i, OPTIONAL_STR) = "1" And GetElement(vntData, "GUBUN", intColCnt, i, OPTIONAL_STR) = "미승인" Then
                            If GetElement(vntData, "DEMANDDAY", intColCnt, i, OPTIONAL_STR) <> "" Then strDEMANDDAY = GetElement(vntData, "DEMANDDAY", intColCnt, i, OPTIONAL_STR).Substring(0, 4) & GetElement(vntData, "DEMANDDAY", intColCnt, i, OPTIONAL_STR).Substring(5, 2) & GetElement(vntData, "DEMANDDAY", intColCnt, i, OPTIONAL_STR).Substring(8, 2)
                            If GetElement(vntData, "TBRDSTDATE", intColCnt, i, OPTIONAL_STR) <> "" Then strTBRDSTDATE = GetElement(vntData, "TBRDSTDATE", intColCnt, i, OPTIONAL_STR).Substring(0, 4) & GetElement(vntData, "TBRDSTDATE", intColCnt, i, OPTIONAL_STR).Substring(5, 2) & GetElement(vntData, "TBRDSTDATE", intColCnt, i, OPTIONAL_STR).Substring(8, 2)
                            If GetElement(vntData, "TBRDEDDATE", intColCnt, i, OPTIONAL_STR) <> "" Then strTBRDEDDATE = GetElement(vntData, "TBRDEDDATE", intColCnt, i, OPTIONAL_STR).Substring(0, 4) & GetElement(vntData, "TBRDEDDATE", intColCnt, i, OPTIONAL_STR).Substring(5, 2) & GetElement(vntData, "TBRDEDDATE", intColCnt, i, OPTIONAL_STR).Substring(8, 2)
                            strYEARMON = GetElement(vntData, "YEARMON", intColCnt, i, OPTIONAL_STR)
                            strSEQ = SelectRtn_SEQ(strYEARMON)

                            If GetElement(vntData, "COMMI_TAX_FLAG", intColCnt, i, OPTIONAL_STR) = "1" Then
                                strCOMMI_TAX_FLAG = "Y"
                            Else
                                strCOMMI_TAX_FLAG = "N"
                            End If

                            intRtn = InsertRtn_MD_OUTDOOR_MEDIUM(vntData, intColCnt, i, strYEARMON, strSEQ, strDEMANDDAY, strTBRDSTDATE, strTBRDEDDATE, strCOMMI_TAX_FLAG)

                            '옥외 확정 리스트에도 함께 인서트 한다. 추후에 리포트나 보고서에 적용됨.
                            intRtn2 = InsertRtn_MD_OUTDOOR_REPORT(vntData, intColCnt, i, strYEARMON, strSEQ, strDEMANDDAY, strTBRDSTDATE, strTBRDEDDATE, strCOMMI_TAX_FLAG)

                            strCONTIDX = GetElement(vntData, "CONTIDX", intColCnt, i, OPTIONAL_STR)
                            strCYEAR = GetElement(vntData, "CYEAR", intColCnt, i, OPTIONAL_STR)
                            strCMONTH = GetElement(vntData, "CMONTH", intColCnt, i, OPTIONAL_STR)
                            strMEDCODE = GetElement(vntData, "MEDCODE", intColCnt, i, OPTIONAL_STR)

                            If strCONTIDX <> "" Then
                                intRtn = UpdateRtn_WB_CONTACT_EXE(strCONTIDX, strCYEAR, strCMONTH, strMEDCODE)
                            End If

                        ElseIf GetElement(vntData, "CHK", intColCnt, i, OPTIONAL_STR) = "1" And GetElement(vntData, "GUBUN", intColCnt, i, OPTIONAL_STR) = "승인" Then
                            strYEARMON = GetElement(vntData, "YEARMON", intColCnt, i, OPTIONAL_STR)
                            strSEQ = GetElement(vntData, "SEQ", intColCnt, i, OPTIONAL_STR)
                            strMEMO = GetElement(vntData, "MEMO", intColCnt, i, OPTIONAL_STR)
                            If GetElement(vntData, "COMMI_TAX_FLAG", intColCnt, i, OPTIONAL_STR) = "1" Then
                                strCOMMI_TAX_FLAG = "Y"
                            Else
                                strCOMMI_TAX_FLAG = "N"
                            End If
                            strGBN_FLAG = GetElement(vntData, "GBN_FLAG", intColCnt, i, OPTIONAL_STR)

                            intRtn = UpdateRtn_MD_OUTDOOR_MEDIUM(strYEARMON, strSEQ, strMEMO, strCOMMI_TAX_FLAG, strGBN_FLAG)

                            intRtn2 = UpdateRtn_MD_OUTDOOR_REPORT(strYEARMON, strSEQ, strMEMO, strCOMMI_TAX_FLAG, strGBN_FLAG)
                        End If

                    Next
                End If

                Return intRows
            Catch err As Exception
                Throw RaiseSysErr(err, CLASS_NAME & ".ProcessRtn_DETAIL_OK")
            Finally
                mobjceMD_OUTDOOR_MEDIUM.Dispose()
                mobjceMD_OUTDOOR_REPORT.Dispose()
            End Try
        End With
    End Function

