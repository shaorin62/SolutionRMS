            strFormat = strFormat & "  AND TAXNO IS NULL AND ISNULL(CONFIRMFLAG, 0) = 1"
            strFormat = strFormat & "  {0} "
            strFormat = strFormat & "  ORDER BY CLIENTBISNO, REAL_MED_BISNO, DEMANDDAY, AMT, TRANSNO"


