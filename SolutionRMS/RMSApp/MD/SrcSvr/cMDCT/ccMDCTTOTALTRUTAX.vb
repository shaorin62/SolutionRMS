            strFormat = strFormat & "  ISNULL(AMT,0) - ISNULL(DBO.MD_GET_COMMISSION_FUN(TRUST_YEARMON,TRUST_SEQ, MED_FLAG),0) GENERALAMT,"
            strFormat = strFormat & "  ISNULL(DBO.MD_GET_COMMISSION_FUN(TRUST_YEARMON,TRUST_SEQ, MED_FLAG),0) OUTLISTAMT,"
