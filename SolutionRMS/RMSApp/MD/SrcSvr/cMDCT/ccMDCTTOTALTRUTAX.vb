            strFormat = strFormat & "  DBO.SC_GET_ADDRESS2_FUN(REAL_MED_CODE) REAL_MEDADDR2,"
            strFormat = strFormat & "  DBO.MD_GET_MATTERNAME_FUN(MATTERCODE) MATTERNAME,"
            strFormat = strFormat & "  '' VOCHNO,"
            strFormat = strFormat & "  SUBSTRING(DEMANDDAY,1,6) TAXYEARMON,"
            strFormat = strFormat & "  NULL TAXNO,"
            strFormat = strFormat & "  TRANSYEARMON, TRANSNO, SEQ,"
            strFormat = strFormat & "  TRUST_YEARMON, TRUST_SEQ,"
            strFormat = strFormat & "  CLIENTCODE, TIMCODE, REAL_MED_CODE,"
            strFormat = strFormat & "  DBO.SC_GET_ACCUSTCODE_FUN(CLIENTCODE) CLIENTACCODE,"
