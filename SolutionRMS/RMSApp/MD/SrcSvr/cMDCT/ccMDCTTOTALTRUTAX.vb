            strFormat = strFormat & "  DBO.SC_GET_BUSINO_FUN(REAL_MED_CODE) REAL_MED_BISNO,"
            strFormat = strFormat & "  DBO.SC_GET_CUSTNAME_FUN(MEDCODE) MEDNAME, "
            strFormat = strFormat & "  ISNULL(AMT,0) AMT,"
            strFormat = strFormat & "  ISNULL(VAT,0) VAT,"
