            strFormat = strFormat & "  MEDCODE, DEPT_CD,"
            strFormat = strFormat & "  SUBSEQ, MATTERCODE,"
            strFormat = strFormat & "  'T' AS MED_FLAG,"

            If strGUBUN = "taxdiv" Then
                strFormat = strFormat & "  1 RANKTRANS"
            ElseIf strGUBUN = "taxgroup" Then
