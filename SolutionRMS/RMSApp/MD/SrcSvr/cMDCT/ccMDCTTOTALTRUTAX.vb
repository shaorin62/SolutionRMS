    Public Function Get_TOTAL_TAX(ByVal strInfoXML As String, _
                                 ByRef intRowCnt As Integer, _
                                 ByRef intColCnt As Integer, _
                                 ByVal strYEARMON As String, _
                                 ByVal strCLIENTCODE As String, _
                                 ByVal strFROM As String, _
                                 ByVal strTO As String) As Object

        Dim strSQL As String            'SQL문
        Dim strFormat As String         '임시 SQL문
        Dim strSelFields As String      '조회필드
        Dim strWhere As String
        Dim vntData As Object
        Dim Con1 As String
        Dim Con2 As String
        Dim Con3 As String
        Dim Con4 As String

        SetConfig(strInfoXML)   '기본정보 설정
        With mobjSCGLConfig

            Con1 = "" : Con2 = "" : Con3 = "" : Con4 = ""

            If strYEARMON <> "" Then Con1 = String.Format(" AND (TAXYEARMON = '{0}')", strYEARMON)
            If strCLIENTCODE <> "" Then Con2 = String.Format(" AND (CLIENTCODE = '{0}')", strCLIENTCODE)

            If strFROM <> "" And strTO <> "" Then
                Con3 = String.Format(" AND (DEMANDDAY BETWEEN '{0}' AND  '{1}')", strFROM, strTO)
            End If
            If strFROM <> "" And strTO = "" Then
                Con3 = String.Format(" AND (DEMANDDAY >= '{0}')", strFROM)
            End If
            If strFROM = "" And strTO <> "" Then
                Con3 = String.Format(" AND (DEMANDDAY <= '{0}')", strTO)
            End If


            strWhere = BuildFields(" ", Con1, Con2, Con3)

            strFormat = "  SELECT "
            strFormat = strFormat & "  CASE WHEN DBO.MD_TAXVOCH_TYPE_CHANGE_FUN(TAXYEARMON, TAXNO) >=1 THEN '상세' "
            strFormat = strFormat & "  ELSE VOCH_TYPE END  VOCH_TYPE_OLD,"
            strFormat = strFormat & "  VOCH_TYPE,"
            strFormat = strFormat & "  0 CHK,"
            strFormat = strFormat & "  TAXYEARMON + '-' + MJFLAG + '-' + dbo.LPAD(TAXNO,4,'0') TAXMANAGE,"
            strFormat = strFormat & "  DEMANDDAY,"
            strFormat = strFormat & "  CLIENTNAME,"
            strFormat = strFormat & "  CLIENTBISNO,"
            strFormat = strFormat & "  DBO.MD_TAXTIMNAME_FUN(TAXYEARMON, TAXNO) TIMNAME,"
            strFormat = strFormat & "  DBO.MD_TAXSUBSEQNAME_FUN(TAXYEARMON, TAXNO) SUBSEQNAME,"
            strFormat = strFormat & "  REAL_MED_NAME,"
            strFormat = strFormat & "  REAL_MED_BISNO,"
            strFormat = strFormat & "  DBO.MD_TAXMEDNAME_FUN(TAXYEARMON, TAXNO) MEDNAME,"
            strFormat = strFormat & "  ISNULL(AMT,0) AMT, ISNULL(VAT,0) VAT, "
            strFormat = strFormat & "  ISNULL(AMT,0)+ISNULL(VAT,0) SUMAMT, "
            strFormat = strFormat & "  SUMM, PRINTDAY, "
            strFormat = strFormat & "  DBO.SC_DEPT_NAME_FUN(DEPT_CD) DEPT_NAME, "
            strFormat = strFormat & "  CLIENTOWNER, CLIENTADDR1, CLIENTADDR2, "
            strFormat = strFormat & "  REAL_MEDOWNER, REAL_MEDADDR1, REAL_MEDADDR2,"
            strFormat = strFormat & "  VOCHNO, "
            strFormat = strFormat & "  TAXYEARMON, TAXNO, "
            strFormat = strFormat & "  MEDFLAG"
            strFormat = strFormat & "  FROM MD_TRUTAX_HDR"
            strFormat = strFormat & "  WHERE 1=1 AND ISNULL(ATTR10,0) <> 999999 AND MEDFLAG = 'T' "
            strFormat = strFormat & "  {0} "

            strSQL = String.Format(strFormat, strWhere)

            '데이터 조회
            Try
                .mobjSCGLSql.SQLConnect(.DBConnStr)
                vntData = .mobjSCGLSql.SQLSelectArr(strSQL, intRowCnt, intColCnt, , True)
                Return vntData
            Catch err As Exception
                Throw RaiseSysErr(err, CLASS_NAME & ".Get_TOTAL_TAX")
            Finally
                .mobjSCGLSql.SQLDisconnect()
            End Try
        End With

    End Function

