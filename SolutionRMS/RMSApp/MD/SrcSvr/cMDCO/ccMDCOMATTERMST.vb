    ' =============== SelectRtnSample Code
    Public Function SelectRtn(ByVal strInfoXML As String, _
                              ByRef intRowCnt As Integer, _
                              ByRef intColCnt As Integer, _
                              ByRef strCLIENTNAME As String, _
                              ByRef strCLIENTCODE As String, _
                              ByRef strTIMNAME As String, _
                              ByRef strTIMCODE As String, _
                              ByRef strSUBSEQNAME As String, _
                              ByRef strSUBSEQ As String, _
                              ByRef strEXCLIENTNAME As String, _
                              ByRef strEXCLIENTCODE As String, _
                              ByRef strMATTERNAME As String, _
                              ByRef strMEDFLAG As String, _
                              ByRef strUSE_YN As String) As Object

        Dim strCols As String        '컬럼변수
        Dim strWhere As String       'Where조건 변수
        Dim strFormat As String      'SQL Format 변수
        Dim strSQL As String         'SQL 변수
        Dim vntData As Object        'Array Return 변수(Array 를사용할 때 선언)
        Dim strSelFields
        Dim intCnt
        Dim intRtn
        Dim Con1, Con2, Con3, Con4, Con5, Con6, Con7, Con8, Con9, Con10, Con11
        SetConfig(strInfoXML)

        Con1 = "" : Con2 = "" : Con3 = "" : Con4 = "" : Con5 = "" : Con6 = ""
        Con7 = "" : Con8 = "" : Con9 = "" : Con10 = "" : Con11 = ""


        If strCLIENTNAME <> "" Then Con1 = String.Format(" AND (DBO.SC_GET_HIGHCUSTNAME_FUN(CUSTCODE) LIKE '%{0}%')", strCLIENTNAME)
        If strCLIENTCODE <> "" Then Con2 = String.Format(" AND (CUSTCODE = '{0}')", strCLIENTCODE)
        If strTIMNAME <> "" Then Con3 = String.Format(" AND (DBO.SC_GET_SUBSEQ_TIMNAME_FUN(SUBSEQ) LIKE '%{0}%')", strTIMNAME)
        If strTIMCODE <> "" Then Con4 = String.Format(" AND (DBO.SC_GET_SUBSEQ_TIMCODE_FUN(SUBSEQ) = '{0}')", strTIMCODE)
        If strSUBSEQNAME <> "" Then Con5 = String.Format(" AND (DBO.SC_GET_SUBSEQNAME_FUN(SUBSEQ) LIKE '%{0}%')", strSUBSEQNAME)
        If strSUBSEQ <> "" Then Con6 = String.Format(" AND (SUBSEQ = '{0}')", strSUBSEQ)
        If strEXCLIENTNAME <> "" Then Con7 = String.Format(" AND (DBO.SC_GET_HIGHCUSTNAME_FUN(EXCLIENTCODE) LIKE '%{0}%')", strEXCLIENTNAME)
        If strEXCLIENTCODE <> "" Then Con8 = String.Format(" AND (EXCLIENTCODE = '{0}')", strEXCLIENTCODE)
        If strMATTERNAME <> "" Then Con9 = String.Format(" AND (MATTER LIKE '%{0}%')", strMATTERNAME)
        If strMEDFLAG <> "" Then
            If strMEDFLAG = "B" Then
                Con10 = " AND (MEDFLAG = 'B')"
            Else
                Con10 = " AND (MEDFLAG IN('A', 'A2','T'))"
            End If
        End If


        If strUSE_YN <> "" Then Con11 = String.Format(" AND (ISNULL(ATTR02,'N') = '{0}')", strUSE_YN)


        strWhere = BuildFields(" ", Con1, Con2, Con3, Con4, Con5, Con6, Con7, Con8, Con9, Con10, Con11)


        strFormat = "  SELECT 0 CHK, "
        strFormat = strFormat & "  MATTERCODE, "
        strFormat = strFormat & "  MATTER, SUBSEQ, '' BTNSUBSEQ, "
        strFormat = strFormat & "  DBO.SC_GET_SUBSEQNAME_FUN(SUBSEQ) SUBSEQNAME, "
        strFormat = strFormat & "  DBO.SC_GET_SUBSEQ_DEPT_NAME_FUN(SUBSEQ) DEPT_NAME, "
        strFormat = strFormat & "  DBO.SC_GET_SUBSEQ_TIMNAME_FUN(SUBSEQ) TIMNAME, "
        strFormat = strFormat & "  DBO.SC_GET_SUBSEQ_CLIENTSUBNAME_FUN(SUBSEQ) CLIENTSUBNAME, "
        strFormat = strFormat & "  CUSTCODE, DBO.SC_GET_HIGHCUSTNAME_FUN(CUSTCODE) CUSTNAME, "
        strFormat = strFormat & "  EXCLIENTCODE, "
        strFormat = strFormat & "  '' BTNEX, "
        strFormat = strFormat & "  DBO.SC_GET_EXCLIENTALLNAME_FUN(EXCLIENTCODE) EXCLIENTNAME, MEMO , "
        strFormat = strFormat & "  case isnull(ATTR02,'N') when 'N' then '미사용' WHEN 'Y' THEN '사용' WHEN 'S' THEN '승인요청' ELSE '등록' end as ATTR02, "
        strFormat = strFormat & "  DBO.SC_EMPNAME_FUN(UUSER) UUSER, UDATE"
        strFormat = strFormat & "  FROM MD_MATTER_MST "
        strFormat = strFormat & "  WHERE 1=1 {0} "



        strSQL = String.Format(strFormat, strWhere)

        '기본정보 Setting
        With mobjSCGLConfig '기본정보 Config 개체
            Try
                ' DB 접속
                .mobjSCGLSql.SQLConnect(.DBConnStr)
                ' ------ Array 데이터 조회 (True 일때 헤더정보 포함 조회(Sheet Data Binding 할 경우 사용), False 일때 데이터만 조회)
                vntData = .mobjSCGLSql.SQLSelectArr(strSQL, intRowCnt, intColCnt, , True)

                Return vntData
            Catch err As Exception
                ' 오류 전송
                Throw RaiseSysErr(err, CLASS_NAME & ".SelectRtn")
            Finally
                ' DB 접속 종료
                .mobjSCGLSql.SQLDisconnect()
            End Try
        End With
    End Function

    Public Function SelectRtn_CountCheck(ByVal strInfoXML As String, _
                                            ByRef intRowCnt As Integer, _
                                            ByRef intColCnt As Integer, _
                                            ByVal strMATTERCODE As String) As Object     'XML  데이터 조회시

        Dim strSQL As String
        Dim strFormat, strSelFields, strWhere As String
        Dim Con1 As String
        Dim vntData As Object


        SetConfig(strInfoXML)   '기본정보 설정
        Con1 = ""
        If strMATTERCODE <> "" Then
            Con1 = String.Format(" AND (MATTERCODE = '{0}')", strMATTERCODE)
        End If
        strWhere = BuildFields(" ", Con1)

        strFormat = strFormat & "  SELECT MEDFLAG, COUNT(*) FROM ("
        strFormat = strFormat & "  SELECT 'B' MEDFLAG, MATTERCODE FROM MD_BOOKING_MEDIUM"
        strFormat = strFormat & "  WHERE 1=1  {0}"
        strFormat = strFormat & "  UNION ALL"
        strFormat = strFormat & "  SELECT 'A2' MEDFLAG, MATTERCODE FROM MD_CATV_MEDIUM"
        strFormat = strFormat & "  WHERE 1=1  {0}"
        strFormat = strFormat & "  UNION ALL"
        strFormat = strFormat & "  SELECT 'A' MEDFLAG, MATTERCODE FROM MD_ELECTRIC_MEDIUM"
        strFormat = strFormat & "  WHERE 1=1  {0}"
        strFormat = strFormat & "  UNION ALL"
        strFormat = strFormat & "  SELECT 'T' MEDFLAG, MATTERCODE FROM MD_TOTAL_MEDIUM"
        strFormat = strFormat & "  WHERE 1=1  {0}"
        strFormat = strFormat & "  ) AAA"
        strFormat = strFormat & "  GROUP BY MEDFLAG"


        strSQL = String.Format(strFormat, strWhere)

        With mobjSCGLConfig
            Try
                .mobjSCGLSql.SQLConnect(.DBConnStr)
                vntData = .mobjSCGLSql.SQLSelectArr(strSQL, intRowCnt, intColCnt)
                Return vntData
            Catch err As Exception
                Throw RaiseSysErr(err, CLASS_NAME & ".SelectRtn")
            Finally
                .mobjSCGLSql.SQLDisconnect()
            End Try
        End With
    End Function

    '==============MD_MATTER_MST 테이블의 신규 MATTERCODE 가져오기
    Public Function Get_NewMatterCode(ByVal strYEAR As String) As String
        Dim strSQL, strFormat, strRtn As String

        With mobjSCGLConfig '기본정보 Config 개체
            Try
                strSQL = "select 'M' +'" & strYEAR & "' + DBO.LPAD(ISNULL(MAX(CAST(SUBSTRING(MATTERCODE,4,5) AS NUMERIC(5,0))),0)+1,5,'0') From MD_MATTER_MST "
                strRtn = .mobjSCGLSql.SQLSelectOneScalar(strSQL)
                Return strRtn
            Catch err As Exception
                ' 오류 전송
                Throw RaiseSysErr(err, CLASS_NAME & ".Get_NewMatterCode")
            End Try
        End With
    End Function


    ' =============== ProcessRtn    거래처 디테일 저장
    Public Function ProcessRtn(ByVal strInfoXML As String, _
                               ByVal vntData As Object, _
                               ByVal strYEAR As String, _
                               ByVal strMEDFLAG As String) As Object

        Dim intRtn As Integer
        Dim i, intColCnt, intRows As Integer
        Dim strMATTERCODE
        Dim strATTR02

        SetConfig(strInfoXML)
        With mobjSCGLConfig
            Try
                .mobjSCGLSql.SQLConnect(.DBConnStr)
                .mobjSCGLSql.SQLBeginTrans()
                If IsArray(vntData) Then
                    '''사용할 Entity 개체생성(Config 정보를 넘겨생성)
                    mobjceMD_MATTER_MST = New ceMD_MATTER_MST(mobjSCGLConfig)
                    '''vntData의 컬럼수, 로우수를 변수입력
                    intColCnt = UBound(vntData, 1) : intRows = UBound(vntData, 2)
                    For i = 1 To intRows
                        strMATTERCODE = ""
                        strATTR02 = ""

                        If GetElement(vntData, "MATTERCODE", intColCnt, i, OPTIONAL_STR) = "" Then
                            strMATTERCODE = Get_NewMatterCode(strYEAR)
                            strATTR02 = GetElement(vntData, "ATTR02", intColCnt, i, OPTIONAL_STR)
                            If strATTR02 = "사용" Then
                                strATTR02 = "Y"
                            ElseIf strATTR02 = "미사용" Then
                                strATTR02 = "N"
                            ElseIf strATTR02 = "미사용" Then
                                strATTR02 = "N"
                            ElseIf strATTR02 = "승인요청" Then
                                strATTR02 = "S"
                            Else
                                strATTR02 = "R"
                            End If
                            intRtn = InsertRtn_MD_MATTER_MST(vntData, intColCnt, i, strMATTERCODE, strMEDFLAG, strATTR02)
                        Else
                            strMATTERCODE = GetElement(vntData, "MATTERCODE", intColCnt, i, OPTIONAL_STR)
                            strATTR02 = GetElement(vntData, "ATTR02", intColCnt, i, OPTIONAL_STR)
                            If strATTR02 = "사용" Then
                                strATTR02 = "Y"
                            ElseIf strATTR02 = "미사용" Then
                                strATTR02 = "N"
                            ElseIf strATTR02 = "미사용" Then
                                strATTR02 = "N"
                            ElseIf strATTR02 = "승인요청" Then
                                strATTR02 = "S"
                            Else
                                strATTR02 = "R"
                            End If
                            intRtn = UpdateRtn_MD_MATTER_MST(vntData, intColCnt, i, strMATTERCODE, strATTR02)
                        End If
                    Next
                End If
                .mobjSCGLSql.SQLCommitTrans()
                Return intRows
            Catch err As Exception
                .mobjSCGLSql.SQLRollbackTrans()
                Throw RaiseSysErr(err, CLASS_NAME & ".ProcessRtn_CUSTDTL")
