    Public Function CUSTNAME_CHECKED(ByVal strInfoXML As String, _
                                     ByRef intRowCnt As Integer, _
                                     ByRef intColCnt As Integer, _
                                     ByVal strBUSINO As String, _
                                     ByVal strMEDFLAG As String, _
                                     ByVal strCUSTNAME As String) As Object

        Dim strSQL As String            'SQL문
        Dim strFormat As String         '임시 SQL문
        Dim strSelFields As String      '조회필드
        Dim strWhere As String
        Dim vntData As Object
        Dim Con1 As String
        Dim Con2 As String
        Dim Con3 As String

        SetConfig(strInfoXML)   '기본정보 설정
        With mobjSCGLConfig

            Con1 = ""
            Con2 = ""
            Con3 = ""

            If strBUSINO <> "" Then Con1 = String.Format(" AND (BUSINO = '{0}')", strBUSINO)
            If strMEDFLAG <> "" Then Con2 = String.Format(" AND (MEDFLAG = '{0}')", strMEDFLAG)
            If strCUSTNAME <> "" Then Con3 = String.Format(" AND (RTRIM(LTRIM(CUSTNAME)) = '{0}')", Trim(strCUSTNAME))

            strWhere = BuildFields(" ", Con1, Con2, Con3)
            strFormat = "SELECT CUSTCODE, CUSTNAME FROM SC_CUST_TEMP WHERE 1=1 {0} "
            strSQL = String.Format(strFormat, strWhere)
            '데이터 조회
            Try
                .mobjSCGLSql.SQLConnect(.DBConnStr)
                vntData = .mobjSCGLSql.SQLSelectArr(strSQL, intRowCnt, intColCnt, , True)
                Return vntData
            Catch err As Exception
                Throw RaiseSysErr(err, CLASS_NAME & ".CUSTNAME_CHECKED")
            Finally
                .mobjSCGLSql.SQLDisconnect()
            End Try
        End With
    End Function

