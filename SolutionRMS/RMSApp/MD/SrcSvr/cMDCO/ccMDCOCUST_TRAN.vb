    ' =============== SelectRtnSample Code
    Public Function SelectRtn(ByVal strInfoXML As String, ByRef intRowCnt As Integer, ByRef intColCnt As Integer, _
                                ByVal strCUSTCODE As String, _
                                ByVal strCUSTNAME As String, _
                                ByVal strBUSINO As String, _
                                ByVal strMEDFLAG As String, _
                                ByVal strMEDDIV As String) As Object     'XML  데이터 조회시

        Dim strSQL As String
        Dim strFormet, strSelFields, strWhere As String
        Dim strChkDate As String = ""
        Dim Con1, Con2, Con3, Con4, Con5 As String
        Dim vntData As Object

        With mobjSCGLConfig
            Try
                SetConfig(strInfoXML)   '기본정보 설정

                Con1 = "" : Con2 = "" : Con3 = "" : Con4 = "" : Con5 = ""

                If strCUSTCODE <> "" Then Con1 = String.Format(" AND (CUSTCODE LIKE  '%{0}%')", strCUSTCODE)
                If strCUSTNAME <> "" Then Con2 = String.Format(" AND (CUSTNAME LIKE  '%{0}%')", strCUSTNAME)
                If strBUSINO <> "" Then Con3 = String.Format(" AND (replace(BUSINO,'-','') LIKE '%{0}%')", Replace(strBUSINO, "-", ""))
                If strMEDFLAG <> "" Then Con4 = String.Format(" AND (MEDFLAG = '{0}')", strMEDFLAG)
                If strMEDDIV <> "" Then Con5 = String.Format(" AND (MEDDIV = '{0}')", strMEDDIV)

                strWhere = BuildFields(" ", Con1, Con2, Con3, Con4, Con5)
                strSelFields = "CUSTCODE , HIGHCUSTCODE , ACCUSTCODE , CASE CUSTTYPE WHEN '2' THEN '계열' ELSE '비계열' END AS CUSTTYPE, CUSTNAME , COMPANYNAME , CUSTOWNER , "
                strSelFields = strSelFields & " CASE MEDFLAG WHEN 'A' THEN '광고주'"
                strSelFields = strSelFields & "              WHEN 'B' THEN '매체사'"
                strSelFields = strSelFields & "              WHEN 'C' THEN '콘티작화료'"
                strSelFields = strSelFields & "	             WHEN 'D' THEN '메이크업'"
                strSelFields = strSelFields & "              WHEN 'E' THEN '효과료'"
                strSelFields = strSelFields & "              WHEN 'F' THEN '촬영료'"
                strSelFields = strSelFields & "              WHEN 'H' THEN '모델'"
                strSelFields = strSelFields & "              WHEN 'I' THEN '일러스트'"
                strSelFields = strSelFields & "              WHEN 'L' THEN '조명료'"
                strSelFields = strSelFields & "              WHEN 'M' THEN '외주처'"
                strSelFields = strSelFields & "              WHEN 'P' THEN 'MPP'"
                strSelFields = strSelFields & "              WHEN 'S' THEN '세트료'"
                strSelFields = strSelFields & "              WHEN 'V' THEN '성우료'"
                strSelFields = strSelFields & "              WHEN 'G' THEN '대대행사'"
                strSelFields = strSelFields & "              WHEN 'Z' THEN '기타' END AS MEDFLAG, BUSINO , JURENTNO,ATTR06,DBO.MD_GET_REALMEDCODE_FUN(HIGHCUSTCODE) HIGHCUSTNAME,ATTR10,MEMO,MEDDIV,DEMANDFLAG,MPP,DBO.SC_GET_HIGHCUSTNAME_FUN(MPP) MPPNAME"

                strFormet = "select {0} from SC_CUST_TEMP where 1=1  {1} "

                strSQL = String.Format(strFormet, strSelFields, strWhere)

                .mobjSCGLSql.SQLConnect(.DBConnStr)
                vntData = .mobjSCGLSql.SQLSelectArr(strSQL, intRowCnt, intColCnt, , True)

                Return vntData
            Catch err As Exception
                Throw RaiseSysErr(err, CLASS_NAME & ".SelectRtn")
            Finally
                .mobjSCGLSql.SQLDisconnect()
            End Try
        End With
    End Function

